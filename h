[36m@@ -0,0 +1,522 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * depd[m
[32m+[m[32m * Copyright(c) 2014-2017 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar callSiteToString = require('./lib/compat').callSiteToString[m
[32m+[m[32mvar eventListenerCount = require('./lib/compat').eventListenerCount[m
[32m+[m[32mvar relative = require('path').relative[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = depd[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the path to base files on.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar basePath = process.cwd()[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if namespace is contained in the string.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction containsNamespace (str, namespace) {[m
[32m+[m[32m  var vals = str.split(/[ ,]+/)[m
[32m+[m[32m  var ns = String(namespace).toLowerCase()[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < vals.length; i++) {[m
[32m+[m[32m    var val = vals[i][m
[32m+[m
[32m+[m[32m    // namespace contained[m
[32m+[m[32m    if (val && (val === '*' || val.toLowerCase() === ns)) {[m
[32m+[m[32m      return true[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return false[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Convert a data descriptor to accessor descriptor.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction convertDataDescriptorToAccessor (obj, prop, message) {[m
[32m+[m[32m  var descriptor = Object.getOwnPropertyDescriptor(obj, prop)[m
[32m+[m[32m  var value = descriptor.value[m
[32m+[m
[32m+[m[32m  descriptor.get = function getter () { return value }[m
[32m+[m
[32m+[m[32m  if (descriptor.writable) {[m
[32m+[m[32m    descriptor.set = function setter (val) { return (value = val) }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  delete descriptor.value[m
[32m+[m[32m  delete descriptor.writable[m
[32m+[m
[32m+[m[32m  Object.defineProperty(obj, prop, descriptor)[m
[32m+[m
[32m+[m[32m  return descriptor[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create arguments string to keep arity.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createArgumentsString (arity) {[m
[32m+[m[32m  var str = ''[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < arity; i++) {[m
[32m+[m[32m    str += ', arg' + i[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return str.substr(2)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create stack string from stack.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createStackString (stack) {[m
[32m+[m[32m  var str = this.name + ': ' + this.namespace[m
[32m+[m
[32m+[m[32m  if (this.message) {[m
[32m+[m[32m    str += ' deprecated ' + this.message[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < stack.length; i++) {[m
[32m+[m[32m    str += '\n    at ' + callSiteToString(stack[i])[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return str[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create deprecate for namespace in caller.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction depd (namespace) {[m
[32m+[m[32m  if (!namespace) {[m
[32m+[m[32m    throw new TypeError('argument namespace is required')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var stack = getStack()[m
[32m+[m[32m  var site = callSiteLocation(stack[1])[m
[32m+[m[32m  var file = site[0][m
[32m+[m
[32m+[m[32m  function deprecate (message) {[m
[32m+[m[32m    // call to self as log[m
[32m+[m[32m    log.call(deprecate, message)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  deprecate._file = file[m
[32m+[m[32m  deprecate._ignored = isignored(namespace)[m
[32m+[m[32m  deprecate._namespace = namespace[m
[32m+[m[32m  deprecate._traced = istraced(namespace)[m
[32m+[m[32m  deprecate._warned = Object.create(null)[m
[32m+[m
[32m+[m[32m  deprecate.function = wrapfunction[m
[32m+[m[32m  deprecate.property = wrapproperty[m
[32m+[m
[32m+[m[32m  return deprecate[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if namespace is ignored.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction isignored (namespace) {[m
[32m+[m[32m  /* istanbul ignore next: tested in a child processs */[m
[32m+[m[32m  if (process.noDeprecation) {[m
[32m+[m[32m    // --no-deprecation support[m
[32m+[m[32m    return true[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var str = process.env.NO_DEPRECATION || ''[m
[32m+[m
[32m+[m[32m  // namespace ignored[m
[32m+[m[32m  return containsNamespace(str, namespace)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if namespace is traced.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction istraced (namespace) {[m
[32m+[m[32m  /* istanbul ignore next: tested in a child processs */[m
[32m+[m[32m  if (process.traceDeprecation) {[m
[32m+[m[32m    // --trace-deprecation support[m
[32m+[m[32m    return true[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var str = process.env.TRACE_DEPRECATION || ''[m
[32m+[m
[32m+[m[32m  // namespace traced[m
[32m+[m[32m  return containsNamespace(str, namespace)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Display deprecation message.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction log (message, site) {[m
[32m+[m[32m  var haslisteners = eventListenerCount(process, 'deprecation') !== 0[m
[32m+[m
[32m+[m[32m  // abort early if no destination[m
[32m+[m[32m  if (!haslisteners && this._ignored) {[m
[32m+[m[32m    return[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var caller[m
[32m+[m[32m  var callFile[m
[32m+[m[32m  var callSite[m
[32m+[m[32m  var depSite[m
[32m+[m[32m  var i = 0[m
[32m+[m[32m  var seen = false[m
[32m+[m[32m  var stack = getStack()[m
[32m+[m[32m  var file = this._file[m
[32m+[m
[32m+[m[32m  if (site) {[m
[32m+[m[32m    // provided site[m
[32m+[m[32m    depSite = site[m
[32m+[m[32m    callSite = callSiteLocation(stack[1])[m
[32m+[m[32m    callSite.name = depSite.name[m
[32m+[m[32m    file = callSite[0][m
[32m+[m[32m  } else {[m
[32m+[m[32m    // get call site[m
[32m+[m[32m    i = 2[m
[32m+[m[32m    depSite = callSiteLocation(stack[i])[m
[32m+[m[32m    callSite = depSite[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // get caller of deprecated thing in relation to file[m
[32m+[m[32m  for (; i < stack.length; i++) {[m
[32m+[m[32m    caller = callSiteLocation(stack[i])[m
[32m+[m[32m    callFile = caller[0][m
[32m+[m
[32m+[m[32m    if (callFile === file) {[m
[32m+[m[32m      seen = true[m
[32m+[m[32m    } else if (callFile === this._file) {[m
[32m+[m[32m      file = this._file[m
[32m+[m[32m    } else if (seen) {[m
[32m+[m[32m      break[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var key = caller[m
[32m+[m[32m    ? depSite.join(':') + '__' + caller.join(':')[m
[32m+[m[32m    : undefined[m
[32m+[m
[32m+[m[32m  if (key !== undefined && key in this._warned) {[m
[32m+[m[32m    // already warned[m
[32m+[m[32m    return[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  this._warned[key] = true[m
[32m+[m
[32m+[m[32m  // generate automatic message from call site[m
[32m+[m[32m  var msg = message[m
[32m+[m[32m  if (!msg) {[m
[32m+[m[32m    msg = callSite === depSite || !callSite.name[m
[32m+[m[32m      ? defaultMessage(depSite)[m
[32m+[m[32m      : defaultMessage(callSite)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // emit deprecation if listeners exist[m
[32m+[m[32m  if (haslisteners) {[m
[32m+[m[32m    var err = DeprecationError(this._namespace, msg, stack.slice(i))[m
[32m+[m[32m    process.emit('deprecation', err)[m
[32m+[m[32m    return[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // format and write message[m
[32m+[m[32m  var format = process.stderr.isTTY[m
[32m+[m[32m    ? formatColor[m
[32m+[m[32m    : formatPlain[m
[32m+[m[32m  var output = format.call(this, msg, caller, stack.slice(i))[m
[32m+[m[32m  process.stderr.write(output + '\n', 'utf8')[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get call site location as array.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction callSiteLocation (callSite) {[m
[32m+[m[32m  var file = callSite.getFileName() || '<anonymous>'[m
[32m+[m[32m  var line = callSite.getLineNumber()[m
[32m+[m[32m  var colm = callSite.getColumnNumber()[m
[32m+[m
[32m+[m[32m  if (callSite.isEval()) {[m
[32m+[m[32m    file = callSite.getEvalOrigin() + ', ' + file[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var site = [file, line, colm][m
[32m+[m
[32m+[m[32m  site.callSite = callSite[m
[32m+[m[32m  site.name = callSite.getFunctionName()[m
[32m+[m
[32m+[m[32m  return site[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Generate a default message from the site.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction defaultMessage (site) {[m
[32m+[m[32m  var callSite = site.callSite[m
[32m+[m[32m  var funcName = site.name[m
[32m+[m
[32m+[m[32m  // make useful anonymous name[m
[32m+[m[32m  if (!funcName) {[m
[32m+[m[32m    funcName = '<anonymous@' + formatLocation(site) + '>'[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var context = callSite.getThis()[m
[32m+[m[32m  var typeName = context && callSite.getTypeName()[m
[32m+[m
[32m+[m[32m  // ignore useless type name[m
[32m+[m[32m  if (typeName === 'Object') {[m
[32m+[m[32m    typeName = undefined[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // make useful type name[m
[32m+[m[32m  if (typeName === 'Function') {[m
[32m+[m[32m    typeName = context.name || typeName[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return typeName && callSite.getMethodName()[m
[32m+[m[32m    ? typeName + '.' + funcName[m
[32m+[m[32m    : funcName[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Format deprecation message without color.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction formatPlain (msg, caller, stack) {[m
[32m+[m[32m  var timestamp = new Date().toUTCString()[m
[32m+[m
[32m+[m[32m  var formatted = timestamp +[m
[32m+[m[32m    ' ' + this._namespace +[m
[32m+[m[32m    ' deprecated ' + msg[m
[32m+[m
[32m+[m[32m  // add stack trace[m
[32m+[m[32m  if (this._traced) {[m
[32m+[m[32m    for (var i = 0; i < stack.length; i++) {[m
[32m+[m[32m      formatted += '\n    at ' + callSiteToString(stack[i])[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return formatted[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (caller) {[m
[32m+[m[32m    formatted += ' at ' + formatLocation(caller)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return formatted[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Format deprecation message with color.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction formatColor (msg, caller, stack) {[m
[32m+[m[32m  var formatted = '\x1b[36;1m' + this._namespace + '\x1b[22;39m' + // bold cyan[m
[32m+[m[32m    ' \x1b[33;1mdeprecated\x1b[22;39m' + // bold yellow[m
[32m+[m[32m    ' \x1b[0m' + msg + '\x1b[39m' // reset[m
[32m+[m
[32m+[m[32m  // add stack trace[m
[32m+[m[32m  if (this._traced) {[m
[32m+[m[32m    for (var i = 0; i < stack.length; i++) {[m
[32m+[m[32m      formatted += '\n    \x1b[36mat ' + callSiteToString(stack[i]) + '\x1b[39m' // cyan[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return formatted[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (caller) {[m
[32m+[m[32m    formatted += ' \x1b[36m' + formatLocation(caller) + '\x1b[39m' // cyan[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return formatted[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Format call site location.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction formatLocation (callSite) {[m
[32m+[m[32m  return relative(basePath, callSite[0]) +[m
[32m+[m[32m    ':' + callSite[1] +[m
[32m+[m[32m    ':' + callSite[2][m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the stack as array of call sites.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getStack () {[m
[32m+[m[32m  var limit = Error.stackTraceLimit[m
[32m+[m[32m  var obj = {}[m
[32m+[m[32m  var prep = Error.prepareStackTrace[m
[32m+[m
[32m+[m[32m  Error.prepareStackTrace = prepareObjectStackTrace[m
[32m+[m[32m  Error.stackTraceLimit = Math.max(10, limit)[m
[32m+[m
[32m+[m[32m  // capture the stack[m
[32m+[m[32m  Error.captureStackTrace(obj)[m
[32m+[m
[32m+[m[32m  // slice this function off the top[m
[32m+[m[32m  var stack = obj.stack.slice(1)[m
[32m+[m
[32m+[m[32m  Error.prepareStackTrace = prep[m
[32m+[m[32m  Error.stackTraceLimit = limit[m
[32m+[m
[32m+[m[32m  return stack[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Capture call site stack from v8.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction prepareObjectStackTrace (obj, stack) {[m
[32m+[m[32m  return stack[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Return a wrapped function in a deprecation message.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction wrapfunction (fn, message) {[m
[32m+[m[32m  if (typeof fn !== 'function') {[m
[32m+[m[32m    throw new TypeError('argument fn must be a function')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var args = createArgumentsString(fn.length)[m
[32m+[m[32m  var deprecate = this // eslint-disable-line no-unused-vars[m
[32m+[m[32m  var stack = getStack()[m
[32m+[m[32m  var site = callSiteLocation(stack[1])[m
[32m+[m
[32m+[m[32m  site.name = fn.name[m
[32m+[m
[32m+[m[32m   // eslint-disable-next-line no-eval[m
[32m+[m[32m  var deprecatedfn = eval('(function (' + args + ') {\n' +[m
[32m+[m[32m    '"use strict"\n' +[m
[32m+[m[32m    'log.call(deprecate, message, site)\n' +[m
[32m+[m[32m    'return fn.apply(this, arguments)\n' +[m
[32m+[m[32m    '})')[m
[32m+[m
[32m+[m[32m  return deprecatedfn[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Wrap property in a deprecation message.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction wrapproperty (obj, prop, message) {[m
[32m+[m[32m  if (!obj || (typeof obj !== 'object' && typeof obj !== 'function')) {[m
[32m+[m[32m    throw new TypeError('argument obj must be object')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var descriptor = Object.getOwnPropertyDescriptor(obj, prop)[m
[32m+[m
[32m+[m[32m  if (!descriptor) {[m
[32m+[m[32m    throw new TypeError('must call property on owner object')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!descriptor.configurable) {[m
[32m+[m[32m    throw new TypeError('property must be configurable')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var deprecate = this[m
[32m+[m[32m  var stack = getStack()[m
[32m+[m[32m  var site = callSiteLocation(stack[1])[m
[32m+[m
[32m+[m[32m  // set site name[m
[32m+[m[32m  site.name = prop[m
[32m+[m
[32m+[m[32m  // convert data descriptor[m
[32m+[m[32m  if ('value' in descriptor) {[m
[32m+[m[32m    descriptor = convertDataDescriptorToAccessor(obj, prop, message)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var get = descriptor.get[m
[32m+[m[32m  var set = descriptor.set[m
[32m+[m
[32m+[m[32m  // wrap getter[m
[32m+[m[32m  if (typeof get === 'function') {[m
[32m+[m[32m    descriptor.get = function getter () {[m
[32m+[m[32m      log.call(deprecate, message, site)[m
[32m+[m[32m      return get.apply(this, arguments)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // wrap setter[m
[32m+[m[32m  if (typeof set === 'function') {[m
[32m+[m[32m    descriptor.set = function setter () {[m
[32m+[m[32m      log.call(deprecate, message, site)[m
[32m+[m[32m      return set.apply(this, arguments)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Object.defineProperty(obj, prop, descriptor)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create DeprecationError for deprecation[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction DeprecationError (namespace, message, stack) {[m
[32m+[m[32m  var error = new Error()[m
[32m+[m[32m  var stackString[m
[32m+[m
[32m+[m[32m  Object.defineProperty(error, 'constructor', {[m
[32m+[m[32m    value: DeprecationError[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m  Object.defineProperty(error, 'message', {[m
[32m+[m[32m    configurable: true,[m
[32m+[m[32m    enumerable: false,[m
[32m+[m[32m    value: message,[m
[32m+[m[32m    writable: true[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m  Object.defineProperty(error, 'name', {[m
[32m+[m[32m    enumerable: false,[m
[32m+[m[32m    configurable: true,[m
[32m+[m[32m    value: 'DeprecationError',[m
[32m+[m[32m    writable: true[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m  Object.defineProperty(error, 'namespace', {[m
[32m+[m[32m    configurable: true,[m
[32m+[m[32m    enumerable: false,[m
[32m+[m[32m    value: namespace,[m
[32m+[m[32m    writable: true[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m  Object.defineProperty(error, 'stack', {[m
[32m+[m[32m    configurable: true,[m
[32m+[m[32m    enumerable: false,[m
[32m+[m[32m    get: function () {[m
[32m+[m[32m      if (stackString !== undefined) {[m
[32m+[m[32m        return stackString[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // prepare stack trace[m
[32m+[m[32m      return (stackString = createStackString.call(this, stack))[m
[32m+[m[32m    },[m
[32m+[m[32m    set: function setter (val) {[m
[32m+[m[32m      stackString = val[m
[32m+[m[32m    }[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m  return error[m
[32m+[m[32m}[m

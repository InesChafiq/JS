[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mmodule.exports = require('./lib/express');[m
[1mdiff --git a/node_modules/express/lib/application.js b/node_modules/express/lib/application.js[m
[1mnew file mode 100644[m
[1mindex 0000000..1abe8d0[m
[1m--- /dev/null[m
[1m+++ b/node_modules/express/lib/application.js[m
[36m@@ -0,0 +1,644 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * express[m
[32m+[m[32m * Copyright(c) 2009-2013 TJ Holowaychuk[m
[32m+[m[32m * Copyright(c) 2013 Roman Shtylman[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar finalhandler = require('finalhandler');[m
[32m+[m[32mvar Router = require('./router');[m
[32m+[m[32mvar methods = require('methods');[m
[32m+[m[32mvar middleware = require('./middleware/init');[m
[32m+[m[32mvar query = require('./middleware/query');[m
[32m+[m[32mvar debug = require('debug')('express:application');[m
[32m+[m[32mvar View = require('./view');[m
[32m+[m[32mvar http = require('http');[m
[32m+[m[32mvar compileETag = require('./utils').compileETag;[m
[32m+[m[32mvar compileQueryParser = require('./utils').compileQueryParser;[m
[32m+[m[32mvar compileTrust = require('./utils').compileTrust;[m
[32m+[m[32mvar deprecate = require('depd')('express');[m
[32m+[m[32mvar flatten = require('array-flatten');[m
[32m+[m[32mvar merge = require('utils-merge');[m
[32m+[m[32mvar resolve = require('path').resolve;[m
[32m+[m[32mvar setPrototypeOf = require('setprototypeof')[m
[32m+[m[32mvar slice = Array.prototype.slice;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Application prototype.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar app = exports = module.exports = {};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Variable for trust proxy inheritance back-compat[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar trustProxyDefaultSymbol = '@@symbol:trust_proxy_default';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Initialize the server.[m
[32m+[m[32m *[m
[32m+[m[32m *   - setup default configuration[m
[32m+[m[32m *   - setup default middleware[m
[32m+[m[32m *   - setup route reflection methods[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mapp.init = function init() {[m
[32m+[m[32m  this.cache = {};[m
[32m+[m[32m  this.engines = {};[m
[32m+[m[32m  this.settings = {};[m
[32m+[m
[32m+[m[32m  this.defaultConfiguration();[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Initialize application configuration.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mapp.defaultConfiguration = function defaultConfiguration() {[m
[32m+[m[32m  var env = process.env.NODE_ENV || 'development';[m
[32m+[m
[32m+[m[32m  // default settings[m
[32m+[m[32m  this.enable('x-powered-by');[m
[32m+[m[32m  this.set('etag', 'weak');[m
[32m+[m[32m  this.set('env', env);[m
[32m+[m[32m  this.set('query parser', 'extended');[m
[32m+[m[32m  this.set('subdomain offset', 2);[m
[32m+[m[32m  this.set('trust proxy', false);[m
[32m+[m
[32m+[m[32m  // trust proxy inherit back-compat[m
[32m+[m[32m  Object.defineProperty(this.settings, trustProxyDefaultSymbol, {[m
[32m+[m[32m    configurable: true,[m
[32m+[m[32m    value: true[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  debug('booting in %s mode', env);[m
[32m+[m
[32m+[m[32m  this.on('mount', function onmount(parent) {[m
[32m+[m[32m    // inherit trust proxy[m
[32m+[m[32m    if (this.settings[trustProxyDefaultSymbol] === true[m
[32m+[m[32m      && typeof parent.settings['trust proxy fn'] === 'function') {[m
[32m+[m[32m      delete this.settings['trust proxy'];[m
[32m+[m[32m      delete this.settings['trust proxy fn'];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // inherit protos[m
[32m+[m[32m    setPrototypeOf(this.request, parent.request)[m
[32m+[m[32m    setPrototypeOf(this.response, parent.response)[m
[32m+[m[32m    setPrototypeOf(this.engines, parent.engines)[m
[32m+[m[32m    setPrototypeOf(this.settings, parent.settings)[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  // setup locals[m
[32m+[m[32m  this.locals = Object.create(null);[m
[32m+[m
[32m+[m[32m  // top-most app is mounted at /[m
[32m+[m[32m  this.mountpath = '/';[m
[32m+[m
[32m+[m[32m  // default locals[m
[32m+[m[32m  this.locals.settings = this.settings;[m
[32m+[m
[32m+[m[32m  // default configuration[m
[32m+[m[32m  this.set('view', View);[m
[32m+[m[32m  this.set('views', resolve('views'));[m
[32m+[m[32m  this.set('jsonp callback name', 'callback');[m
[32m+[m
[32m+[m[32m  if (env === 'production') {[m
[32m+[m[32m    this.enable('view cache');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Object.defineProperty(this, 'router', {[m
[32m+[m[32m    get: function() {[m
[32m+[m[32m      throw new Error('\'app.router\' is deprecated!\nPlease see the 3.x to 4.x migration guide for details on how to update your app.');[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * lazily adds the base router if it has not yet been added.[m
[32m+[m[32m *[m
[32m+[m[32m * We cannot add the base router in the defaultConfiguration because[m
[32m+[m[32m * it reads app settings which might be set after that has run.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m[32mapp.lazyrouter = function lazyrouter() {[m
[32m+[m[32m  if (!this._router) {[m
[32m+[m[32m    this._router = new Router({[m
[32m+[m[32m      caseSensitive: this.enabled('case sensitive routing'),[m
[32m+[m[32m      strict: this.enabled('strict routing')[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this._router.use(query(this.get('query parser fn')));[m
[32m+[m[32m    this._router.use(middleware.init(this));[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Dispatch a req, res pair into the application. Starts pipeline processing.[m
[32m+[m[32m *[m
[32m+[m[32m * If no callback is provided, then default error handlers will respond[m
[32m+[m[32m * in the event of an error bubbling through the stack.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mapp.handle = function handle(req, res, callback) {[m
[32m+[m[32m  var router = this._router;[m
[32m+[m
[32m+[m[32m  // final handler[m
[32m+[m[32m  var done = callback || finalhandler(req, res, {[m
[32m+[m[32m    env: this.get('env'),[m
[32m+[m[32m    onerror: logerror.bind(this)[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  // no routes[m
[32m+[m[32m  if (!router) {[m
[32m+[m[32m    debug('no routes defined on app');[m
[32m+[m[32m    done();[m
[32m+[m[32m    return;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  router.handle(req, res, done);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Proxy `Router#use()` to add middleware to the app router.[m
[32m+[m[32m * See Router#use() documentation for details.[m
[32m+[m[32m *[m
[32m+[m[32m * If the _fn_ parameter is an express app, then it will be[m
[32m+[m[32m * mounted at the _route_ specified.[m
[32m+[m[32m *[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mapp.use = function use(fn) {[m
[32m+[m[32m  var offset = 0;[m
[32m+[m[32m  var path = '/';[m
[32m+[m
[32m+[m[32m  // default path to '/'[m
[32m+[m[32m  // disambiguate app.use([fn])[m
[32m+[m[32m  if (typeof fn !== 'function') {[m
[32m+[m[32m    var arg = fn;[m
[32m+[m
[32m+[m[32m    while (Array.isArray(arg) && arg.length !== 0) {[m
[32m+[m[32m      arg = arg[0];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // first arg is the path[m
[32m+[m[32m    if (typeof arg !== 'function') {[m
[32m+[m[32m      offset = 1;[m
[32m+[m[32m      path = fn;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var fns = flatten(slice.call(arguments, offset));[m
[32m+[m
[32m+[m[32m  if (fns.length === 0) {[m
[32m+[m[32m    throw new TypeError('app.use() requires middleware functions');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // setup router[m
[32m+[m[32m  this.lazyrouter();[m
[32m+[m[32m  var router = this._router;[m
[32m+[m
[32m+[m[32m  fns.forEach(function (fn) {[m
[32m+[m[32m    // non-express app[m
[32m+[m[32m    if (!fn || !fn.handle || !fn.set) {[m
[32m+[m[32m      return router.use(path, fn);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    debug('.use app under %s', path);[m
[32m+[m[32m    fn.mountpath = path;[m
[32m+[m[32m    fn.parent = this;[m
[32m+[m
[32m+[m[32m    // restore .app property on req and res[m
[32m+[m[32m    router.use(path, function mounted_app(req, res, next) {[m
[32m+[m[32m      var orig = req.app;[m
[32m+[m[32m      fn.handle(req, res, function (err) {[m
[32m+[m[32m        setPrototypeOf(req, orig.request)[m
[32m+[m[32m        setPrototypeOf(res, orig.response)[m
[32m+[m[32m        next(err);[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // mounted an app[m
[32m+[m[32m    fn.emit('mount', this);[m
[32m+[m[32m  }, this);[m
[32m+[m
[32m+[m[32m  return this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Proxy to the app `Router#route()`[m
[32m+[m[32m * Returns a new `Route` instance for the _path_.[m
[32m+[m[32m *[m
[32m+[m[32m * Routes are isolated middleware stacks for specific paths.[m
[32m+[m[32m * See the Route api docs for details.[m
[32m+[m[32m *[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mapp.route = function route(path) {[m
[32m+[m[32m  this.lazyrouter();[m
[32m+[m[32m  return this._router.route(path);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Register the given template engine callback `fn`[m
[32m+[m[32m * as `ext`.[m
[32m+[m[32m *[m
[32m+[m[32m * By default will `require()` the engine based on the[m
[32m+[m[32m * file extension. For example if you try to render[m
[32m+[m[32m * a "foo.ejs" file Express will invoke the following internally:[m
[32m+[m[32m *[m
[32m+[m[32m *     app.engine('ejs', require('ejs').__express);[m
[32m+[m[32m *[m
[32m+[m[32m * For engines that do not provide `.__express` out of the box,[m
[32m+[m[32m * or if you wish to "map" a different extension to the template engine[m
[32m+[m[32m * you may use this method. For example mapping the EJS template engine to[m
[32m+[m[32m * ".html" files:[m
[32m+[m[32m *[m
[32m+[m[32m *     app.engine('html', require('ejs').renderFile);[m
[32m+[m[32m *[m
[32m+[m[32m * In this case EJS provides a `.renderFile()` method with[m
[32m+[m[32m * the same signature that Express expects: `(path, options, callback)`,[m
[32m+[m[32m * though note that it aliases this method as `ejs.__express` internally[m
[32m+[m[32m * so if you're using ".ejs" extensions you dont need to do anything.[m
[32m+[m[32m *[m
[32m+[m[32m * Some template engines do not follow this convention, the[m
[32m+[m[32m * [Consolidate.js](https://github.com/tj/consolidate.js)[m
[32m+[m[32m * library was created to map all of node's popular template[m
[32m+[m[32m * engines to follow this convention, thus allowing them to[m
[32m+[m[32m * work seamlessly within Express.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} ext[m
[32m+[m[32m * @param {Function} fn[m
[32m+[m[32m * @return {app} for chaining[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mapp.engine = function engine(ext, fn) {[m
[32m+[m[32m  if (typeof fn !== 'function') {[m
[32m+[m[32m    throw new Error('callback function required');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // get file extension[m
[32m+[m[32m  var extension = ext[0] !== '.'[m
[32m+[m[32m    ? '.' + ext[m
[32m+[m[32m    : ext;[m
[32m+[m
[32m+[m[32m  // store engine[m
[32m+[m[32m  this.engines[extension] = fn;[m
[32m+[m
[32m+[m[32m  return this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Proxy to `Router#param()` with one added api feature. The _name_ parameter[m
[32m+[m[32m * can be an array of names.[m
[32m+[m[32m *[m
[32m+[m[32m * See the Router#param() docs for more details.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String|Array} name[m
[32m+[m[32m * @param {Function} fn[m
[32m+[m[32m * @return {app} for chaining[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mapp.param = function param(name, fn) {[m
[32m+[m[32m  this.lazyrouter();[m
[32m+[m
[32m+[m[32m  if (Array.isArray(name)) {[m
[32m+[m[32m    for (var i = 0; i < name.length; i++) {[m
[32m+[m[32m      this.param(name[i], fn);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return this;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  this._router.param(name, fn);[m
[32m+[m
[32m+[m[32m  return this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Assign `setting` to `val`, or return `setting`'s value.[m
[32m+[m[32m *[m
[32m+[m[32m *    app.set('foo', 'bar');[m
[32m+[m[32m *    app.get('foo');[m
[32m+[m[32m *    // => "bar"[m
[32m+[m[32m *[m
[32m+[m[32m * Mounted servers inherit their parent server's settings.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} setting[m
[32m+[m[32m * @param {*} [val][m
[32m+[m[32m * @return {Server} for chaining[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mapp.set = function set(setting, val) {[m
[32m+[m[32m  if (arguments.length === 1) {[m
[32m+[m[32m    // app.get(setting)[m
[32m+[m[32m    return this.settings[setting];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  debug('set "%s" to %o', setting, val);[m
[32m+[m
[32m+[m[32m  // set value[m
[32m+[m[32m  this.settings[setting] = val;[m
[32m+[m
[32m+[m[32m  // trigger matched settings[m
[32m+[m[32m  switch (setting) {[m
[32m+[m[32m    case 'etag':[m
[32m+[m[32m      this.set('etag fn', compileETag(val));[m
[32m+[m[32m      break;[m
[32m+[m[32m    case 'query parser':[m
[32m+[m[32m      this.set('query parser fn', compileQueryParser(val));[m
[32m+[m[32m      break;[m
[32m+[m[32m    case 'trust proxy':[m
[32m+[m[32m      this.set('trust proxy fn', compileTrust(val));[m
[32m+[m
[32m+[m[32m      // trust proxy inherit back-compat[m
[32m+[m[32m      Object.defineProperty(this.settings, trustProxyDefaultSymbol, {[m
[32m+[m[32m        configurable: true,[m
[32m+[m[32m        value: false[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      break;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Return the app's absolute pathname[m
[32m+[m[32m * based on the parent(s) that have[m
[32m+[m[32m * mounted it.[m
[32m+[m[32m *[m
[32m+[m[32m * For example if the application was[m
[32m+[m[32m * mounted as "/admin", which itself[m
[32m+[m[32m * was mounted as "/blog" then the[m
[32m+[m[32m * return value would be "/blog/admin".[m
[32m+[m[32m *[m
[32m+[m[32m * @return {String}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mapp.path = function path() {[m
[32m+[m[32m  return this.parent[m
[32m+[m[32m    ? this.parent.path() + this.mountpath[m
[32m+[m[32m    : '';[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Check if `setting` is enabled (truthy).[m
[32m+[m[32m *[m
[32m+[m[32m *    app.enabled('foo')[m
[32m+[m[32m *    // => false[m
[32m+[m[32m *[m
[32m+[m[32m *    app.enable('foo')[m
[32m+[m[32m *    app.enabled('foo')[m
[32m+[m[32m *    // => true[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} setting[m
[32m+[m[32m * @return {Boolean}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mapp.enabled = function enabled(setting) {[m
[32m+[m[32m  return Boolean(this.set(setting));[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Check if `setting` is disabled.[m
[32m+[m[32m *[m
[32m+[m[32m *    app.disabled('foo')[m
[32m+[m[32m *    // => true[m
[32m+[m[32m *[m
[32m+[m[32m *    app.enable('foo')[m
[32m+[m[32m *    app.disabled('foo')[m
[32m+[m[32m *    // => false[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} setting[m
[32m+[m[32m * @return {Boolean}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mapp.disabled = function disabled(setting) {[m
[32m+[m[32m  return !this.set(setting);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Enable `setting`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} setting[m
[32m+[m[32m * @return {app} for chaining[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mapp.enable = function enable(setting) {[m
[32m+[m[32m  return this.set(setting, true);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Disable `setting`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} setting[m
[32m+[m[32m * @return {app} for chaining[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mapp.disable = function disable(setting) {[m
[32m+[m[32m  return this.set(setting, false);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Delegate `.VERB(...)` calls to `router.VERB(...)`.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmethods.forEach(function(method){[m
[32m+[m[32m  app[method] = function(path){[m
[32m+[m[32m    if (method === 'get' && arguments.length === 1) {[m
[32m+[m[32m      // app.get(setting)[m
[32m+[m[32m      return this.set(path);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.lazyrouter();[m
[32m+[m
[32m+[m[32m    var route = this._router.route(path);[m
[32m+[m[32m    route[method].apply(route, slice.call(arguments, 1));[m
[32m+[m[32m    return this;[m
[32m+[m[32m  };[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Special-cased "all" method, applying the given route `path`,[m
[32m+[m[32m * middleware, and callback to _every_ HTTP method.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} path[m
[32m+[m[32m * @param {Function} ...[m
[32m+[m[32m * @return {app} for chaining[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mapp.all = function all(path) {[m
[32m+[m[32m  this.lazyrouter();[m
[32m+[m
[32m+[m[32m  var route = this._router.route(path);[m
[32m+[m[32m  var args = slice.call(arguments, 1);[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < methods.length; i++) {[m
[32m+[m[32m    route[methods[i]].apply(route, args);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// del -> delete alias[m
[32m+[m
[32m+[m[32mapp.del = deprecate.function(app.delete, 'app.del: Use app.delete instead');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Render the given view `name` name with `options`[m
[32m+[m[32m * and a callback accepting an error and the[m
[32m+[m[32m * rendered template string.[m
[32m+[m[32m *[m
[32m+[m[32m * Example:[m
[32m+[m[32m *[m
[32m+[m[32m *    app.render('email', { name: 'Tobi' }, function(err, html){[m
[32m+[m[32m *      // ...[m
[32m+[m[32m *    })[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} name[m
[32m+[m[32m * @param {Object|Function} options or fn[m
[32m+[m[32m * @param {Function} callback[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mapp.render = function render(name, options, callback) {[m
[32m+[m[32m  var cache = this.cache;[m
[32m+[m[32m  var done = callback;[m
[32m+[m[32m  var engines = this.engines;[m
[32m+[m[32m  var opts = options;[m
[32m+[m[32m  var renderOptions = {};[m
[32m+[m[32m  var view;[m
[32m+[m
[32m+[m[32m  // support callback function as second arg[m
[32m+[m[32m  if (typeof options === 'function') {[m
[32m+[m[32m    done = options;[m
[32m+[m[32m    opts = {};[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // merge app.locals[m
[32m+[m[32m  merge(renderOptions, this.locals);[m
[32m+[m
[32m+[m[32m  // merge options._locals[m
[32m+[m[32m  if (opts._locals) {[m
[32m+[m[32m    merge(renderOptions, opts._locals);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // merge options[m
[32m+[m[32m  merge(renderOptions, opts);[m
[32m+[m
[32m+[m[32m  // set .cache unless explicitly provided[m
[32m+[m[32m  if (renderOptions.cache == null) {[m
[32m+[m[32m    renderOptions.cache = this.enabled('view cache');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // primed cache[m
[32m+[m[32m  if (renderOptions.cache) {[m
[32m+[m[32m    view = cache[name];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // view[m
[32m+[m[32m  if (!view) {[m
[32m+[m[32m    var View = this.get('view');[m
[32m+[m
[32m+[m[32m    view = new View(name, {[m
[32m+[m[32m      defaultEngine: this.get('view engine'),[m
[32m+[m[32m      root: this.get('views'),[m
[32m+[m[32m      engines: engines[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    if (!view.path) {[m
[32m+[m[32m      var dirs = Array.isArray(view.root) && view.root.length > 1[m
[32m+[m[32m        ? 'directories "' + view.root.slice(0, -1).join('", "') + '" or "' + view.root[view.root.length - 1] + '"'[m
[32m+[m[32m        : 'directory "' + view.root + '"'[m
[32m+[m[32m      var err = new Error('Failed to lookup view "' + name + '" in views ' + dirs);[m
[32m+[m[32m      err.view = view;[m
[32m+[m[32m      return done(err);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // prime the cache[m
[32m+[m[32m    if (renderOptions.cache) {[m
[32m+[m[32m      cache[name] = view;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // render[m
[32m+[m[32m  tryRender(view, renderOptions, done);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Listen for connections.[m
[32m+[m[32m *[m
[32m+[m[32m * A node `http.Server` is returned, with this[m
[32m+[m[32m * application (which is a `Function`) as its[m
[32m+[m[32m * callback. If you wish to create both an HTTP[m
[32m+[m[32m * and HTTPS server you may do so with the "http"[m
[32m+[m[32m * and "https" modules as shown here:[m
[32m+[m[32m *[m
[32m+[m[32m *    var http = require('http')[m
[32m+[m[32m *      , https = require('https')[m
[32m+[m[32m *      , express = require('express')[m
[32m+[m[32m *      , app = express();[m
[32m+[m[32m *[m
[32m+[m[32m *    http.createServer(app).listen(80);[m
[32m+[m[32m *    https.createServer({ ... }, app).listen(443);[m
[32m+[m[32m *[m
[32m+[m[32m * @return {http.Server}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mapp.listen = function listen() {[m
[32m+[m[32m  var server = http.createServer(this);[m
[32m+[m[32m  return server.listen.apply(server, arguments);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Log error using console.error.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Error} err[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction logerror(err) {[m
[32m+[m[32m  /* istanbul ignore next */[m
[32m+[m[32m  if (this.get('env') !== 'test') console.error(err.stack || err.toString());[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Try rendering a view.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction tryRender(view, options, callback) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    view.render(options, callback);[m
[32m+[m[32m  } catch (err) {[m
[32m+[m[32m    callback(err);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m

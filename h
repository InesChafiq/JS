[1mdiff --git a/node_modules/debug/src/browser.js b/node_modules/debug/src/browser.js[m
[1mnew file mode 100644[m
[1mindex 0000000..7106924[m
[1m--- /dev/null[m
[1m+++ b/node_modules/debug/src/browser.js[m
[36m@@ -0,0 +1,185 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * This is the web browser implementation of `debug()`.[m
[32m+[m[32m *[m
[32m+[m[32m * Expose `debug()` as the module.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports = module.exports = require('./debug');[m
[32m+[m[32mexports.log = log;[m
[32m+[m[32mexports.formatArgs = formatArgs;[m
[32m+[m[32mexports.save = save;[m
[32m+[m[32mexports.load = load;[m
[32m+[m[32mexports.useColors = useColors;[m
[32m+[m[32mexports.storage = 'undefined' != typeof chrome[m
[32m+[m[32m               && 'undefined' != typeof chrome.storage[m
[32m+[m[32m                  ? chrome.storage.local[m
[32m+[m[32m                  : localstorage();[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Colors.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.colors = [[m
[32m+[m[32m  'lightseagreen',[m
[32m+[m[32m  'forestgreen',[m
[32m+[m[32m  'goldenrod',[m
[32m+[m[32m  'dodgerblue',[m
[32m+[m[32m  'darkorchid',[m
[32m+[m[32m  'crimson'[m
[32m+[m[32m];[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Currently only WebKit-based Web Inspectors, Firefox >= v31,[m
[32m+[m[32m * and the Firebug extension (any Firefox version) are known[m
[32m+[m[32m * to support "%c" CSS customizations.[m
[32m+[m[32m *[m
[32m+[m[32m * TODO: add a `localStorage` variable to explicitly enable/disable colors[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction useColors() {[m
[32m+[m[32m  // NB: In an Electron preload script, document will be defined but not fully[m
[32m+[m[32m  // initialized. Since we know we're in Chrome, we'll just detect this case[m
[32m+[m[32m  // explicitly[m
[32m+[m[32m  if (typeof window !== 'undefined' && window.process && window.process.type === 'renderer') {[m
[32m+[m[32m    return true;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // is webkit? http://stackoverflow.com/a/16459606/376773[m
[32m+[m[32m  // document is undefined in react-native: https://github.com/facebook/react-native/pull/1632[m
[32m+[m[32m  return (typeof document !== 'undefined' && document.documentElement && document.documentElement.style && document.documentElement.style.WebkitAppearance) ||[m
[32m+[m[32m    // is firebug? http://stackoverflow.com/a/398120/376773[m
[32m+[m[32m    (typeof window !== 'undefined' && window.console && (window.console.firebug || (window.console.exception && window.console.table))) ||[m
[32m+[m[32m    // is firefox >= v31?[m
[32m+[m[32m    // https://developer.mozilla.org/en-US/docs/Tools/Web_Console#Styling_messages[m
[32m+[m[32m    (typeof navigator !== 'undefined' && navigator.userAgent && navigator.userAgent.toLowerCase().match(/firefox\/(\d+)/) && parseInt(RegExp.$1, 10) >= 31) ||[m
[32m+[m[32m    // double check webkit in userAgent just in case we are in a worker[m
[32m+[m[32m    (typeof navigator !== 'undefined' && navigator.userAgent && navigator.userAgent.toLowerCase().match(/applewebkit\/(\d+)/));[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Map %j to `JSON.stringify()`, since no Web Inspectors do that by default.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.formatters.j = function(v) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    return JSON.stringify(v);[m
[32m+[m[32m  } catch (err) {[m
[32m+[m[32m    return '[UnexpectedJSONParseError]: ' + err.message;[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Colorize log arguments if enabled.[m
[32m+[m[32m *[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction formatArgs(args) {[m
[32m+[m[32m  var useColors = this.useColors;[m
[32m+[m
[32m+[m[32m  args[0] = (useColors ? '%c' : '')[m
[32m+[m[32m    + this.namespace[m
[32m+[m[32m    + (useColors ? ' %c' : ' ')[m
[32m+[m[32m    + args[0][m
[32m+[m[32m    + (useColors ? '%c ' : ' ')[m
[32m+[m[32m    + '+' + exports.humanize(this.diff);[m
[32m+[m
[32m+[m[32m  if (!useColors) return;[m
[32m+[m
[32m+[m[32m  var c = 'color: ' + this.color;[m
[32m+[m[32m  args.splice(1, 0, c, 'color: inherit')[m
[32m+[m
[32m+[m[32m  // the final "%c" is somewhat tricky, because there could be other[m
[32m+[m[32m  // arguments passed either before or after the %c, so we need to[m
[32m+[m[32m  // figure out the correct index to insert the CSS into[m
[32m+[m[32m  var index = 0;[m
[32m+[m[32m  var lastC = 0;[m
[32m+[m[32m  args[0].replace(/%[a-zA-Z%]/g, function(match) {[m
[32m+[m[32m    if ('%%' === match) return;[m
[32m+[m[32m    index++;[m
[32m+[m[32m    if ('%c' === match) {[m
[32m+[m[32m      // we only are interested in the *last* %c[m
[32m+[m[32m      // (the user may have provided their own)[m
[32m+[m[32m      lastC = index;[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  args.splice(lastC, 0, c);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Invokes `console.log()` when available.[m
[32m+[m[32m * No-op when `console.log` is not a "function".[m
[32m+[m[32m *[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction log() {[m
[32m+[m[32m  // this hackery is required for IE8/9, where[m
[32m+[m[32m  // the `console.log` function doesn't have 'apply'[m
[32m+[m[32m  return 'object' === typeof console[m
[32m+[m[32m    && console.log[m
[32m+[m[32m    && Function.prototype.apply.call(console.log, console, arguments);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Save `namespaces`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} namespaces[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction save(namespaces) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    if (null == namespaces) {[m
[32m+[m[32m      exports.storage.removeItem('debug');[m
[32m+[m[32m    } else {[m
[32m+[m[32m      exports.storage.debug = namespaces;[m
[32m+[m[32m    }[m
[32m+[m[32m  } catch(e) {}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Load `namespaces`.[m
[32m+[m[32m *[m
[32m+[m[32m * @return {String} returns the previously persisted debug modes[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction load() {[m
[32m+[m[32m  var r;[m
[32m+[m[32m  try {[m
[32m+[m[32m    r = exports.storage.debug;[m
[32m+[m[32m  } catch(e) {}[m
[32m+[m
[32m+[m[32m  // If debug isn't set in LS, and we're in Electron, try to load $DEBUG[m
[32m+[m[32m  if (!r && typeof process !== 'undefined' && 'env' in process) {[m
[32m+[m[32m    r = process.env.DEBUG;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return r;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Enable namespaces listed in `localStorage.debug` initially.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.enable(load());[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Localstorage attempts to return the localstorage.[m
[32m+[m[32m *[m
[32m+[m[32m * This is necessary because safari throws[m
[32m+[m[32m * when a user disables cookies/localstorage[m
[32m+[m[32m * and you attempt to access it.[m
[32m+[m[32m *[m
[32m+[m[32m * @return {LocalStorage}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction localstorage() {[m
[32m+[m[32m  try {[m
[32m+[m[32m    return window.localStorage;[m
[32m+[m[32m  } catch (e) {}[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/debug/src/debug.js b/node_modules/debug/src/debug.js[m
[1mnew file mode 100644[m
[1mindex 0000000..6a5e3fc[m
[1m--- /dev/null[m
[1m+++ b/node_modules/debug/src/debug.js[m
[36m@@ -0,0 +1,202 @@[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * This is the common logic for both the Node.js and web browser[m
[32m+[m[32m * implementations of `debug()`.[m
[32m+[m[32m *[m
[32m+[m[32m * Expose `debug()` as the module.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports = module.exports = createDebug.debug = createDebug['default'] = createDebug;[m
[32m+[m[32mexports.coerce = coerce;[m
[32m+[m[32mexports.disable = disable;[m
[32m+[m[32mexports.enable = enable;[m
[32m+[m[32mexports.enabled = enabled;[m
[32m+[m[32mexports.humanize = require('ms');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * The currently active debug mode names, and names to skip.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.names = [];[m
[32m+[m[32mexports.skips = [];[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Map of special "%n" handling functions, for the debug "format" argument.[m
[32m+[m[32m *[m
[32m+[m[32m * Valid key names are a single, lower or upper-case letter, i.e. "n" and "N".[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.formatters = {};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Previous log timestamp.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar prevTime;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Select a color.[m
[32m+[m[32m * @param {String} namespace[m
[32m+[m[32m * @return {Number}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction selectColor(namespace) {[m
[32m+[m[32m  var hash = 0, i;[m
[32m+[m
[32m+[m[32m  for (i in namespace) {[m
[32m+[m[32m    hash  = ((hash << 5) - hash) + namespace.charCodeAt(i);[m
[32m+[m[32m    hash |= 0; // Convert to 32bit integer[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return exports.colors[Math.abs(hash) % exports.colors.length];[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a debugger with the given `namespace`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} namespace[m
[32m+[m[32m * @return {Function}[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createDebug(namespace) {[m
[32m+[m
[32m+[m[32m  function debug() {[m
[32m+[m[32m    // disabled?[m
[32m+[m[32m    if (!debug.enabled) return;[m
[32m+[m
[32m+[m[32m    var self = debug;[m
[32m+[m
[32m+[m[32m    // set `diff` timestamp[m
[32m+[m[32m    var curr = +new Date();[m
[32m+[m[32m    var ms = curr - (prevTime || curr);[m
[32m+[m[32m    self.diff = ms;[m
[32m+[m[32m    self.prev = prevTime;[m
[32m+[m[32m    self.curr = curr;[m
[32m+[m[32m    prevTime = curr;[m
[32m+[m
[32m+[m[32m    // turn the `arguments` into a proper Array[m
[32m+[m[32m    var args = new Array(arguments.length);[m
[32m+[m[32m    for (var i = 0; i < args.length; i++) {[m
[32m+[m[32m      args[i] = arguments[i];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    args[0] = exports.coerce(args[0]);[m
[32m+[m
[32m+[m[32m    if ('string' !== typeof args[0]) {[m
[32m+[m[32m      // anything else let's inspect with %O[m
[32m+[m[32m      args.unshift('%O');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // apply any `formatters` transformations[m
[32m+[m[32m    var index = 0;[m
[32m+[m[32m    args[0] = args[0].replace(/%([a-zA-Z%])/g, function(match, format) {[m
[32m+[m[32m      // if we encounter an escaped % then don't increase the array index[m
[32m+[m[32m      if (match === '%%') return match;[m
[32m+[m[32m      index++;[m
[32m+[m[32m      var formatter = exports.formatters[format];[m
[32m+[m[32m      if ('function' === typeof formatter) {[m
[32m+[m[32m        var val = args[index];[m
[32m+[m[32m        match = formatter.call(self, val);[m
[32m+[m
[32m+[m[32m        // now we need to remove `args[index]` since it's inlined in the `format`[m
[32m+[m[32m        args.splice(index, 1);[m
[32m+[m[32m        index--;[m
[32m+[m[32m      }[m
[32m+[m[32m      return match;[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // apply env-specific formatting (colors, etc.)[m
[32m+[m[32m    exports.formatArgs.call(self, args);[m
[32m+[m
[32m+[m[32m    var logFn = debug.log || exports.log || console.log.bind(console);[m
[32m+[m[32m    logFn.apply(self, args);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  debug.namespace = namespace;[m
[32m+[m[32m  debug.enabled = exports.enabled(namespace);[m
[32m+[m[32m  debug.useColors = exports.useColors();[m
[32m+[m[32m  debug.color = selectColor(namespace);[m
[32m+[m
[32m+[m[32m  // env-specific initialization logic for debug instances[m
[32m+[m[32m  if ('function' === typeof exports.init) {[m
[32m+[m[32m    exports.init(debug);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return debug;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Enables a debug mode by namespaces. This can include modes[m
[32m+[m[32m * separated by a colon and wildcards.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} namespaces[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction enable(namespaces) {[m
[32m+[m[32m  exports.save(namespaces);[m
[32m+[m
[32m+[m[32m  exports.names = [];[m
[32m+[m[32m  exports.skips = [];[m
[32m+[m
[32m+[m[32m  var split = (typeof namespaces === 'string' ? namespaces : '').split(/[\s,]+/);[m
[32m+[m[32m  var len = split.length;[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < len; i++) {[m
[32m+[m[32m    if (!split[i]) continue; // ignore empty strings[m
[32m+[m[32m    namespaces = split[i].replace(/\*/g, '.*?');[m
[32m+[m[32m    if (namespaces[0] === '-') {[m
[32m+[m[32m      exports.skips.push(new RegExp('^' + namespaces.substr(1) + '$'));[m
[32m+[m[32m    } else {[m
[32m+[m[32m      exports.names.push(new RegExp('^' + namespaces + '$'));[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Disable debug output.[m
[32m+[m[32m *[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction disable() {[m
[32m+[m[32m  exports.enable('');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Returns true if the given mode name is enabled, false otherwise.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} name[m
[32m+[m[32m * @return {Boolean}[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction enabled(name) {[m
[32m+[m[32m  var i, len;[m
[32m+[m[32m  for (i = 0, len = exports.skips.length; i < len; i++) {[m
[32m+[m[32m    if (exports.skips[i].test(name)) {[m
[32m+[m[32m      return false;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  for (i = 0, len = exports.names.length; i < len; i++) {[m
[32m+[m[32m    if (exports.names[i].test(name)) {[m
[32m+[m[32m      return true;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  return false;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Coerce `val`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Mixed} val[m
[32m+[m[32m * @return {Mixed}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction coerce(val) {[m
[32m+[m[32m  if (val instanceof Error) return val.stack || val.message;[m
[32m+[m[32m  return val;[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/debug/src/index.js b/node_modules/debug/src/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..e12cf4d[m
[1m--- /dev/null[m
[1m+++ b/node_modules/debug/src/index.js[m
[36m@@ -0,0 +1,10 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * Detect Electron renderer process, which is node, but we should[m
[32m+[m[32m * treat as a browser.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mif (typeof process !== 'undefined' && process.type === 'renderer') {[m
[32m+[m[32m  module.exports = require('./browser.js');[m
[32m+[m[32m} else {[m
[32m+[m[32m  module.exports = require('./node.js');[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/debug/src/inspector-log.js b/node_modules/debug/src/inspector-log.js[m
[1mnew file mode 100644[m
[1mindex 0000000..60ea6c0[m
[1m--- /dev/null[m
[1m+++ b/node_modules/debug/src/inspector-log.js[m
[36m@@ -0,0 +1,15 @@[m
[32m+[m[32mmodule.exports = inspectorLog;[m
[32m+[m
[32m+[m[32m// black hole[m
[32m+[m[32mconst nullStream = new (require('stream').Writable)();[m
[32m+[m[32mnullStream._write = () => {};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Outputs a `console.log()` to the Node.js Inspector console *only*.[m
[32m+[m[32m */[m
[32m+[m[32mfunction inspectorLog() {[m
[32m+[m[32m  const stdout = console._stdout;[m
[32m+[m[32m  console._stdout = nullStream;[m
[32m+[m[32m  console.log.apply(console, arguments);[m
[32m+[m[32m  console._stdout = stdout;[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/debug/src/node.js b/node_modules/debug/src/node.js[m
[1mnew file mode 100644[m
[1mindex 0000000..b15109c[m
[1m--- /dev/null[m
[1m+++ b/node_modules/debug/src/node.js[m
[36m@@ -0,0 +1,248 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar tty = require('tty');[m
[32m+[m[32mvar util = require('util');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * This is the Node.js implementation of `debug()`.[m
[32m+[m[32m *[m
[32m+[m[32m * Expose `debug()` as the module.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports = module.exports = require('./debug');[m
[32m+[m[32mexports.init = init;[m
[32m+[m[32mexports.log = log;[m
[32m+[m[32mexports.formatArgs = formatArgs;[m
[32m+[m[32mexports.save = save;[m
[32m+[m[32mexports.load = load;[m
[32m+[m[32mexports.useColors = useColors;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Colors.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.colors = [6, 2, 3, 4, 5, 1];[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Build up the default `inspectOpts` object from the environment variables.[m
[32m+[m[32m *[m
[32m+[m[32m *   $ DEBUG_COLORS=no DEBUG_DEPTH=10 DEBUG_SHOW_HIDDEN=enabled node script.js[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.inspectOpts = Object.keys(process.env).filter(function (key) {[m
[32m+[m[32m  return /^debug_/i.test(key);[m
[32m+[m[32m}).reduce(function (obj, key) {[m
[32m+[m[32m  // camel-case[m
[32m+[m[32m  var prop = key[m
[32m+[m[32m    .substring(6)[m
[32m+[m[32m    .toLowerCase()[m
[32m+[m[32m    .replace(/_([a-z])/g, function (_, k) { return k.toUpperCase() });[m
[32m+[m
[32m+[m[32m  // coerce string value into JS value[m
[32m+[m[32m  var val = process.env[key];[m
[32m+[m[32m  if (/^(yes|on|true|enabled)$/i.test(val)) val = true;[m
[32m+[m[32m  else if (/^(no|off|false|disabled)$/i.test(val)) val = false;[m
[32m+[m[32m  else if (val === 'null') val = null;[m
[32m+[m[32m  else val = Number(val);[m
[32m+[m
[32m+[m[32m  obj[prop] = val;[m
[32m+[m[32m  return obj;[m
[32m+[m[32m}, {});[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * The file descriptor to write the `debug()` calls to.[m
[32m+[m[32m * Set the `DEBUG_FD` env variable to override with another value. i.e.:[m
[32m+[m[32m *[m
[32m+[m[32m *   $ DEBUG_FD=3 node script.js 3>debug.log[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar fd = parseInt(process.env.DEBUG_FD, 10) || 2;[m
[32m+[m
[32m+[m[32mif (1 !== fd && 2 !== fd) {[m
[32m+[m[32m  util.deprecate(function(){}, 'except for stderr(2) and stdout(1), any other usage of DEBUG_FD is deprecated. Override debug.log if you want to use a different log function (https://git.io/debug_fd)')()[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar stream = 1 === fd ? process.stdout :[m
[32m+[m[32m             2 === fd ? process.stderr :[m
[32m+[m[32m             createWritableStdioStream(fd);[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Is stdout a TTY? Colored output is enabled when `true`.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction useColors() {[m
[32m+[m[32m  return 'colors' in exports.inspectOpts[m
[32m+[m[32m    ? Boolean(exports.inspectOpts.colors)[m
[32m+[m[32m    : tty.isatty(fd);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Map %o to `util.inspect()`, all on a single line.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.formatters.o = function(v) {[m
[32m+[m[32m  this.inspectOpts.colors = this.useColors;[m
[32m+[m[32m  return util.inspect(v, this.inspectOpts)[m
[32m+[m[32m    .split('\n').map(function(str) {[m
[32m+[m[32m      return str.trim()[m
[32m+[m[32m    }).join(' ');[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Map %o to `util.inspect()`, allowing multiple lines if needed.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.formatters.O = function(v) {[m
[32m+[m[32m  this.inspectOpts.colors = this.useColors;[m
[32m+[m[32m  return util.inspect(v, this.inspectOpts);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Adds ANSI color escape codes if enabled.[m
[32m+[m[32m *[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction formatArgs(args) {[m
[32m+[m[32m  var name = this.namespace;[m
[32m+[m[32m  var useColors = this.useColors;[m
[32m+[m
[32m+[m[32m  if (useColors) {[m
[32m+[m[32m    var c = this.color;[m
[32m+[m[32m    var prefix = '  \u001b[3' + c + ';1m' + name + ' ' + '\u001b[0m';[m
[32m+[m
[32m+[m[32m    args[0] = prefix + args[0].split('\n').join('\n' + prefix);[m
[32m+[m[32m    args.push('\u001b[3' + c + 'm+' + exports.humanize(this.diff) + '\u001b[0m');[m
[32m+[m[32m  } else {[m
[32m+[m[32m    args[0] = new Date().toUTCString()[m
[32m+[m[32m      + ' ' + name + ' ' + args[0];[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Invokes `util.format()` with the specified arguments and writes to `stream`.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction log() {[m
[32m+[m[32m  return stream.write(util.format.apply(util, arguments) + '\n');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Save `namespaces`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} namespaces[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction save(namespaces) {[m
[32m+[m[32m  if (null == namespaces) {[m
[32m+[m[32m    // If you set a process.env field to null or undefined, it gets cast to the[m
[32m+[m[32m    // string 'null' or 'undefined'. Just delete instead.[m
[32m+[m[32m    delete process.env.DEBUG;[m
[32m+[m[32m  } else {[m
[32m+[m[32m    process.env.DEBUG = namespaces;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Load `namespaces`.[m
[32m+[m[32m *[m
[32m+[m[32m * @return {String} returns the previously persisted debug modes[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction load() {[m
[32m+[m[32m  return process.env.DEBUG;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Copied from `node/src/node.js`.[m
[32m+[m[32m *[m
[32m+[m[32m * XXX: It's lame that node doesn't expose this API out-of-the-box. It also[m
[32m+[m[32m * relies on the undocumented `tty_wrap.guessHandleType()` which is also lame.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createWritableStdioStream (fd) {[m
[32m+[m[32m  var stream;[m
[32m+[m[32m  var tty_wrap = process.binding('tty_wrap');[m
[32m+[m
[32m+[m[32m  // Note stream._type is used for test-module-load-list.js[m
[32m+[m
[32m+[m[32m  switch (tty_wrap.guessHandleType(fd)) {[m
[32m+[m[32m    case 'TTY':[m
[32m+[m[32m      stream = new tty.WriteStream(fd);[m
[32m+[m[32m      stream._type = 'tty';[m
[32m+[m
[32m+[m[32m      // Hack to have stream not keep the event loop alive.[m
[32m+[m[32m      // See https://github.com/joyent/node/issues/1726[m
[32m+[m[32m      if (stream._handle && stream._handle.unref) {[m
[32m+[m[32m        stream._handle.unref();[m
[32m+[m[32m      }[m
[32m+[m[32m      break;[m
[32m+[m
[32m+[m[32m    case 'FILE':[m
[32m+[m[32m      var fs = require('fs');[m
[32m+[m[32m      stream = new fs.SyncWriteStream(fd, { autoClose: false });[m
[32m+[m[32m      stream._type = 'fs';[m
[32m+[m[32m      break;[m
[32m+[m
[32m+[m[32m    case 'PIPE':[m
[32m+[m[32m    case 'TCP':[m
[32m+[m[32m      var net = require('net');[m
[32m+[m[32m      stream = new net.Socket({[m
[32m+[m[32m        fd: fd,[m
[32m+[m[32m        readable: false,[m
[32m+[m[32m        writable: true[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      // FIXME Should probably have an option in net.Socket to create a[m
[32m+[m[32m      // stream from an existing fd which is writable only. But for now[m
[32m+[m[32m      // we'll just add this hack and set the `readable` member to false.[m
[32m+[m[32m      // Test: ./node test/fixtures/echo.js < /etc/passwd[m
[32m+[m[32m      stream.readable = false;[m
[32m+[m[32m      stream.read = null;[m
[32m+[m[32m      stream._type = 'pipe';[m
[32m+[m
[32m+[m[32m      // FIXME Hack to have stream not keep the event loop alive.[m
[32m+[m[32m      // See https://github.com/joyent/node/issues/1726[m
[32m+[m[32m      if (stream._handle && stream._handle.unref) {[m
[32m+[m[32m        stream._handle.unref();[m
[32m+[m[32m      }[m
[32m+[m[32m      break;[m
[32m+[m
[32m+[m[32m    default:[m
[32m+[m[32m      // Probably an error on in uv_guess_handle()[m
[32m+[m[32m      throw new Error('Implement me. Unknown stream file type!');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // For supporting legacy API we put the FD here.[m
[32m+[m[32m  stream.fd = fd;[m
[32m+[m
[32m+[m[32m  stream._isStdio = true;[m
[32m+[m
[32m+[m[32m  return stream;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Init logic for `debug` instances.[m
[32m+[m[32m *[m
[32m+[m[32m * Create a new `inspectOpts` object in case `useColors` is set[m
[32m+[m[32m * differently for a particular `debug` instance.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction init (debug) {[m
[32m+[m[32m  debug.inspectOpts = {};[m
[32m+[m
[32m+[m[32m  var keys = Object.keys(exports.inspectOpts);[m
[32m+[m[32m  for (var i = 0; i < keys.length; i++) {[m
[32m+[m[32m    debug.inspectOpts[keys[i]] = exports.inspectOpts[keys[i]];[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Enable namespaces listed in `process.env.DEBUG` initially.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.enable(load());[m
[1mdiff --git a/node_modules/depd/History.md b/node_modules/depd/History.md[m
[1mnew file mode 100644[m
[1mindex 0000000..507ecb8[m
[1m--- /dev/null[m
[1m+++ b/node_modules/depd/History.md[m
[36m@@ -0,0 +1,96 @@[m

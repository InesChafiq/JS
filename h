[36m@@ -0,0 +1,504 @@[m
[32m+[m[32m# Define the main object[m
[32m+[m[32mipaddr = {}[m
[32m+[m
[32m+[m[32mroot = this[m
[32m+[m
[32m+[m[32m# Export for both the CommonJS and browser-like environment[m
[32m+[m[32mif module? && module.exports[m
[32m+[m[32m  module.exports = ipaddr[m
[32m+[m[32melse[m
[32m+[m[32m  root['ipaddr'] = ipaddr[m
[32m+[m
[32m+[m[32m# A generic CIDR (Classless Inter-Domain Routing) RFC1518 range matcher.[m
[32m+[m[32mmatchCIDR = (first, second, partSize, cidrBits) ->[m
[32m+[m[32m  if first.length != second.length[m
[32m+[m[32m    throw new Error "ipaddr: cannot match CIDR for objects with different lengths"[m
[32m+[m
[32m+[m[32m  part = 0[m
[32m+[m[32m  while cidrBits > 0[m
[32m+[m[32m    shift = partSize - cidrBits[m
[32m+[m[32m    shift = 0 if shift < 0[m
[32m+[m
[32m+[m[32m    if first[part] >> shift != second[part] >> shift[m
[32m+[m[32m      return false[m
[32m+[m
[32m+[m[32m    cidrBits -= partSize[m
[32m+[m[32m    part     += 1[m
[32m+[m
[32m+[m[32m  return true[m
[32m+[m
[32m+[m[32m# An utility function to ease named range matching. See examples below.[m
[32m+[m[32mipaddr.subnetMatch = (address, rangeList, defaultName='unicast') ->[m
[32m+[m[32m  for rangeName, rangeSubnets of rangeList[m
[32m+[m[32m    # ECMA5 Array.isArray isn't available everywhere[m
[32m+[m[32m    if rangeSubnets[0] && !(rangeSubnets[0] instanceof Array)[m
[32m+[m[32m      rangeSubnets = [ rangeSubnets ][m
[32m+[m
[32m+[m[32m    for subnet in rangeSubnets[m
[32m+[m[32m      return rangeName if address.match.apply(address, subnet)[m
[32m+[m
[32m+[m[32m  return defaultName[m
[32m+[m
[32m+[m[32m# An IPv4 address (RFC791).[m
[32m+[m[32mclass ipaddr.IPv4[m
[32m+[m[32m  # Constructs a new IPv4 address from an array of four octets[m
[32m+[m[32m  # in network order (MSB first)[m
[32m+[m[32m  # Verifies the input.[m
[32m+[m[32m  constructor: (octets) ->[m
[32m+[m[32m    if octets.length != 4[m
[32m+[m[32m      throw new Error "ipaddr: ipv4 octet count should be 4"[m
[32m+[m
[32m+[m[32m    for octet in octets[m
[32m+[m[32m      if !(0 <= octet <= 255)[m
[32m+[m[32m        throw new Error "ipaddr: ipv4 octet should fit in 8 bits"[m
[32m+[m
[32m+[m[32m    @octets = octets[m
[32m+[m
[32m+[m[32m  # The 'kind' method exists on both IPv4 and IPv6 classes.[m
[32m+[m[32m  kind: ->[m
[32m+[m[32m    return 'ipv4'[m
[32m+[m
[32m+[m[32m  # Returns the address in convenient, decimal-dotted format.[m
[32m+[m[32m  toString: ->[m
[32m+[m[32m    return @octets.join "."[m
[32m+[m
[32m+[m[32m  # Returns an array of byte-sized values in network order (MSB first)[m
[32m+[m[32m  toByteArray: ->[m
[32m+[m[32m    return @octets.slice(0) # octets.clone[m
[32m+[m
[32m+[m[32m  # Checks if this address matches other one within given CIDR range.[m
[32m+[m[32m  match: (other, cidrRange) ->[m
[32m+[m[32m    if cidrRange == undefined[m
[32m+[m[32m      [other, cidrRange] = other[m
[32m+[m
[32m+[m[32m    if other.kind() != 'ipv4'[m
[32m+[m[32m      throw new Error "ipaddr: cannot match ipv4 address with non-ipv4 one"[m
[32m+[m
[32m+[m[32m    return matchCIDR(this.octets, other.octets, 8, cidrRange)[m
[32m+[m
[32m+[m[32m  # Special IPv4 address ranges.[m
[32m+[m[32m  # See also https://en.wikipedia.org/wiki/Reserved_IP_addresses[m
[32m+[m[32m  SpecialRanges:[m
[32m+[m[32m    unspecified: [[m
[32m+[m[32m      [ new IPv4([0,     0,    0,   0]),  8 ][m
[32m+[m[32m    ][m
[32m+[m[32m    broadcast: [[m
[32m+[m[32m      [ new IPv4([255, 255,  255, 255]), 32 ][m
[32m+[m[32m    ][m
[32m+[m[32m    multicast: [ # RFC3171[m
[32m+[m[32m      [ new IPv4([224,   0,    0,   0]), 4  ][m
[32m+[m[32m    ][m
[32m+[m[32m    linkLocal: [ # RFC3927[m
[32m+[m[32m      [ new IPv4([169,   254,  0,   0]), 16 ][m
[32m+[m[32m    ][m
[32m+[m[32m    loopback: [ # RFC5735[m
[32m+[m[32m      [ new IPv4([127,   0,    0,   0]), 8  ][m
[32m+[m[32m    ][m
[32m+[m[32m    carrierGradeNat: [ # RFC6598[m
[32m+[m[32m      [ new IPv4([100,   64,   0,   0]), 10 ][m
[32m+[m[32m    ][m
[32m+[m[32m    private: [ # RFC1918[m
[32m+[m[32m      [ new IPv4([10,    0,    0,   0]), 8  ][m
[32m+[m[32m      [ new IPv4([172,   16,   0,   0]), 12 ][m
[32m+[m[32m      [ new IPv4([192,   168,  0,   0]), 16 ][m
[32m+[m[32m    ][m
[32m+[m[32m    reserved: [ # Reserved and testing-only ranges; RFCs 5735, 5737, 2544, 1700[m
[32m+[m[32m      [ new IPv4([192,   0,    0,   0]), 24 ][m
[32m+[m[32m      [ new IPv4([192,   0,    2,   0]), 24 ][m
[32m+[m[32m      [ new IPv4([192,  88,   99,   0]), 24 ][m
[32m+[m[32m      [ new IPv4([198,  51,  100,   0]), 24 ][m
[32m+[m[32m      [ new IPv4([203,   0,  113,   0]), 24 ][m
[32m+[m[32m      [ new IPv4([240,   0,    0,   0]), 4  ][m
[32m+[m[32m    ][m
[32m+[m
[32m+[m[32m  # Checks if the address corresponds to one of the special ranges.[m
[32m+[m[32m  range: ->[m
[32m+[m[32m    return ipaddr.subnetMatch(this, @SpecialRanges)[m
[32m+[m
[32m+[m[32m  # Convrets this IPv4 address to an IPv4-mapped IPv6 address.[m
[32m+[m[32m  toIPv4MappedAddress: ->[m
[32m+[m[32m    return ipaddr.IPv6.parse "::ffff:#{@toString()}"[m
[32m+[m
[32m+[m[32m  # returns a number of leading ones in IPv4 address, making sure that[m
[32m+[m[32m  # the rest is a solid sequence of 0's (valid netmask)[m
[32m+[m[32m  # returns either the CIDR length or null if mask is not valid[m
[32m+[m[32m  prefixLengthFromSubnetMask: ->[m
[32m+[m[32m    # number of zeroes in octet[m
[32m+[m[32m    zerotable =[m
[32m+[m[32m      0:   8[m
[32m+[m[32m      128: 7[m
[32m+[m[32m      192: 6[m
[32m+[m[32m      224: 5[m
[32m+[m[32m      240: 4[m
[32m+[m[32m      248: 3[m
[32m+[m[32m      252: 2[m
[32m+[m[32m      254: 1[m
[32m+[m[32m      255: 0[m
[32m+[m
[32m+[m[32m    cidr = 0[m
[32m+[m[32m    # non-zero encountered stop scanning for zeroes[m
[32m+[m[32m    stop = false[m
[32m+[m[32m    for i in [3..0] by -1[m
[32m+[m[32m      octet = @octets[i][m
[32m+[m[32m      if octet of zerotable[m
[32m+[m[32m        zeros = zerotable[octet][m
[32m+[m[32m        if stop and zeros != 0[m
[32m+[m[32m          return null[m
[32m+[m[32m        unless zeros == 8[m
[32m+[m[32m          stop = true[m
[32m+[m[32m        cidr += zeros[m
[32m+[m[32m      else[m
[32m+[m[32m        return null[m
[32m+[m[32m    return 32 - cidr[m
[32m+[m
[32m+[m[32m# A list of regular expressions that match arbitrary IPv4 addresses,[m
[32m+[m[32m# for which a number of weird notations exist.[m
[32m+[m[32m# Note that an address like 0010.0xa5.1.1 is considered legal.[m
[32m+[m[32mipv4Part = "(0?\\d+|0x[a-f0-9]+)"[m
[32m+[m[32mipv4Regexes =[m
[32m+[m[32m  fourOctet: new RegExp "^#{ipv4Part}\\.#{ipv4Part}\\.#{ipv4Part}\\.#{ipv4Part}$", 'i'[m
[32m+[m[32m  longValue: new RegExp "^#{ipv4Part}$", 'i'[m
[32m+[m
[32m+[m[32m# Classful variants (like a.b, where a is an octet, and b is a 24-bit[m
[32m+[m[32m# value representing last three octets; this corresponds to a class C[m
[32m+[m[32m# address) are omitted due to classless nature of modern Internet.[m
[32m+[m[32mipaddr.IPv4.parser = (string) ->[m
[32m+[m[32m  parseIntAuto = (string) ->[m
[32m+[m[32m    if string[0] == "0" && string[1] != "x"[m
[32m+[m[32m      parseInt(string, 8)[m
[32m+[m[32m    else[m
[32m+[m[32m      parseInt(string)[m
[32m+[m
[32m+[m[32m  # parseInt recognizes all that octal & hexadecimal weirdness for us[m
[32m+[m[32m  if match = string.match(ipv4Regexes.fourOctet)[m
[32m+[m[32m    return (parseIntAuto(part) for part in match[1..5])[m
[32m+[m[32m  else if match = string.match(ipv4Regexes.longValue)[m
[32m+[m[32m    value = parseIntAuto(match[1])[m
[32m+[m[32m    if value > 0xffffffff || value < 0[m
[32m+[m[32m      throw new Error "ipaddr: address outside defined range"[m
[32m+[m[32m    return ((value >> shift) & 0xff for shift in [0..24] by 8).reverse()[m
[32m+[m[32m  else[m
[32m+[m[32m    return null[m
[32m+[m
[32m+[m[32m# An IPv6 address (RFC2460)[m
[32m+[m[32mclass ipaddr.IPv6[m
[32m+[m[32m  # Constructs an IPv6 address from an array of eight 16-bit parts[m
[32m+[m[32m  # or sixteen 8-bit parts in network order (MSB first).[m
[32m+[m[32m  # Throws an error if the input is invalid.[m
[32m+[m[32m  constructor: (parts) ->[m
[32m+[m[32m    if parts.length == 16[m
[32m+[m[32m      @parts = [][m
[32m+[m[32m      for i in [0..14] by 2[m
[32m+[m[32m        @parts.push((parts[i] << 8) | parts[i + 1])[m
[32m+[m[32m    else if parts.length == 8[m
[32m+[m[32m      @parts = parts[m
[32m+[m[32m    else[m
[32m+[m[32m      throw new Error "ipaddr: ipv6 part count should be 8 or 16"[m
[32m+[m
[32m+[m[32m    for part in @parts[m
[32m+[m[32m      if !(0 <= part <= 0xffff)[m
[32m+[m[32m        throw new Error "ipaddr: ipv6 part should fit in 16 bits"[m
[32m+[m
[32m+[m[32m  # The 'kind' method exists on both IPv4 and IPv6 classes.[m
[32m+[m[32m  kind: ->[m
[32m+[m[32m    return 'ipv6'[m
[32m+[m
[32m+[m[32m  # Returns the address in compact, human-readable format like[m
[32m+[m[32m  # 2001:db8:8:66::1[m
[32m+[m[32m  toString: ->[m
[32m+[m[32m    stringParts = (part.toString(16) for part in @parts)[m
[32m+[m
[32m+[m[32m    compactStringParts = [][m
[32m+[m[32m    pushPart = (part) -> compactStringParts.push part[m
[32m+[m
[32m+[m[32m    state = 0[m
[32m+[m[32m    for part in stringParts[m
[32m+[m[32m      switch state[m
[32m+[m[32m        when 0[m
[32m+[m[32m          if part == '0'[m
[32m+[m[32m            pushPart('')[m
[32m+[m[32m          else[m
[32m+[m[32m            pushPart(part)[m
[32m+[m
[32m+[m[32m          state = 1[m
[32m+[m[32m        when 1[m
[32m+[m[32m          if part == '0'[m
[32m+[m[32m            state = 2[m
[32m+[m[32m          else[m
[32m+[m[32m            pushPart(part)[m
[32m+[m[32m        when 2[m
[32m+[m[32m          unless part == '0'[m
[32m+[m[32m            pushPart('')[m
[32m+[m[32m            pushPart(part)[m
[32m+[m[32m            state = 3[m
[32m+[m[32m        when 3[m
[32m+[m[32m          pushPart(part)[m
[32m+[m
[32m+[m[32m    if state == 2[m
[32m+[m[32m      pushPart('')[m
[32m+[m[32m      pushPart('')[m
[32m+[m
[32m+[m[32m    return compactStringParts.join ":"[m
[32m+[m
[32m+[m[32m  # Returns an array of byte-sized values in network order (MSB first)[m
[32m+[m[32m  toByteArray: ->[m
[32m+[m[32m    bytes = [][m
[32m+[m[32m    for part in @parts[m
[32m+[m[32m      bytes.push(part >> 8)[m
[32m+[m[32m      bytes.push(part & 0xff)[m
[32m+[m
[32m+[m[32m    return bytes[m
[32m+[m
[32m+[m[32m  # Returns the address in expanded format with all zeroes included, like[m
[32m+[m[32m  # 2001:db8:8:66:0:0:0:1[m
[32m+[m[32m  toNormalizedString: ->[m
[32m+[m[32m    return (part.toString(16) for part in @parts).join ":"[m
[32m+[m
[32m+[m[32m  # Checks if this address matches other one within given CIDR range.[m
[32m+[m[32m  match: (other, cidrRange) ->[m
[32m+[m[32m    if cidrRange == undefined[m
[32m+[m[32m      [other, cidrRange] = other[m
[32m+[m
[32m+[m[32m    if other.kind() != 'ipv6'[m
[32m+[m[32m      throw new Error "ipaddr: cannot match ipv6 address with non-ipv6 one"[m
[32m+[m
[32m+[m[32m    return matchCIDR(this.parts, other.parts, 16, cidrRange)[m
[32m+[m
[32m+[m[32m  # Special IPv6 ranges[m
[32m+[m[32m  SpecialRanges:[m
[32m+[m[32m    unspecified: [ new IPv6([0,      0,      0, 0, 0,      0,      0, 0]), 128 ] # RFC4291, here and after[m
[32m+[m[32m    linkLocal:   [ new IPv6([0xfe80, 0,      0, 0, 0,      0,      0, 0]), 10  ][m
[32m+[m[32m    multicast:   [ new IPv6([0xff00, 0,      0, 0, 0,      0,      0, 0]), 8   ][m
[32m+[m[32m    loopback:    [ new IPv6([0,      0,      0, 0, 0,      0,      0, 1]), 128 ][m
[32m+[m[32m    uniqueLocal: [ new IPv6([0xfc00, 0,      0, 0, 0,      0,      0, 0]), 7   ][m
[32m+[m[32m    ipv4Mapped:  [ new IPv6([0,      0,      0, 0, 0,      0xffff, 0, 0]), 96  ][m
[32m+[m[32m    rfc6145:     [ new IPv6([0,      0,      0, 0, 0xffff, 0,      0, 0]), 96  ] # RFC6145[m
[32m+[m[32m    rfc6052:     [ new IPv6([0x64,   0xff9b, 0, 0, 0,      0,      0, 0]), 96  ] # RFC6052[m
[32m+[m[32m    '6to4':      [ new IPv6([0x2002, 0,      0, 0, 0,      0,      0, 0]), 16  ] # RFC3056[m
[32m+[m[32m    teredo:      [ new IPv6([0x2001, 0,      0, 0, 0,      0,      0, 0]), 32  ] # RFC6052, RFC6146[m
[32m+[m[32m    reserved: [[m
[32m+[m[32m      [ new IPv6([ 0x2001, 0xdb8, 0, 0, 0, 0, 0, 0]), 32 ] # RFC4291[m
[32m+[m[32m    ][m
[32m+[m
[32m+[m[32m  # Checks if the address corresponds to one of the special ranges.[m
[32m+[m[32m  range: ->[m
[32m+[m[32m    return ipaddr.subnetMatch(this, @SpecialRanges)[m
[32m+[m
[32m+[m[32m  # Checks if this address is an IPv4-mapped IPv6 address.[m
[32m+[m[32m  isIPv4MappedAddress: ->[m
[32m+[m[32m    return @range() == 'ipv4Mapped'[m
[32m+[m
[32m+[m[32m  # Converts this address to IPv4 address if it is an IPv4-mapped IPv6 address.[m
[32m+[m[32m  # Throws an error otherwise.[m
[32m+[m[32m  toIPv4Address: ->[m
[32m+[m[32m    unless @isIPv4MappedAddress()[m
[32m+[m[32m      throw new Error "ipaddr: trying to convert a generic ipv6 address to ipv4"[m
[32m+[m
[32m+[m[32m    [high, low] = @parts[-2..-1][m
[32m+[m
[32m+[m[32m    return new ipaddr.IPv4([high >> 8, high & 0xff, low >> 8, low & 0xff])[m
[32m+[m
[32m+[m[32m# IPv6-matching regular expressions.[m
[32m+[m[32m# For IPv6, the task is simpler: it is enough to match the colon-delimited[m
[32m+[m[32m# hexadecimal IPv6 and a transitional variant with dotted-decimal IPv4 at[m
[32m+[m[32m# the end.[m
[32m+[m[32mipv6Part = "(?:[0-9a-f]+::?)+"[m
[32m+[m[32mipv6Regexes =[m
[32m+[m[32m  native:       new RegExp "^(::)?(#{ipv6Part})?([0-9a-f]+)?(::)?$", 'i'[m
[32m+[m[32m  transitional: new RegExp "^((?:#{ipv6Part})|(?:::)(?:#{ipv6Part})?)" +[m
[32m+[m[32m                           "#{ipv4Part}\\.#{ipv4Part}\\.#{ipv4Part}\\.#{ipv4Part}$", 'i'[m
[32m+[m
[32m+[m[32m# Expand :: in an IPv6 address or address part consisting of `parts` groups.[m
[32m+[m[32mexpandIPv6 = (string, parts) ->[m
[32m+[m[32m  # More than one '::' means invalid adddress[m
[32m+[m[32m  if string.indexOf('::') != string.lastIndexOf('::')[m
[32m+[m[32m    return null[m
[32m+[m
[32m+[m[32m  # How many parts do we already have?[m
[32m+[m[32m  colonCount = 0[m
[32m+[m[32m  lastColon = -1[m
[32m+[m[32m  while (lastColon = string.indexOf(':', lastColon + 1)) >= 0[m
[32m+[m[32m    colonCount++[m
[32m+[m
[32m+[m[32m  # 0::0 is two parts more than ::[m
[32m+[m[32m  colonCount-- if string.substr(0, 2) == '::'[m
[32m+[m[32m  colonCount-- if string.substr(-2, 2) == '::'[m
[32m+[m
[32m+[m[32m  # The following loop would hang if colonCount > parts[m
[32m+[m[32m  if colonCount > parts[m
[32m+[m[32m    return null[m
[32m+[m
[32m+[m[32m  # replacement = ':' + '0:' * (parts - colonCount)[m
[32m+[m[32m  replacementCount = parts - colonCount[m
[32m+[m[32m  replacement = ':'[m
[32m+[m[32m  while replacementCount--[m
[32m+[m[32m    replacement += '0:'[m
[32m+[m
[32m+[m[32m  # Insert the missing zeroes[m
[32m+[m[32m  string = string.replace('::', replacement)[m
[32m+[m
[32m+[m[32m  # Trim any garbage which may be hanging around if :: was at the edge in[m
[32m+[m[32m  # the source string[m
[32m+[m[32m  string = string[1..-1] if string[0] == ':'[m
[32m+[m[32m  string = string[0..-2] if string[string.length-1] == ':'[m
[32m+[m
[32m+[m[32m  return (parseInt(part, 16) for part in string.split(":"))[m
[32m+[m
[32m+[m[32m# Parse an IPv6 address.[m
[32m+[m[32mipaddr.IPv6.parser = (string) ->[m
[32m+[m[32m  if string.match(ipv6Regexes['native'])[m
[32m+[m[32m    return expandIPv6(string, 8)[m
[32m+[m
[32m+[m[32m  else if match = string.match(ipv6Regexes['transitional'])[m
[32m+[m[32m    parts = expandIPv6(match[1][0..-2], 6)[m
[32m+[m[32m    if parts[m
[32m+[m[32m      octets = [parseInt(match[2]), parseInt(match[3]),[m
[32m+[m[32m                parseInt(match[4]), parseInt(match[5])][m
[32m+[m[32m      for octet in octets[m
[32m+[m[32m        if !(0 <= octet <= 255)[m
[32m+[m[32m          return null[m
[32m+[m
[32m+[m[32m      parts.push(octets[0] << 8 | octets[1])[m
[32m+[m[32m      parts.push(octets[2] << 8 | octets[3])[m
[32m+[m[32m      return parts[m
[32m+[m
[32m+[m[32m  return null[m
[32m+[m
[32m+[m[32m# Checks if a given string is formatted like IPv4/IPv6 address.[m
[32m+[m[32mipaddr.IPv4.isIPv4 = ipaddr.IPv6.isIPv6 = (string) ->[m
[32m+[m[32m  return @parser(string) != null[m
[32m+[m
[32m+[m[32m# Checks if a given string is a valid IPv4/IPv6 address.[m
[32m+[m[32mipaddr.IPv4.isValid = (string) ->[m
[32m+[m[32m  try[m
[32m+[m[32m    new this(@parser(string))[m
[32m+[m[32m    return true[m
[32m+[m[32m  catch e[m
[32m+[m[32m    return false[m
[32m+[m
[32m+[m[32mipaddr.IPv4.isValidFourPartDecimal = (string) ->[m
[32m+[m[32m  if ipaddr.IPv4.isValid(string) and string.match(/^\d+(\.\d+){3}$/)[m
[32m+[m[32m    return true[m
[32m+[m[32m  else[m
[32m+[m[32m    return false[m
[32m+[m
[32m+[m[32mipaddr.IPv6.isValid = (string) ->[m
[32m+[m[32m  # Since IPv6.isValid is always called first, this shortcut[m
[32m+[m[32m  # provides a substantial performance gain.[m
[32m+[m[32m  if typeof string == "string" and string.indexOf(":") == -1[m
[32m+[m[32m    return false[m
[32m+[m
[32m+[m[32m  try[m
[32m+[m[32m    new this(@parser(string))[m
[32m+[m[32m    return true[m
[32m+[m[32m  catch e[m
[32m+[m[32m    return false[m
[32m+[m
[32m+[m[32m# Tries to parse and validate a string with IPv4/IPv6 address.[m
[32m+[m[32m# Throws an error if it fails.[m
[32m+[m[32mipaddr.IPv4.parse = ipaddr.IPv6.parse = (string) ->[m
[32m+[m[32m  parts = @parser(string)[m
[32m+[m[32m  if parts == null[m
[32m+[m[32m    throw new Error "ipaddr: string is not formatted like ip address"[m
[32m+[m
[32m+[m[32m  return new this(parts)[m
[32m+[m
[32m+[m[32mipaddr.IPv4.parseCIDR = (string) ->[m
[32m+[m[32m  if match = string.match(/^(.+)\/(\d+)$/)[m
[32m+[m[32m    maskLength = parseInt(match[2])[m
[32m+[m[32m    if maskLength >= 0 and maskLength <= 32[m
[32m+[m[32m      return [@parse(match[1]), maskLength][m
[32m+[m
[32m+[m[32m  throw new Error "ipaddr: string is not formatted like an IPv4 CIDR range"[m
[32m+[m
[32m+[m[32m# A utility function to return subnet mask in IPv4 format given the prefix length[m
[32m+[m[32mipaddr.IPv4.subnetMaskFromPrefixLength = (prefix) ->[m
[32m+[m[32m  if prefix < 0 or prefix > 32[m
[32m+[m[32m    throw new Error('ipaddr: invalid prefix length')[m
[32m+[m[32m  octets = Array(4).fill(0)[m
[32m+[m[32m  j = 0[m
[32m+[m[32m  while j < Math.floor(prefix / 8)[m
[32m+[m[32m    octets[j] = 255[m
[32m+[m[32m    j++[m
[32m+[m[32m  octets[Math.floor(prefix / 8)] = Math.pow(2, (prefix % 8)) - 1 << 8 - (prefix % 8)[m
[32m+[m[32m  new (ipaddr.IPv4)(octets)[m
[32m+[m
[32m+[m[32m# A utility function to return broadcast address given the IPv4 interface and prefix length in CIDR notation[m
[32m+[m[32mipaddr.IPv4.broadcastAddressFromCIDR = (string) ->[m
[32m+[m[32m  try[m
[32m+[m[32m    ipInterface = ipaddr.IPv4.parseCIDR(string)[0][m
[32m+[m[32m    subnetMask = @subnetMaskFromPrefixLength([ ipaddr.IPv4.parseCIDR(string)[1] ])[m
[32m+[m[32m    octets = [][m
[32m+[m[32m    i = 0[m
[32m+[m[32m    while i < 4[m
[32m+[m[32m      # Broadcast address is bitwise OR between ip interface and inverted mask[m
[32m+[m[32m      octets.push parseInt(ipInterface.octets[i], 10) | parseInt(subnetMask.octets[i], 10) ^ 255[m
[32m+[m[32m      i++[m
[32m+[m[32m    return new (ipaddr.IPv4)(octets)[m
[32m+[m[32m  catch error[m
[32m+[m[32m    throw new Error('ipaddr: the address does not have IPv4 CIDR format')[m
[32m+[m[32m  return[m
[32m+[m
[32m+[m[32m# A utility function to return network address given the IPv4 interface and prefix length in CIDR notation[m
[32m+[m[32mipaddr.IPv4.networkAddressFromCIDR = (string) ->[m
[32m+[m[32m  try[m
[32m+[m[32m    ipInterface = ipaddr.IPv4.parseCIDR(string)[0][m
[32m+[m[32m    subnetMask = @subnetMaskFromPrefixLength([ ipaddr.IPv4.parseCIDR(string)[1] ])[m
[32m+[m[32m    octets = [][m
[32m+[m[32m    i = 0[m
[32m+[m[32m    while i < 4[m
[32m+[m[32m      # Network address is bitwise AND between ip interface and mask[m
[32m+[m[32m      octets.push parseInt(ipInterface.octets[i], 10) & parseInt(subnetMask.octets[i], 10)[m
[32m+[m[32m      i++[m
[32m+[m[32m    return new (ipaddr.IPv4)(octets)[m
[32m+[m[32m  catch error[m
[32m+[m[32m    throw new Error('ipaddr: the address does not have IPv4 CIDR format')[m
[32m+[m[32m  return[m
[32m+[m
[32m+[m[32mipaddr.IPv6.parseCIDR = (string) ->[m
[32m+[m[32m  if match = string.match(/^(.+)\/(\d+)$/)[m
[32m+[m[32m    maskLength = parseInt(match[2])[m
[32m+[m[32m    if maskLength >= 0 and maskLength <= 128[m
[32m+[m[32m      return [@parse(match[1]), maskLength][m
[32m+[m
[32m+[m[32m  throw new Error "ipaddr: string is not formatted like an IPv6 CIDR range"[m
[32m+[m
[32m+[m[32m# Checks if the address is valid IP address[m
[32m+[m[32mipaddr.isValid = (string) ->[m
[32m+[m[32m  return ipaddr.IPv6.isValid(string) || ipaddr.IPv4.isValid(string)[m
[32m+[m
[32m+[m[32m# Try to parse an address and throw an error if it is impossible[m
[32m+[m[32mipaddr.parse = (string) ->[m
[32m+[m[32m  if ipaddr.IPv6.isValid(string)[m
[32m+[m[32m    return ipaddr.IPv6.parse(string)[m
[32m+[m[32m  else if ipaddr.IPv4.isValid(string)[m
[32m+[m[32m    return ipaddr.IPv4.parse(string)[m
[32m+[m[32m  else[m
[32m+[m[32m    throw new Error "ipaddr: the address has neither IPv6 nor IPv4 format"[m
[32m+[m
[32m+[m[32mipaddr.parseCIDR = (string) ->[m
[32m+[m[32m  try[m
[32m+[m[32m    return ipaddr.IPv6.parseCIDR(string)[m
[32m+[m[32m  catch e[m
[32m+[m[32m    try[m
[32m+[m[32m      return ipaddr.IPv4.parseCIDR(string)[m
[32m+[m[32m    catch e[m
[32m+[m[32m      throw new Error "ipaddr: the address has neither IPv6 nor IPv4 CIDR format"[m
[32m+[m
[32m+[m[32m# Try to parse an array in network order (MSB first) for IPv4 and IPv6[m
[32m+[m[32mipaddr.fromByteArray = (bytes) ->[m
[32m+[m[32m  length = bytes.length[m
[32m+[m[32m  if length == 4[m
[32m+[m[32m    return new ipaddr.IPv4(bytes)[m
[32m+[m[32m  else if length == 16[m
[32m+[m[32m    return new ipaddr.IPv6(bytes)[m
[32m+[m[32m  else[m
[32m+[m[32m    throw new Error "ipaddr: the binary input is neither an IPv6 nor IPv4 address"[m
[32m+[m
[32m+[m[32m# Parse an address and return plain IPv4 address if it is an IPv4-mapped address[m
[32m+[m[32mipaddr.process = (string) ->[m
[32m+[m[32m  addr = @parse(string)[m
[32m+[m[32m  if addr.kind() == 'ipv6' && addr.isIPv4MappedAddress()[m
[32m+[m[32m    return addr.toIPv4Address()[m
[32m+[m[32m  else[m
[32m+[m[32m    return addr[m

[32m+[m[32m  },[m
[32m+[m[32m  "bundleDependencies": false,[m
[32m+[m[32m  "dependencies": {},[m
[32m+[m[32m  "deprecated": false,[m
[32m+[m[32m  "description": "Sign and unsign cookies",[m
[32m+[m[32m  "devDependencies": {[m
[32m+[m[32m    "mocha": "*",[m
[32m+[m[32m    "should": "*"[m
[32m+[m[32m  },[m
[32m+[m[32m  "homepage": "https://github.com/visionmedia/node-cookie-signature#readme",[m
[32m+[m[32m  "keywords": [[m
[32m+[m[32m    "cookie",[m
[32m+[m[32m    "sign",[m
[32m+[m[32m    "unsign"[m
[32m+[m[32m  ],[m
[32m+[m[32m  "license": "MIT",[m
[32m+[m[32m  "main": "index",[m
[32m+[m[32m  "name": "cookie-signature",[m
[32m+[m[32m  "repository": {[m
[32m+[m[32m    "type": "git",[m
[32m+[m[32m    "url": "git+https://github.com/visionmedia/node-cookie-signature.git"[m
[32m+[m[32m  },[m
[32m+[m[32m  "scripts": {[m
[32m+[m[32m    "test": "mocha --require should --reporter spec"[m
[32m+[m[32m  },[m

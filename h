[1mnew file mode 100644[m
[1mindex 0000000..3f77391[m
[1m--- /dev/null[m
[1m+++ b/node_modules/serve-static/index.js[m
[36m@@ -0,0 +1,209 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * serve-static[m
[32m+[m[32m * Copyright(c) 2010 Sencha Inc.[m
[32m+[m[32m * Copyright(c) 2011 TJ Holowaychuk[m
[32m+[m[32m * Copyright(c) 2014-2016 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar encodeUrl = require('encodeurl')[m
[32m+[m[32mvar escapeHtml = require('escape-html')[m
[32m+[m[32mvar parseUrl = require('parseurl')[m
[32m+[m[32mvar resolve = require('path').resolve[m
[32m+[m[32mvar send = require('send')[m
[32m+[m[32mvar url = require('url')[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = serveStatic[m
[32m+[m[32mmodule.exports.mime = send.mime[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * @param {string} root[m
[32m+[m[32m * @param {object} [options][m
[32m+[m[32m * @return {function}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction serveStatic (root, options) {[m
[32m+[m[32m  if (!root) {[m
[32m+[m[32m    throw new TypeError('root path required')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (typeof root !== 'string') {[m
[32m+[m[32m    throw new TypeError('root path must be a string')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // copy options object[m
[32m+[m[32m  var opts = Object.create(options || null)[m
[32m+[m
[32m+[m[32m  // fall-though[m
[32m+[m[32m  var fallthrough = opts.fallthrough !== false[m
[32m+[m
[32m+[m[32m  // default redirect[m
[32m+[m[32m  var redirect = opts.redirect !== false[m
[32m+[m
[32m+[m[32m  // headers listener[m
[32m+[m[32m  var setHeaders = opts.setHeaders[m
[32m+[m
[32m+[m[32m  if (setHeaders && typeof setHeaders !== 'function') {[m
[32m+[m[32m    throw new TypeError('option setHeaders must be function')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // setup options for send[m
[32m+[m[32m  opts.maxage = opts.maxage || opts.maxAge || 0[m
[32m+[m[32m  opts.root = resolve(root)[m
[32m+[m
[32m+[m[32m  // construct directory listener[m
[32m+[m[32m  var onDirectory = redirect[m
[32m+[m[32m    ? createRedirectDirectoryListener()[m
[32m+[m[32m    : createNotFoundDirectoryListener()[m
[32m+[m
[32m+[m[32m  return function serveStatic (req, res, next) {[m
[32m+[m[32m    if (req.method !== 'GET' && req.method !== 'HEAD') {[m
[32m+[m[32m      if (fallthrough) {[m
[32m+[m[32m        return next()[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // method not allowed[m
[32m+[m[32m      res.statusCode = 405[m
[32m+[m[32m      res.setHeader('Allow', 'GET, HEAD')[m
[32m+[m[32m      res.setHeader('Content-Length', '0')[m
[32m+[m[32m      res.end()[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var forwardError = !fallthrough[m
[32m+[m[32m    var originalUrl = parseUrl.original(req)[m
[32m+[m[32m    var path = parseUrl(req).pathname[m
[32m+[m
[32m+[m[32m    // make sure redirect occurs at mount[m
[32m+[m[32m    if (path === '/' && originalUrl.pathname.substr(-1) !== '/') {[m
[32m+[m[32m      path = ''[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // create send stream[m
[32m+[m[32m    var stream = send(req, path, opts)[m
[32m+[m
[32m+[m[32m    // add directory handler[m
[32m+[m[32m    stream.on('directory', onDirectory)[m
[32m+[m
[32m+[m[32m    // add headers listener[m
[32m+[m[32m    if (setHeaders) {[m
[32m+[m[32m      stream.on('headers', setHeaders)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // add file listener for fallthrough[m
[32m+[m[32m    if (fallthrough) {[m
[32m+[m[32m      stream.on('file', function onFile () {[m
[32m+[m[32m        // once file is determined, always forward error[m
[32m+[m[32m        forwardError = true[m
[32m+[m[32m      })[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // forward errors[m
[32m+[m[32m    stream.on('error', function error (err) {[m
[32m+[m[32m      if (forwardError || !(err.statusCode < 500)) {[m
[32m+[m[32m        next(err)[m
[32m+[m[32m        return[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      next()[m
[32m+[m[32m    })[m
[32m+[m
[32m+[m[32m    // pipe[m
[32m+[m[32m    stream.pipe(res)[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Collapse all leading slashes into a single slash[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m[32mfunction collapseLeadingSlashes (str) {[m
[32m+[m[32m  for (var i = 0; i < str.length; i++) {[m
[32m+[m[32m    if (str.charCodeAt(i) !== 0x2f /* / */) {[m
[32m+[m[32m      break[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return i > 1[m
[32m+[m[32m    ? '/' + str.substr(i)[m
[32m+[m[32m    : str[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m /**[m
[32m+[m[32m * Create a minimal HTML document.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} title[m
[32m+[m[32m * @param {string} body[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createHtmlDocument (title, body) {[m
[32m+[m[32m  return '<!DOCTYPE html>\n' +[m
[32m+[m[32m    '<html lang="en">\n' +[m
[32m+[m[32m    '<head>\n' +[m
[32m+[m[32m    '<meta charset="utf-8">\n' +[m
[32m+[m[32m    '<title>' + title + '</title>\n' +[m
[32m+[m[32m    '</head>\n' +[m
[32m+[m[32m    '<body>\n' +[m
[32m+[m[32m    '<pre>' + body + '</pre>\n' +[m
[32m+[m[32m    '</body>\n'[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a directory listener that just 404s.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createNotFoundDirectoryListener () {[m
[32m+[m[32m  return function notFound () {[m
[32m+[m[32m    this.error(404)[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a directory listener that performs a redirect.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createRedirectDirectoryListener () {[m
[32m+[m[32m  return function redirect (res) {[m
[32m+[m[32m    if (this.hasTrailingSlash()) {[m
[32m+[m[32m      this.error(404)[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // get original URL[m
[32m+[m[32m    var originalUrl = parseUrl.original(this.req)[m
[32m+[m
[32m+[m[32m    // append trailing slash[m
[32m+[m[32m    originalUrl.path = null[m
[32m+[m[32m    originalUrl.pathname = collapseLeadingSlashes(originalUrl.pathname + '/')[m
[32m+[m
[32m+[m[32m    // reformat the URL[m
[32m+[m[32m    var loc = encodeUrl(url.format(originalUrl))[m
[32m+[m[32m    var doc = createHtmlDocument('Redirecting', 'Redirecting to <a href="' + escapeHtml(loc) + '">' +[m
[32m+[m[32m      escapeHtml(loc) + '</a>')[m
[32m+[m
[32m+[m[32m    // send redirect response[m
[32m+[m[32m    res.statusCode = 301[m
[32m+[m[32m    res.setHeader('Content-Type', 'text/html; charset=UTF-8')[m
[32m+[m[32m    res.setHeader('Content-Length', Buffer.byteLength(doc))[m
[32m+[m[32m    res.setHeader('Content-Security-Policy', "default-src 'self'")[m
[32m+[m[32m    res.setHeader('X-Content-Type-Options', 'nosniff')[m
[32m+[m[32m    res.setHeader('Location', loc)[m
[32m+[m[32m    res.end(doc)[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/serve-static/package.json b/node_modules/serve-static/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..786aadf[m
[1m--- /dev/null[m
[1m+++ b/node_modules/serve-static/package.json[m
[36m@@ -0,0 +1,75 @@[m
[32m+[m[32m{[m
[32m+[m[32m  "_from": "serve-static@1.12.6",[m
[32m+[m[32m  "_id": "serve-static@1.12.6",[m

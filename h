[36m@@ -0,0 +1,137 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * fresh[m
[32m+[m[32m * Copyright(c) 2012 TJ Holowaychuk[m
[32m+[m[32m * Copyright(c) 2016-2017 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp to check for no-cache token in Cache-Control.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar CACHE_CONTROL_NO_CACHE_REGEXP = /(?:^|,)\s*?no-cache\s*?(?:,|$)/[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = fresh[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Check freshness of the response using request and response headers.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} reqHeaders[m
[32m+[m[32m * @param {Object} resHeaders[m
[32m+[m[32m * @return {Boolean}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction fresh (reqHeaders, resHeaders) {[m
[32m+[m[32m  // fields[m
[32m+[m[32m  var modifiedSince = reqHeaders['if-modified-since'][m
[32m+[m[32m  var noneMatch = reqHeaders['if-none-match'][m
[32m+[m
[32m+[m[32m  // unconditional request[m
[32m+[m[32m  if (!modifiedSince && !noneMatch) {[m
[32m+[m[32m    return false[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Always return stale when Cache-Control: no-cache[m
[32m+[m[32m  // to support end-to-end reload requests[m
[32m+[m[32m  // https://tools.ietf.org/html/rfc2616#section-14.9.4[m
[32m+[m[32m  var cacheControl = reqHeaders['cache-control'][m
[32m+[m[32m  if (cacheControl && CACHE_CONTROL_NO_CACHE_REGEXP.test(cacheControl)) {[m
[32m+[m[32m    return false[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // if-none-match[m
[32m+[m[32m  if (noneMatch && noneMatch !== '*') {[m
[32m+[m[32m    var etag = resHeaders['etag'][m
[32m+[m
[32m+[m[32m    if (!etag) {[m
[32m+[m[32m      return false[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var etagStale = true[m
[32m+[m[32m    var matches = parseTokenList(noneMatch)[m
[32m+[m[32m    for (var i = 0; i < matches.length; i++) {[m
[32m+[m[32m      var match = matches[i][m
[32m+[m[32m      if (match === etag || match === 'W/' + etag || 'W/' + match === etag) {[m
[32m+[m[32m        etagStale = false[m
[32m+[m[32m        break[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (etagStale) {[m
[32m+[m[32m      return false[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // if-modified-since[m
[32m+[m[32m  if (modifiedSince) {[m
[32m+[m[32m    var lastModified = resHeaders['last-modified'][m
[32m+[m[32m    var modifiedStale = !lastModified || !(parseHttpDate(lastModified) <= parseHttpDate(modifiedSince))[m
[32m+[m
[32m+[m[32m    if (modifiedStale) {[m
[32m+[m[32m      return false[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return true[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse an HTTP Date into a number.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} date[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parseHttpDate (date) {[m
[32m+[m[32m  var timestamp = date && Date.parse(date)[m
[32m+[m
[32m+[m[32m  // istanbul ignore next: guard against date.js Date.parse patching[m
[32m+[m[32m  return typeof timestamp === 'number'[m
[32m+[m[32m    ? timestamp[m
[32m+[m[32m    : NaN[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse a HTTP token list.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} str[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parseTokenList (str) {[m
[32m+[m[32m  var end = 0[m
[32m+[m[32m  var list = [][m
[32m+[m[32m  var start = 0[m
[32m+[m
[32m+[m[32m  // gather tokens[m
[32m+[m[32m  for (var i = 0, len = str.length; i < len; i++) {[m
[32m+[m[32m    switch (str.charCodeAt(i)) {[m
[32m+[m[32m      case 0x20: /*   */[m
[32m+[m[32m        if (start === end) {[m
[32m+[m[32m          start = end = i + 1[m
[32m+[m[32m        }[m
[32m+[m[32m        break[m
[32m+[m[32m      case 0x2c: /* , */[m
[32m+[m[32m        list.push(str.substring(start, end))[m
[32m+[m[32m        start = end = i + 1[m
[32m+[m[32m        break[m
[32m+[m[32m      default:[m
[32m+[m[32m        end = i + 1[m
[32m+[m[32m        break[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // final token[m
[32m+[m[32m  list.push(str.substring(start, end))[m
[32m+[m
[32m+[m[32m  return list[m
[32m+[m[32m}[m

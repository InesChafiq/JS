[1mdiff --git a/node_modules/express/lib/middleware/query.js b/node_modules/express/lib/middleware/query.js[m
[1mnew file mode 100644[m
[1mindex 0000000..7e91669[m
[1m--- /dev/null[m
[1m+++ b/node_modules/express/lib/middleware/query.js[m
[36m@@ -0,0 +1,47 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * express[m
[32m+[m[32m * Copyright(c) 2009-2013 TJ Holowaychuk[m
[32m+[m[32m * Copyright(c) 2013 Roman Shtylman[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar merge = require('utils-merge')[m
[32m+[m[32mvar parseUrl = require('parseurl');[m
[32m+[m[32mvar qs = require('qs');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * @param {Object} options[m
[32m+[m[32m * @return {Function}[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = function query(options) {[m
[32m+[m[32m  var opts = merge({}, options)[m
[32m+[m[32m  var queryparse = qs.parse;[m
[32m+[m
[32m+[m[32m  if (typeof options === 'function') {[m
[32m+[m[32m    queryparse = options;[m
[32m+[m[32m    opts = undefined;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (opts !== undefined && opts.allowPrototypes === undefined) {[m
[32m+[m[32m    // back-compat for qs module[m
[32m+[m[32m    opts.allowPrototypes = true;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return function query(req, res, next){[m
[32m+[m[32m    if (!req.query) {[m
[32m+[m[32m      var val = parseUrl(req).query;[m
[32m+[m[32m      req.query = queryparse(val, opts);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    next();[m
[32m+[m[32m  };[m
[32m+[m[32m};[m
[1mdiff --git a/node_modules/express/lib/request.js b/node_modules/express/lib/request.js[m
[1mnew file mode 100644[m
[1mindex 0000000..3432e67[m
[1m--- /dev/null[m
[1m+++ b/node_modules/express/lib/request.js[m
[36m@@ -0,0 +1,517 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * express[m
[32m+[m[32m * Copyright(c) 2009-2013 TJ Holowaychuk[m
[32m+[m[32m * Copyright(c) 2013 Roman Shtylman[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar accepts = require('accepts');[m
[32m+[m[32mvar deprecate = require('depd')('express');[m
[32m+[m[32mvar isIP = require('net').isIP;[m
[32m+[m[32mvar typeis = require('type-is');[m
[32m+[m[32mvar http = require('http');[m
[32m+[m[32mvar fresh = require('fresh');[m
[32m+[m[32mvar parseRange = require('range-parser');[m
[32m+[m[32mvar parse = require('parseurl');[m
[32m+[m[32mvar proxyaddr = require('proxy-addr');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Request prototype.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar req = Object.create(http.IncomingMessage.prototype)[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = req[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Return request header.[m
[32m+[m[32m *[m
[32m+[m[32m * The `Referrer` header field is special-cased,[m
[32m+[m[32m * both `Referrer` and `Referer` are interchangeable.[m
[32m+[m[32m *[m
[32m+[m[32m * Examples:[m
[32m+[m[32m *[m
[32m+[m[32m *     req.get('Content-Type');[m
[32m+[m[32m *     // => "text/plain"[m
[32m+[m[32m *[m
[32m+[m[32m *     req.get('content-type');[m
[32m+[m[32m *     // => "text/plain"[m
[32m+[m[32m *[m
[32m+[m[32m *     req.get('Something');[m
[32m+[m[32m *     // => undefined[m
[32m+[m[32m *[m
[32m+[m[32m * Aliased as `req.header()`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} name[m
[32m+[m[32m * @return {String}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mreq.get =[m
[32m+[m[32mreq.header = function header(name) {[m
[32m+[m[32m  if (!name) {[m
[32m+[m[32m    throw new TypeError('name argument is required to req.get');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (typeof name !== 'string') {[m
[32m+[m[32m    throw new TypeError('name must be a string to req.get');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var lc = name.toLowerCase();[m
[32m+[m
[32m+[m[32m  switch (lc) {[m
[32m+[m[32m    case 'referer':[m
[32m+[m[32m    case 'referrer':[m
[32m+[m[32m      return this.headers.referrer[m
[32m+[m[32m        || this.headers.referer;[m
[32m+[m[32m    default:[m
[32m+[m[32m      return this.headers[lc];[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * To do: update docs.[m
[32m+[m[32m *[m
[32m+[m[32m * Check if the given `type(s)` is acceptable, returning[m
[32m+[m[32m * the best match when true, otherwise `undefined`, in which[m
[32m+[m[32m * case you should respond with 406 "Not Acceptable".[m
[32m+[m[32m *[m
[32m+[m[32m * The `type` value may be a single MIME type string[m
[32m+[m[32m * such as "application/json", an extension name[m
[32m+[m[32m * such as "json", a comma-delimited list such as "json, html, text/plain",[m
[32m+[m[32m * an argument list such as `"json", "html", "text/plain"`,[m
[32m+[m[32m * or an array `["json", "html", "text/plain"]`. When a list[m
[32m+[m[32m * or array is given, the _best_ match, if any is returned.[m
[32m+[m[32m *[m
[32m+[m[32m * Examples:[m
[32m+[m[32m *[m
[32m+[m[32m *     // Accept: text/html[m
[32m+[m[32m *     req.accepts('html');[m
[32m+[m[32m *     // => "html"[m
[32m+[m[32m *[m
[32m+[m[32m *     // Accept: text/*, application/json[m
[32m+[m[32m *     req.accepts('html');[m
[32m+[m[32m *     // => "html"[m
[32m+[m[32m *     req.accepts('text/html');[m
[32m+[m[32m *     // => "text/html"[m
[32m+[m[32m *     req.accepts('json, text');[m
[32m+[m[32m *     // => "json"[m
[32m+[m[32m *     req.accepts('application/json');[m
[32m+[m[32m *     // => "application/json"[m
[32m+[m[32m *[m
[32m+[m[32m *     // Accept: text/*, application/json[m
[32m+[m[32m *     req.accepts('image/png');[m
[32m+[m[32m *     req.accepts('png');[m
[32m+[m[32m *     // => undefined[m
[32m+[m[32m *[m
[32m+[m[32m *     // Accept: text/*;q=.5, application/json[m
[32m+[m[32m *     req.accepts(['html', 'json']);[m
[32m+[m[32m *     req.accepts('html', 'json');[m
[32m+[m[32m *     req.accepts('html, json');[m
[32m+[m[32m *     // => "json"[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String|Array} type(s)[m
[32m+[m[32m * @return {String|Array|Boolean}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mreq.accepts = function(){[m
[32m+[m[32m  var accept = accepts(this);[m
[32m+[m[32m  return accept.types.apply(accept, arguments);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Check if the given `encoding`s are accepted.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} ...encoding[m
[32m+[m[32m * @return {String|Array}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mreq.acceptsEncodings = function(){[m
[32m+[m[32m  var accept = accepts(this);[m
[32m+[m[32m  return accept.encodings.apply(accept, arguments);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mreq.acceptsEncoding = deprecate.function(req.acceptsEncodings,[m
[32m+[m[32m  'req.acceptsEncoding: Use acceptsEncodings instead');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Check if the given `charset`s are acceptable,[m
[32m+[m[32m * otherwise you should respond with 406 "Not Acceptable".[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} ...charset[m
[32m+[m[32m * @return {String|Array}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mreq.acceptsCharsets = function(){[m
[32m+[m[32m  var accept = accepts(this);[m
[32m+[m[32m  return accept.charsets.apply(accept, arguments);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mreq.acceptsCharset = deprecate.function(req.acceptsCharsets,[m
[32m+[m[32m  'req.acceptsCharset: Use acceptsCharsets instead');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Check if the given `lang`s are acceptable,[m
[32m+[m[32m * otherwise you should respond with 406 "Not Acceptable".[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} ...lang[m
[32m+[m[32m * @return {String|Array}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mreq.acceptsLanguages = function(){[m
[32m+[m[32m  var accept = accepts(this);[m
[32m+[m[32m  return accept.languages.apply(accept, arguments);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mreq.acceptsLanguage = deprecate.function(req.acceptsLanguages,[m
[32m+[m[32m  'req.acceptsLanguage: Use acceptsLanguages instead');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse Range header field, capping to the given `size`.[m
[32m+[m[32m *[m
[32m+[m[32m * Unspecified ranges such as "0-" require knowledge of your resource length. In[m
[32m+[m[32m * the case of a byte range this is of course the total number of bytes. If the[m
[32m+[m[32m * Range header field is not given `undefined` is returned, `-1` when unsatisfiable,[m
[32m+[m[32m * and `-2` when syntactically invalid.[m
[32m+[m[32m *[m
[32m+[m[32m * When ranges are returned, the array has a "type" property which is the type of[m
[32m+[m[32m * range that is required (most commonly, "bytes"). Each array element is an object[m
[32m+[m[32m * with a "start" and "end" property for the portion of the range.[m
[32m+[m[32m *[m
[32m+[m[32m * The "combine" option can be set to `true` and overlapping & adjacent ranges[m
[32m+[m[32m * will be combined into a single range.[m
[32m+[m[32m *[m
[32m+[m[32m * NOTE: remember that ranges are inclusive, so for example "Range: users=0-3"[m
[32m+[m[32m * should respond with 4 users when available, not 3.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {number} size[m
[32m+[m[32m * @param {object} [options][m
[32m+[m[32m * @param {boolean} [options.combine=false][m
[32m+[m[32m * @return {number|array}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mreq.range = function range(size, options) {[m
[32m+[m[32m  var range = this.get('Range');[m
[32m+[m[32m  if (!range) return;[m
[32m+[m[32m  return parseRange(size, range, options);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Return the value of param `name` when present or `defaultValue`.[m
[32m+[m[32m *[m
[32m+[m[32m *  - Checks route placeholders, ex: _/user/:id_[m
[32m+[m[32m *  - Checks body params, ex: id=12, {"id":12}[m
[32m+[m[32m *  - Checks query string params, ex: ?id=12[m
[32m+[m[32m *[m
[32m+[m[32m * To utilize request bodies, `req.body`[m
[32m+[m[32m * should be an object. This can be done by using[m
[32m+[m[32m * the `bodyParser()` middleware.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} name[m
[32m+[m[32m * @param {Mixed} [defaultValue][m
[32m+[m[32m * @return {String}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mreq.param = function param(name, defaultValue) {[m
[32m+[m[32m  var params = this.params || {};[m
[32m+[m[32m  var body = this.body || {};[m
[32m+[m[32m  var query = this.query || {};[m
[32m+[m
[32m+[m[32m  var args = arguments.length === 1[m
[32m+[m[32m    ? 'name'[m
[32m+[m[32m    : 'name, default';[m
[32m+[m[32m  deprecate('req.param(' + args + '): Use req.params, req.body, or req.query instead');[m
[32m+[m
[32m+[m[32m  if (null != params[name] && params.hasOwnProperty(name)) return params[name];[m
[32m+[m[32m  if (null != body[name]) return body[name];[m
[32m+[m[32m  if (null != query[name]) return query[name];[m
[32m+[m
[32m+[m[32m  return defaultValue;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Check if the incoming request contains the "Content-Type"[m
[32m+[m[32m * header field, and it contains the give mime `type`.[m
[32m+[m[32m *[m
[32m+[m[32m * Examples:[m
[32m+[m[32m *[m
[32m+[m[32m *      // With Content-Type: text/html; charset=utf-8[m
[32m+[m[32m *      req.is('html');[m
[32m+[m[32m *      req.is('text/html');[m
[32m+[m[32m *      req.is('text/*');[m
[32m+[m[32m *      // => true[m
[32m+[m[32m *[m
[32m+[m[32m *      // When Content-Type is application/json[m
[32m+[m[32m *      req.is('json');[m
[32m+[m[32m *      req.is('application/json');[m
[32m+[m[32m *      req.is('application/*');[m
[32m+[m[32m *      // => true[m
[32m+[m[32m *[m
[32m+[m[32m *      req.is('html');[m
[32m+[m[32m *      // => false[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String|Array} types...[m
[32m+[m[32m * @return {String|false|null}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mreq.is = function is(types) {[m
[32m+[m[32m  var arr = types;[m
[32m+[m
[32m+[m[32m  // support flattened arguments[m
[32m+[m[32m  if (!Array.isArray(types)) {[m
[32m+[m[32m    arr = new Array(arguments.length);[m
[32m+[m[32m    for (var i = 0; i < arr.length; i++) {[m
[32m+[m[32m      arr[i] = arguments[i];[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return typeis(this, arr);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Return the protocol string "http" or "https"[m
[32m+[m[32m * when requested with TLS. When the "trust proxy"[m
[32m+[m[32m * setting trusts the socket address, the[m
[32m+[m[32m * "X-Forwarded-Proto" header field will be trusted[m
[32m+[m[32m * and used if present.[m
[32m+[m[32m *[m
[32m+[m[32m * If you're running behind a reverse proxy that[m
[32m+[m[32m * supplies https for you this may be enabled.[m
[32m+[m[32m *[m
[32m+[m[32m * @return {String}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mdefineGetter(req, 'protocol', function protocol(){[m
[32m+[m[32m  var proto = this.connection.encrypted[m
[32m+[m[32m    ? 'https'[m
[32m+[m[32m    : 'http';[m
[32m+[m[32m  var trust = this.app.get('trust proxy fn');[m
[32m+[m
[32m+[m[32m  if (!trust(this.connection.remoteAddress, 0)) {[m
[32m+[m[32m    return proto;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Note: X-Forwarded-Proto is normally only ever a[m
[32m+[m[32m  //       single value, but this is to be safe.[m
[32m+[m[32m  proto = this.get('X-Forwarded-Proto') || proto;[m
[32m+[m[32m  return proto.split(/\s*,\s*/)[0];[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Short-hand for:[m
[32m+[m[32m *[m
[32m+[m[32m *    req.protocol === 'https'[m
[32m+[m[32m *[m
[32m+[m[32m * @return {Boolean}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mdefineGetter(req, 'secure', function secure(){[m
[32m+[m[32m  return this.protocol === 'https';[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Return the remote address from the trusted proxy.[m
[32m+[m[32m *[m
[32m+[m[32m * The is the remote address on the socket unless[m
[32m+[m[32m * "trust proxy" is set.[m
[32m+[m[32m *[m
[32m+[m[32m * @return {String}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mdefineGetter(req, 'ip', function ip(){[m
[32m+[m[32m  var trust = this.app.get('trust proxy fn');[m
[32m+[m[32m  return proxyaddr(this, trust);[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * When "trust proxy" is set, trusted proxy addresses + client.[m
[32m+[m[32m *[m
[32m+[m[32m * For example if the value were "client, proxy1, proxy2"[m
[32m+[m[32m * you would receive the array `["client", "proxy1", "proxy2"]`[m
[32m+[m[32m * where "proxy2" is the furthest down-stream and "proxy1" and[m
[32m+[m[32m * "proxy2" were trusted.[m
[32m+[m[32m *[m
[32m+[m[32m * @return {Array}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mdefineGetter(req, 'ips', function ips() {[m
[32m+[m[32m  var trust = this.app.get('trust proxy fn');[m
[32m+[m[32m  var addrs = proxyaddr.all(this, trust);[m
[32m+[m
[32m+[m[32m  // reverse the order (to farthest -> closest)[m
[32m+[m[32m  // and remove socket address[m
[32m+[m[32m  addrs.reverse().pop()[m
[32m+[m
[32m+[m[32m  return addrs[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Return subdomains as an array.[m
[32m+[m[32m *[m
[32m+[m[32m * Subdomains are the dot-separated parts of the host before the main domain of[m
[32m+[m[32m * the app. By default, the domain of the app is assumed to be the last two[m
[32m+[m[32m * parts of the host. This can be changed by setting "subdomain offset".[m
[32m+[m[32m *[m
[32m+[m[32m * For example, if the domain is "tobi.ferrets.example.com":[m
[32m+[m[32m * If "subdomain offset" is not set, req.subdomains is `["ferrets", "tobi"]`.[m
[32m+[m[32m * If "subdomain offset" is 3, req.subdomains is `["tobi"]`.[m
[32m+[m[32m *[m
[32m+[m[32m * @return {Array}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mdefineGetter(req, 'subdomains', function subdomains() {[m
[32m+[m[32m  var hostname = this.hostname;[m
[32m+[m
[32m+[m[32m  if (!hostname) return [];[m
[32m+[m
[32m+[m[32m  var offset = this.app.get('subdomain offset');[m
[32m+[m[32m  var subdomains = !isIP(hostname)[m
[32m+[m[32m    ? hostname.split('.').reverse()[m
[32m+[m[32m    : [hostname];[m
[32m+[m
[32m+[m[32m  return subdomains.slice(offset);[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Short-hand for `url.parse(req.url).pathname`.[m
[32m+[m[32m *[m
[32m+[m[32m * @return {String}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mdefineGetter(req, 'path', function path() {[m
[32m+[m[32m  return parse(this).pathname;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse the "Host" header field to a hostname.[m
[32m+[m[32m *[m
[32m+[m[32m * When the "trust proxy" setting trusts the socket[m
[32m+[m[32m * address, the "X-Forwarded-Host" header field will[m
[32m+[m[32m * be trusted.[m
[32m+[m[32m *[m
[32m+[m[32m * @return {String}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mdefineGetter(req, 'hostname', function hostname(){[m
[32m+[m[32m  var trust = this.app.get('trust proxy fn');[m
[32m+[m[32m  var host = this.get('X-Forwarded-Host');[m
[32m+[m
[32m+[m[32m  if (!host || !trust(this.connection.remoteAddress, 0)) {[m
[32m+[m[32m    host = this.get('Host');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!host) return;[m
[32m+[m
[32m+[m[32m  // IPv6 literal support[m
[32m+[m[32m  var offset = host[0] === '['[m
[32m+[m[32m    ? host.indexOf(']') + 1[m
[32m+[m[32m    : 0;[m
[32m+[m[32m  var index = host.indexOf(':', offset);[m
[32m+[m
[32m+[m[32m  return index !== -1[m
[32m+[m[32m    ? host.substring(0, index)[m
[32m+[m[32m    : host;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// TODO: change req.host to return host in next major[m
[32m+[m
[32m+[m[32mdefineGetter(req, 'host', deprecate.function(function host(){[m
[32m+[m[32m  return this.hostname;[m
[32m+[m[32m}, 'req.host: Use req.hostname instead'));[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Check if the request is fresh, aka[m
[32m+[m[32m * Last-Modified and/or the ETag[m
[32m+[m[32m * still match.[m
[32m+[m[32m *[m
[32m+[m[32m * @return {Boolean}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mdefineGetter(req, 'fresh', function(){[m
[32m+[m[32m  var method = this.method;[m
[32m+[m[32m  var res = this.res[m
[32m+[m[32m  var status = res.statusCode[m
[32m+[m
[32m+[m[32m  // GET or HEAD for weak freshness validation only[m
[32m+[m[32m  if ('GET' !== method && 'HEAD' !== method) return false;[m
[32m+[m
[32m+[m[32m  // 2xx or 304 as per rfc2616 14.26[m
[32m+[m[32m  if ((status >= 200 && status < 300) || 304 === status) {[m
[32m+[m[32m    return fresh(this.headers, {[m
[32m+[m[32m      'etag': res.get('ETag'),[m
[32m+[m[32m      'last-modified': res.get('Last-Modified')[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return false;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Check if the request is stale, aka[m
[32m+[m[32m * "Last-Modified" and / or the "ETag" for the[m
[32m+[m[32m * resource has changed.[m
[32m+[m[32m *[m
[32m+[m[32m * @return {Boolean}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mdefineGetter(req, 'stale', function stale(){[m
[32m+[m[32m  return !this.fresh;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Check if the request was an _XMLHttpRequest_.[m
[32m+[m[32m *[m
[32m+[m[32m * @return {Boolean}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mdefineGetter(req, 'xhr', function xhr(){[m
[32m+[m[32m  var val = this.get('X-Requested-With') || '';[m
[32m+[m[32m  return val.toLowerCase() === 'xmlhttprequest';[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Helper function for creating a getter on an object.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} obj[m
[32m+[m[32m * @param {String} name[m
[32m+[m[32m * @param {Function} getter[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m[32mfunction defineGetter(obj, name, getter) {[m
[32m+[m[32m  Object.defineProperty(obj, name, {[m
[32m+[m[32m    configurable: true,[m
[32m+[m[32m    enumerable: true,[m
[32m+[m[32m    get: getter[m
[32m+[m[32m  });[m
[32m+[m[32m}[m

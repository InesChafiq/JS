[1mdiff --git a/node_modules/etag/index.js b/node_modules/etag/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..2a585c9[m
[1m--- /dev/null[m
[1m+++ b/node_modules/etag/index.js[m
[36m@@ -0,0 +1,131 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * etag[m
[32m+[m[32m * Copyright(c) 2014-2016 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = etag[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar crypto = require('crypto')[m
[32m+[m[32mvar Stats = require('fs').Stats[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module variables.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar toString = Object.prototype.toString[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Generate an entity tag.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Buffer|string} entity[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction entitytag (entity) {[m
[32m+[m[32m  if (entity.length === 0) {[m
[32m+[m[32m    // fast-path empty[m
[32m+[m[32m    return '"0-2jmj7l5rSw0yVb/vlWAYkK/YBwk"'[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // compute hash of entity[m
[32m+[m[32m  var hash = crypto[m
[32m+[m[32m    .createHash('sha1')[m
[32m+[m[32m    .update(entity, 'utf8')[m
[32m+[m[32m    .digest('base64')[m
[32m+[m[32m    .substring(0, 27)[m
[32m+[m
[32m+[m[32m  // compute length of entity[m
[32m+[m[32m  var len = typeof entity === 'string'[m
[32m+[m[32m    ? Buffer.byteLength(entity, 'utf8')[m
[32m+[m[32m    : entity.length[m
[32m+[m
[32m+[m[32m  return '"' + len.toString(16) + '-' + hash + '"'[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a simple ETag.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string|Buffer|Stats} entity[m
[32m+[m[32m * @param {object} [options][m
[32m+[m[32m * @param {boolean} [options.weak][m
[32m+[m[32m * @return {String}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction etag (entity, options) {[m
[32m+[m[32m  if (entity == null) {[m
[32m+[m[32m    throw new TypeError('argument entity is required')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // support fs.Stats object[m
[32m+[m[32m  var isStats = isstats(entity)[m
[32m+[m[32m  var weak = options && typeof options.weak === 'boolean'[m
[32m+[m[32m    ? options.weak[m
[32m+[m[32m    : isStats[m
[32m+[m
[32m+[m[32m  // validate argument[m
[32m+[m[32m  if (!isStats && typeof entity !== 'string' && !Buffer.isBuffer(entity)) {[m
[32m+[m[32m    throw new TypeError('argument entity must be string, Buffer, or fs.Stats')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // generate entity tag[m
[32m+[m[32m  var tag = isStats[m
[32m+[m[32m    ? stattag(entity)[m
[32m+[m[32m    : entitytag(entity)[m
[32m+[m
[32m+[m[32m  return weak[m
[32m+[m[32m    ? 'W/' + tag[m
[32m+[m[32m    : tag[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if object is a Stats object.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} obj[m
[32m+[m[32m * @return {boolean}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction isstats (obj) {[m
[32m+[m[32m  // genuine fs.Stats[m
[32m+[m[32m  if (typeof Stats === 'function' && obj instanceof Stats) {[m
[32m+[m[32m    return true[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // quack quack[m
[32m+[m[32m  return obj && typeof obj === 'object' &&[m
[32m+[m[32m    'ctime' in obj && toString.call(obj.ctime) === '[object Date]' &&[m
[32m+[m[32m    'mtime' in obj && toString.call(obj.mtime) === '[object Date]' &&[m
[32m+[m[32m    'ino' in obj && typeof obj.ino === 'number' &&[m
[32m+[m[32m    'size' in obj && typeof obj.size === 'number'[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Generate a tag for a stat.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} stat[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction stattag (stat) {[m
[32m+[m[32m  var mtime = stat.mtime.getTime().toString(16)[m
[32m+[m[32m  var size = stat.size.toString(16)[m
[32m+[m
[32m+[m[32m  return '"' + size + '-' + mtime + '"'[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/etag/package.json b/node_modules/etag/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..261441f[m
[1m--- /dev/null[m
[1m+++ b/node_modules/etag/package.json[m
[36m@@ -0,0 +1,86 @@[m
[32m+[m[32m{[m
[32m+[m[32m  "_from": "etag@~1.8.0",[m
[32m+[m[32m  "_id": "etag@1.8.1",[m
[32m+[m[32m  "_inBundle": false,[m
[32m+[m[32m  "_integrity": "sha1-Qa4u62XvpiJorr/qg6x9eSmbCIc=",[m
[32m+[m[32m  "_location": "/etag",[m
[32m+[m[32m  "_phantomChildren": {},[m
[32m+[m[32m  "_requested": {[m
[32m+[m[32m    "type": "range",[m
[32m+[m[32m    "registry": true,[m
[32m+[m[32m    "raw": "etag@~1.8.0",[m
[32m+[m[32m    "name": "etag",[m
[32m+[m[32m    "escapedName": "etag",[m
[32m+[m[32m    "rawSpec": "~1.8.0",[m
[32m+[m[32m    "saveSpec": null,[m
[32m+[m[32m    "fetchSpec": "~1.8.0"[m
[32m+[m[32m  },[m
[32m+[m[32m  "_requiredBy": [[m
[32m+[m[32m    "/express",[m
[32m+[m[32m    "/send"[m
[32m+[m[32m  ],[m
[32m+[m[32m  "_resolved": "https://registry.npmjs.org/etag/-/etag-1.8.1.tgz",[m

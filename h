[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar EventEmitter = require('events').EventEmitter[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mlazyProperty(module.exports, 'callSiteToString', function callSiteToString () {[m
[32m+[m[32m  var limit = Error.stackTraceLimit[m
[32m+[m[32m  var obj = {}[m
[32m+[m[32m  var prep = Error.prepareStackTrace[m
[32m+[m
[32m+[m[32m  function prepareObjectStackTrace (obj, stack) {[m
[32m+[m[32m    return stack[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Error.prepareStackTrace = prepareObjectStackTrace[m
[32m+[m[32m  Error.stackTraceLimit = 2[m
[32m+[m
[32m+[m[32m  // capture the stack[m
[32m+[m[32m  Error.captureStackTrace(obj)[m
[32m+[m
[32m+[m[32m  // slice the stack[m
[32m+[m[32m  var stack = obj.stack.slice()[m
[32m+[m
[32m+[m[32m  Error.prepareStackTrace = prep[m
[32m+[m[32m  Error.stackTraceLimit = limit[m
[32m+[m
[32m+[m[32m  return stack[0].toString ? toString : require('./callsite-tostring')[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mlazyProperty(module.exports, 'eventListenerCount', function eventListenerCount () {[m
[32m+[m[32m  return EventEmitter.listenerCount || require('./event-listener-count')[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Define a lazy property.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction lazyProperty (obj, prop, getter) {[m
[32m+[m[32m  function get () {[m
[32m+[m[32m    var val = getter()[m
[32m+[m
[32m+[m[32m    Object.defineProperty(obj, prop, {[m
[32m+[m[32m      configurable: true,[m
[32m+[m[32m      enumerable: true,[m
[32m+[m[32m      value: val[m
[32m+[m[32m    })[m
[32m+[m
[32m+[m[32m    return val[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Object.defineProperty(obj, prop, {[m
[32m+[m[32m    configurable: true,[m
[32m+[m[32m    enumerable: true,[m
[32m+[m[32m    get: get[m
[32m+[m[32m  })[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Call toString() on the obj[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction toString (obj) {[m
[32m+[m[32m  return obj.toString()[m
[32m+[m[32m}[m

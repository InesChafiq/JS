[36m@@ -0,0 +1,42 @@[m
[32m+[m
[32m+[m[32m# cookie-signature[m
[32m+[m
[32m+[m[32m  Sign and unsign cookies.[m
[32m+[m
[32m+[m[32m## Example[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar cookie = require('cookie-signature');[m
[32m+[m
[32m+[m[32mvar val = cookie.sign('hello', 'tobiiscool');[m
[32m+[m[32mval.should.equal('hello.DGDUkGlIkCzPz+C0B064FNgHdEjox7ch8tOBGslZ5QI');[m
[32m+[m
[32m+[m[32mvar val = cookie.sign('hello', 'tobiiscool');[m
[32m+[m[32mcookie.unsign(val, 'tobiiscool').should.equal('hello');[m
[32m+[m[32mcookie.unsign(val, 'luna').should.be.false;[m
[32m+[m[32m```[m
[32m+[m

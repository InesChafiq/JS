[32m+[m[32m[downloads-url]: https://npmjs.org/package/forwarded[m
[1mdiff --git a/node_modules/forwarded/index.js b/node_modules/forwarded/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..7833b3d[m
[1m--- /dev/null[m
[1m+++ b/node_modules/forwarded/index.js[m
[36m@@ -0,0 +1,76 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * forwarded[m
[32m+[m[32m * Copyright(c) 2014-2017 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = forwarded[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get all addresses in the request, using the `X-Forwarded-For` header.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} req[m
[32m+[m[32m * @return {array}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction forwarded (req) {[m
[32m+[m[32m  if (!req) {[m
[32m+[m[32m    throw new TypeError('argument req is required')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // simple header parsing[m
[32m+[m[32m  var proxyAddrs = parse(req.headers['x-forwarded-for'] || '')[m
[32m+[m[32m  var socketAddr = req.connection.remoteAddress[m
[32m+[m[32m  var addrs = [socketAddr].concat(proxyAddrs)[m
[32m+[m
[32m+[m[32m  // return all addresses[m
[32m+[m[32m  return addrs[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse the X-Forwarded-For header.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} header[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parse (header) {[m
[32m+[m[32m  var end = header.length[m
[32m+[m[32m  var list = [][m
[32m+[m[32m  var start = header.length[m
[32m+[m
[32m+[m[32m  // gather addresses, backwards[m
[32m+[m[32m  for (var i = header.length - 1; i >= 0; i--) {[m
[32m+[m[32m    switch (header.charCodeAt(i)) {[m
[32m+[m[32m      case 0x20: /*   */[m
[32m+[m[32m        if (start === end) {[m
[32m+[m[32m          start = end = i[m
[32m+[m[32m        }[m
[32m+[m[32m        break[m
[32m+[m[32m      case 0x2c: /* , */[m
[32m+[m[32m        if (start !== end) {[m
[32m+[m[32m          list.push(header.substring(start, end))[m
[32m+[m[32m        }[m
[32m+[m[32m        start = end = i[m
[32m+[m[32m        break[m
[32m+[m[32m      default:[m
[32m+[m[32m        start = i[m
[32m+[m[32m        break[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // final address[m
[32m+[m[32m  if (start !== end) {[m
[32m+[m[32m    list.push(header.substring(start, end))[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return list[m
[32m+[m[32m}[m

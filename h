[1mnew file mode 100644[m
[1mindex 0000000..036f18b[m
[1m--- /dev/null[m
[1m+++ b/node_modules/qs/lib/stringify.js[m
[36m@@ -0,0 +1,210 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar utils = require('./utils');[m
[32m+[m[32mvar formats = require('./formats');[m
[32m+[m
[32m+[m[32mvar arrayPrefixGenerators = {[m
[32m+[m[32m    brackets: function brackets(prefix) { // eslint-disable-line func-name-matching[m
[32m+[m[32m        return prefix + '[]';[m
[32m+[m[32m    },[m
[32m+[m[32m    indices: function indices(prefix, key) { // eslint-disable-line func-name-matching[m
[32m+[m[32m        return prefix + '[' + key + ']';[m
[32m+[m[32m    },[m
[32m+[m[32m    repeat: function repeat(prefix) { // eslint-disable-line func-name-matching[m
[32m+[m[32m        return prefix;[m
[32m+[m[32m    }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar toISO = Date.prototype.toISOString;[m
[32m+[m
[32m+[m[32mvar defaults = {[m
[32m+[m[32m    delimiter: '&',[m
[32m+[m[32m    encode: true,[m
[32m+[m[32m    encoder: utils.encode,[m
[32m+[m[32m    encodeValuesOnly: false,[m
[32m+[m[32m    serializeDate: function serializeDate(date) { // eslint-disable-line func-name-matching[m
[32m+[m[32m        return toISO.call(date);[m
[32m+[m[32m    },[m
[32m+[m[32m    skipNulls: false,[m
[32m+[m[32m    strictNullHandling: false[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar stringify = function stringify( // eslint-disable-line func-name-matching[m
[32m+[m[32m    object,[m
[32m+[m[32m    prefix,[m
[32m+[m[32m    generateArrayPrefix,[m
[32m+[m[32m    strictNullHandling,[m
[32m+[m[32m    skipNulls,[m
[32m+[m[32m    encoder,[m
[32m+[m[32m    filter,[m
[32m+[m[32m    sort,[m
[32m+[m[32m    allowDots,[m
[32m+[m[32m    serializeDate,[m
[32m+[m[32m    formatter,[m
[32m+[m[32m    encodeValuesOnly[m
[32m+[m[32m) {[m
[32m+[m[32m    var obj = object;[m
[32m+[m[32m    if (typeof filter === 'function') {[m
[32m+[m[32m        obj = filter(prefix, obj);[m
[32m+[m[32m    } else if (obj instanceof Date) {[m
[32m+[m[32m        obj = serializeDate(obj);[m
[32m+[m[32m    } else if (obj === null) {[m
[32m+[m[32m        if (strictNullHandling) {[m
[32m+[m[32m            return encoder && !encodeValuesOnly ? encoder(prefix, defaults.encoder) : prefix;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        obj = '';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (typeof obj === 'string' || typeof obj === 'number' || typeof obj === 'boolean' || utils.isBuffer(obj)) {[m
[32m+[m[32m        if (encoder) {[m
[32m+[m[32m            var keyValue = encodeValuesOnly ? prefix : encoder(prefix, defaults.encoder);[m
[32m+[m[32m            return [formatter(keyValue) + '=' + formatter(encoder(obj, defaults.encoder))];[m
[32m+[m[32m        }[m
[32m+[m[32m        return [formatter(prefix) + '=' + formatter(String(obj))];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var values = [];[m
[32m+[m
[32m+[m[32m    if (typeof obj === 'undefined') {[m
[32m+[m[32m        return values;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var objKeys;[m
[32m+[m[32m    if (Array.isArray(filter)) {[m
[32m+[m[32m        objKeys = filter;[m
[32m+[m[32m    } else {[m
[32m+[m[32m        var keys = Object.keys(obj);[m
[32m+[m[32m        objKeys = sort ? keys.sort(sort) : keys;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < objKeys.length; ++i) {[m
[32m+[m[32m        var key = objKeys[i];[m
[32m+[m
[32m+[m[32m        if (skipNulls && obj[key] === null) {[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (Array.isArray(obj)) {[m
[32m+[m[32m            values = values.concat(stringify([m
[32m+[m[32m                obj[key],[m
[32m+[m[32m                generateArrayPrefix(prefix, key),[m
[32m+[m[32m                generateArrayPrefix,[m
[32m+[m[32m                strictNullHandling,[m
[32m+[m[32m                skipNulls,[m
[32m+[m[32m                encoder,[m
[32m+[m[32m                filter,[m
[32m+[m[32m                sort,[m
[32m+[m[32m                allowDots,[m
[32m+[m[32m                serializeDate,[m
[32m+[m[32m                formatter,[m
[32m+[m[32m                encodeValuesOnly[m
[32m+[m[32m            ));[m
[32m+[m[32m        } else {[m
[32m+[m[32m            values = values.concat(stringify([m
[32m+[m[32m                obj[key],[m
[32m+[m[32m                prefix + (allowDots ? '.' + key : '[' + key + ']'),[m
[32m+[m[32m                generateArrayPrefix,[m
[32m+[m[32m                strictNullHandling,[m
[32m+[m[32m                skipNulls,[m
[32m+[m[32m                encoder,[m
[32m+[m[32m                filter,[m
[32m+[m[32m                sort,[m
[32m+[m[32m                allowDots,[m
[32m+[m[32m                serializeDate,[m
[32m+[m[32m                formatter,[m
[32m+[m[32m                encodeValuesOnly[m
[32m+[m[32m            ));[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return values;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = function (object, opts) {[m
[32m+[m[32m    var obj = object;[m
[32m+[m[32m    var options = opts ? utils.assign({}, opts) : {};[m
[32m+[m
[32m+[m[32m    if (options.encoder !== null && options.encoder !== undefined && typeof options.encoder !== 'function') {[m
[32m+[m[32m        throw new TypeError('Encoder has to be a function.');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var delimiter = typeof options.delimiter === 'undefined' ? defaults.delimiter : options.delimiter;[m
[32m+[m[32m    var strictNullHandling = typeof options.strictNullHandling === 'boolean' ? options.strictNullHandling : defaults.strictNullHandling;[m
[32m+[m[32m    var skipNulls = typeof options.skipNulls === 'boolean' ? options.skipNulls : defaults.skipNulls;[m
[32m+[m[32m    var encode = typeof options.encode === 'boolean' ? options.encode : defaults.encode;[m
[32m+[m[32m    var encoder = typeof options.encoder === 'function' ? options.encoder : defaults.encoder;[m
[32m+[m[32m    var sort = typeof options.sort === 'function' ? options.sort : null;[m
[32m+[m[32m    var allowDots = typeof options.allowDots === 'undefined' ? false : options.allowDots;[m
[32m+[m[32m    var serializeDate = typeof options.serializeDate === 'function' ? options.serializeDate : defaults.serializeDate;[m
[32m+[m[32m    var encodeValuesOnly = typeof options.encodeValuesOnly === 'boolean' ? options.encodeValuesOnly : defaults.encodeValuesOnly;[m
[32m+[m[32m    if (typeof options.format === 'undefined') {[m
[32m+[m[32m        options.format = formats.default;[m
[32m+[m[32m    } else if (!Object.prototype.hasOwnProperty.call(formats.formatters, options.format)) {[m
[32m+[m[32m        throw new TypeError('Unknown format option provided.');[m
[32m+[m[32m    }[m
[32m+[m[32m    var formatter = formats.formatters[options.format];[m
[32m+[m[32m    var objKeys;[m
[32m+[m[32m    var filter;[m
[32m+[m
[32m+[m[32m    if (typeof options.filter === 'function') {[m
[32m+[m[32m        filter = options.filter;[m
[32m+[m[32m        obj = filter('', obj);[m
[32m+[m[32m    } else if (Array.isArray(options.filter)) {[m
[32m+[m[32m        filter = options.filter;[m
[32m+[m[32m        objKeys = filter;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var keys = [];[m
[32m+[m
[32m+[m[32m    if (typeof obj !== 'object' || obj === null) {[m
[32m+[m[32m        return '';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var arrayFormat;[m
[32m+[m[32m    if (options.arrayFormat in arrayPrefixGenerators) {[m
[32m+[m[32m        arrayFormat = options.arrayFormat;[m
[32m+[m[32m    } else if ('indices' in options) {[m
[32m+[m[32m        arrayFormat = options.indices ? 'indices' : 'repeat';[m
[32m+[m[32m    } else {[m
[32m+[m[32m        arrayFormat = 'indices';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var generateArrayPrefix = arrayPrefixGenerators[arrayFormat];[m
[32m+[m
[32m+[m[32m    if (!objKeys) {[m
[32m+[m[32m        objKeys = Object.keys(obj);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (sort) {[m
[32m+[m[32m        objKeys.sort(sort);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < objKeys.length; ++i) {[m
[32m+[m[32m        var key = objKeys[i];[m
[32m+[m
[32m+[m[32m        if (skipNulls && obj[key] === null) {[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        keys = keys.concat(stringify([m
[32m+[m[32m            obj[key],[m
[32m+[m[32m            key,[m
[32m+[m[32m            generateArrayPrefix,[m
[32m+[m[32m            strictNullHandling,[m
[32m+[m[32m            skipNulls,[m
[32m+[m[32m            encode ? encoder : null,[m
[32m+[m[32m            filter,[m
[32m+[m[32m            sort,[m
[32m+[m[32m            allowDots,[m
[32m+[m[32m            serializeDate,[m
[32m+[m[32m            formatter,[m
[32m+[m[32m            encodeValuesOnly[m
[32m+[m[32m        ));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var joined = keys.join(delimiter);[m
[32m+[m[32m    var prefix = options.addQueryPrefix === true ? '?' : '';[m
[32m+[m
[32m+[m[32m    return joined.length > 0 ? prefix + joined : '';[m
[32m+[m[32m};[m
[1mdiff --git a/node_modules/qs/lib/utils.js b/node_modules/qs/lib/utils.js[m
[1mnew file mode 100644[m
[1mindex 0000000..9be00de[m
[1m--- /dev/null[m
[1m+++ b/node_modules/qs/lib/utils.js[m
[36m@@ -0,0 +1,192 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar has = Object.prototype.hasOwnProperty;[m
[32m+[m
[32m+[m[32mvar hexTable = (function () {[m
[32m+[m[32m    var array = [];[m
[32m+[m[32m    for (var i = 0; i < 256; ++i) {[m
[32m+[m[32m        array.push('%' + ((i < 16 ? '0' : '') + i.toString(16)).toUpperCase());[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return array;[m
[32m+[m[32m}());[m
[32m+[m
[32m+[m[32mexports.arrayToObject = function (source, options) {[m
[32m+[m[32m    var obj = options && options.plainObjects ? Object.create(null) : {};[m
[32m+[m[32m    for (var i = 0; i < source.length; ++i) {[m
[32m+[m[32m        if (typeof source[i] !== 'undefined') {[m
[32m+[m[32m            obj[i] = source[i];[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return obj;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexports.merge = function (target, source, options) {[m
[32m+[m[32m    if (!source) {[m
[32m+[m[32m        return target;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (typeof source !== 'object') {[m
[32m+[m[32m        if (Array.isArray(target)) {[m
[32m+[m[32m            target.push(source);[m
[32m+[m[32m        } else if (typeof target === 'object') {[m
[32m+[m[32m            if (options.plainObjects || options.allowPrototypes || !has.call(Object.prototype, source)) {[m
[32m+[m[32m                target[source] = true;[m
[32m+[m[32m            }[m
[32m+[m[32m        } else {[m
[32m+[m[32m            return [target, source];[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return target;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (typeof target !== 'object') {[m
[32m+[m[32m        return [target].concat(source);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var mergeTarget = target;[m
[32m+[m[32m    if (Array.isArray(target) && !Array.isArray(source)) {[m
[32m+[m[32m        mergeTarget = exports.arrayToObject(target, options);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (Array.isArray(target) && Array.isArray(source)) {[m
[32m+[m[32m        source.forEach(function (item, i) {[m
[32m+[m[32m            if (has.call(target, i)) {[m
[32m+[m[32m                if (target[i] && typeof target[i] === 'object') {[m
[32m+[m[32m                    target[i] = exports.merge(target[i], item, options);[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    target.push(item);[m
[32m+[m[32m                }[m
[32m+[m[32m            } else {[m
[32m+[m[32m                target[i] = item;[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m        return target;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return Object.keys(source).reduce(function (acc, key) {[m
[32m+[m[32m        var value = source[key];[m
[32m+[m
[32m+[m[32m        if (has.call(acc, key)) {[m
[32m+[m[32m            acc[key] = exports.merge(acc[key], value, options);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            acc[key] = value;[m
[32m+[m[32m        }[m
[32m+[m[32m        return acc;[m
[32m+[m[32m    }, mergeTarget);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexports.assign = function assignSingleSource(target, source) {[m
[32m+[m[32m    return Object.keys(source).reduce(function (acc, key) {[m
[32m+[m[32m        acc[key] = source[key];[m
[32m+[m[32m        return acc;[m
[32m+[m[32m    }, target);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexports.decode = function (str) {[m
[32m+[m[32m    try {[m
[32m+[m[32m        return decodeURIComponent(str.replace(/\+/g, ' '));[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m        return str;[m
[32m+[m[32m    }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexports.encode = function (str) {[m
[32m+[m[32m    // This code was originally written by Brian White (mscdex) for the io.js core querystring library.[m
[32m+[m[32m    // It has been adapted here for stricter adherence to RFC 3986[m
[32m+[m[32m    if (str.length === 0) {[m
[32m+[m[32m        return str;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var string = typeof str === 'string' ? str : String(str);[m
[32m+[m
[32m+[m[32m    var out = '';[m
[32m+[m[32m    for (var i = 0; i < string.length; ++i) {[m
[32m+[m[32m        var c = string.charCodeAt(i);[m
[32m+[m
[32m+[m[32m        if ([m
[32m+[m[32m            c === 0x2D    // -[m
[32m+[m[32m            || c === 0x2E // .[m
[32m+[m[32m            || c === 0x5F // _[m
[32m+[m[32m            || c === 0x7E // ~[m
[32m+[m[32m            || (c >= 0x30 && c <= 0x39) // 0-9[m
[32m+[m[32m            || (c >= 0x41 && c <= 0x5A) // a-z[m
[32m+[m[32m            || (c >= 0x61 && c <= 0x7A) // A-Z[m
[32m+[m[32m        ) {[m
[32m+[m[32m            out += string.charAt(i);[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (c < 0x80) {[m
[32m+[m[32m            out = out + hexTable[c];[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (c < 0x800) {[m
[32m+[m[32m            out = out + (hexTable[0xC0 | (c >> 6)] + hexTable[0x80 | (c & 0x3F)]);[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (c < 0xD800 || c >= 0xE000) {[m
[32m+[m[32m            out = out + (hexTable[0xE0 | (c >> 12)] + hexTable[0x80 | ((c >> 6) & 0x3F)] + hexTable[0x80 | (c & 0x3F)]);[m
[32m+[m[32m            continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        i += 1;[m
[32m+[m[32m        c = 0x10000 + (((c & 0x3FF) << 10) | (string.charCodeAt(i) & 0x3FF));[m
[32m+[m[32m        out += hexTable[0xF0 | (c >> 18)][m
[32m+[m[32m            + hexTable[0x80 | ((c >> 12) & 0x3F)][m
[32m+[m[32m            + hexTable[0x80 | ((c >> 6) & 0x3F)][m
[32m+[m[32m            + hexTable[0x80 | (c & 0x3F)];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return out;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexports.compact = function (obj, references) {[m
[32m+[m[32m    if (typeof obj !== 'object' || obj === null) {[m
[32m+[m[32m        return obj;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var refs = references || [];[m
[32m+[m[32m    var lookup = refs.indexOf(obj);[m
[32m+[m[32m    if (lookup !== -1) {[m
[32m+[m[32m        return refs[lookup];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    refs.push(obj);[m
[32m+[m
[32m+[m[32m    if (Array.isArray(obj)) {[m
[32m+[m[32m        var compacted = [];[m
[32m+[m
[32m+[m[32m        for (var i = 0; i < obj.length; ++i) {[m
[32m+[m[32m            if (obj[i] && typeof obj[i] === 'object') {[m
[32m+[m[32m                compacted.push(exports.compact(obj[i], refs));[m
[32m+[m[32m            } else if (typeof obj[i] !== 'undefined') {[m
[32m+[m[32m                compacted.push(obj[i]);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return compacted;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var keys = Object.keys(obj);[m
[32m+[m[32m    keys.forEach(function (key) {[m
[32m+[m[32m        obj[key] = exports.compact(obj[key], refs);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    return obj;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexports.isRegExp = function (obj) {[m
[32m+[m[32m    return Object.prototype.toString.call(obj) === '[object RegExp]';[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexports.isBuffer = function (obj) {[m
[32m+[m[32m    if (obj === null || typeof obj === 'undefined') {[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return !!(obj.constructor && obj.constructor.isBuffer && obj.constructor.isBuffer(obj));[m
[32m+[m[32m};[m

[32m+[m[32m[downloads-url]: https://npmjs.org/package/encodeurl[m
[1mdiff --git a/node_modules/encodeurl/index.js b/node_modules/encodeurl/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..fc4906c[m
[1m--- /dev/null[m
[1m+++ b/node_modules/encodeurl/index.js[m
[36m@@ -0,0 +1,60 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * encodeurl[m
[32m+[m[32m * Copyright(c) 2016 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = encodeUrl[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp to match non-URL code points, *after* encoding (i.e. not including "%")[m
[32m+[m[32m * and including invalid escape sequences.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar ENCODE_CHARS_REGEXP = /(?:[^\x21\x25\x26-\x3B\x3D\x3F-\x5B\x5D\x5F\x61-\x7A\x7E]|%(?:[^0-9A-Fa-f]|[0-9A-Fa-f][^0-9A-Fa-f]|$))+/g[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp to match unmatched surrogate pair.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar UNMATCHED_SURROGATE_PAIR_REGEXP = /(^|[^\uD800-\uDBFF])[\uDC00-\uDFFF]|[\uD800-\uDBFF]([^\uDC00-\uDFFF]|$)/g[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * String to replace unmatched surrogate pair with.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar UNMATCHED_SURROGATE_PAIR_REPLACE = '$1\uFFFD$2'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Encode a URL to a percent-encoded form, excluding already-encoded sequences.[m
[32m+[m[32m *[m
[32m+[m[32m * This function will take an already-encoded URL and encode all the non-URL[m
[32m+[m[32m * code points. This function will not encode the "%" character unless it is[m
[32m+[m[32m * not part of a valid sequence (`%20` will be left as-is, but `%foo` will[m
[32m+[m[32m * be encoded as `%25foo`).[m
[32m+[m[32m *[m
[32m+[m[32m * This encode is meant to be "safe" and does not throw errors. It will try as[m
[32m+[m[32m * hard as it can to properly encode the given URL, including replacing any raw,[m
[32m+[m[32m * unpaired surrogate pairs with the Unicode replacement character prior to[m
[32m+[m[32m * encoding.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} url[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction encodeUrl (url) {[m
[32m+[m[32m  return String(url)[m
[32m+[m[32m    .replace(UNMATCHED_SURROGATE_PAIR_REGEXP, UNMATCHED_SURROGATE_PAIR_REPLACE)[m
[32m+[m[32m    .replace(ENCODE_CHARS_REGEXP, encodeURI)[m
[32m+[m[32m}[m

[32m+[m[32m  fs.writeFileSync 'lib/ipaddr.js', CoffeeScript.compile source.toString()[m
[32m+[m
[32m+[m[32m  invoke 'test'[m
[32m+[m[32m  invoke 'compress'[m
[32m+[m
[32m+[m[32mtask 'test', 'run the bundled tests', (cb) ->[m
[32m+[m[32m  nodeunit.reporters.default.run ['test'][m
[32m+[m
[32m+[m[32mtask 'compress', 'uglify the resulting javascript', (cb) ->[m
[32m+[m[32m  source = fs.readFileSync 'lib/ipaddr.js', 'utf-8'[m
[32m+[m[32m  fs.writeFileSync('ipaddr.min.js', UglifyJS.minify(source).code)[m

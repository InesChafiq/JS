[32m+[m[32m# EE First[m
[32m+[m
[32m+[m[32m[![NPM version][npm-image]][npm-url][m
[32m+[m[32m[![Build status][travis-image]][travis-url][m
[32m+[m[32m[![Test coverage][coveralls-image]][coveralls-url][m
[32m+[m[32m[![License][license-image]][license-url][m
[32m+[m[32m[![Downloads][downloads-image]][downloads-url][m
[32m+[m[32m[![Gittip][gittip-image]][gittip-url][m
[32m+[m
[32m+[m[32mGet the first event in a set of event emitters and event pairs,[m
[32m+[m[32mthen clean up after itself.[m
[32m+[m
[32m+[m[32m## Install[m
[32m+[m
[32m+[m[32m```sh[m
[32m+[m[32m$ npm install ee-first[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## API[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar first = require('ee-first')[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### first(arr, listener)[m
[32m+[m
[32m+[m[32mInvoke `listener` on the first event from the list specified in `arr`. `arr` is[m
[32m+[m[32man array of arrays, with each array in the format `[ee, ...event]`. `listener`[m
[32m+[m[32mwill be called only once, the first time any of the given events are emitted. If[m
[32m+[m[32m`error` is one of the listened events, then if that fires first, the `listener`[m
[32m+[m[32mwill be given the `err` argument.[m
[32m+[m
[32m+[m[32mThe `listener` is invoked as `listener(err, ee, event, args)`, where `err` is the[m
[32m+[m[32mfirst argument emitted from an `error` event, if applicable; `ee` is the event[m
[32m+[m[32memitter that fired; `event` is the string event name that fired; and `args` is an[m
[32m+[m[32marray of the arguments that were emitted on the event.[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar ee1 = new EventEmitter()[m
[32m+[m[32mvar ee2 = new EventEmitter()[m
[32m+[m
[32m+[m[32mfirst([[m
[32m+[m[32m  [ee1, 'close', 'end', 'error'],[m
[32m+[m[32m  [ee2, 'error'][m
[32m+[m[32m], function (err, ee, event, args) {[m
[32m+[m[32m  // listener invoked[m
[32m+[m[32m})[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m#### .cancel()[m
[32m+[m
[32m+[m[32mThe group of listeners can be cancelled before being invoked and have all the event[m
[32m+[m[32mlisteners removed from the underlying event emitters.[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar thunk = first([[m
[32m+[m[32m  [ee1, 'close', 'end', 'error'],[m
[32m+[m[32m  [ee2, 'error'][m
[32m+[m[32m], function (err, ee, event, args) {[m
[32m+[m[32m  // listener invoked[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32m// cancel and clean up[m
[32m+[m[32mthunk.cancel()[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m[npm-image]: https://img.shields.io/npm/v/ee-first.svg?style=flat-square[m
[32m+[m[32m[npm-url]: https://npmjs.org/package/ee-first[m
[32m+[m[32m[github-tag]: http://img.shields.io/github/tag/jonathanong/ee-first.svg?style=flat-square[m
[32m+[m[32m[github-url]: https://github.com/jonathanong/ee-first/tags[m
[32m+[m[32m[travis-image]: https://img.shields.io/travis/jonathanong/ee-first.svg?style=flat-square[m
[32m+[m[32m[travis-url]: https://travis-ci.org/jonathanong/ee-first[m
[32m+[m[32m[coveralls-image]: https://img.shields.io/coveralls/jonathanong/ee-first.svg?style=flat-square[m
[32m+[m[32m[coveralls-url]: https://coveralls.io/r/jonathanong/ee-first?branch=master[m
[32m+[m[32m[license-image]: http://img.shields.io/npm/l/ee-first.svg?style=flat-square[m
[32m+[m[32m[license-url]: LICENSE.md[m
[32m+[m[32m[downloads-image]: http://img.shields.io/npm/dm/ee-first.svg?style=flat-square[m
[32m+[m[32m[downloads-url]: https://npmjs.org/package/ee-first[m
[32m+[m[32m[gittip-image]: https://img.shields.io/gittip/jonathanong.svg?style=flat-square[m
[32m+[m[32m[gittip-url]: https://www.gittip.com/jonathanong/[m
[1mdiff --git a/node_modules/ee-first/index.js b/node_modules/ee-first/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..501287c[m
[1m--- /dev/null[m
[1m+++ b/node_modules/ee-first/index.js[m
[36m@@ -0,0 +1,95 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * ee-first[m
[32m+[m[32m * Copyright(c) 2014 Jonathan Ong[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = first[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the first event in a set of event emitters and event pairs.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {array} stuff[m
[32m+[m[32m * @param {function} done[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction first(stuff, done) {[m
[32m+[m[32m  if (!Array.isArray(stuff))[m
[32m+[m[32m    throw new TypeError('arg must be an array of [ee, events...] arrays')[m
[32m+[m
[32m+[m[32m  var cleanups = [][m
[32m+[m
[32m+[m[32m  for (var i = 0; i < stuff.length; i++) {[m
[32m+[m[32m    var arr = stuff[i][m
[32m+[m
[32m+[m[32m    if (!Array.isArray(arr) || arr.length < 2)[m
[32m+[m[32m      throw new TypeError('each array member must be [ee, events...]')[m
[32m+[m
[32m+[m[32m    var ee = arr[0][m
[32m+[m
[32m+[m[32m    for (var j = 1; j < arr.length; j++) {[m
[32m+[m[32m      var event = arr[j][m
[32m+[m[32m      var fn = listener(event, callback)[m
[32m+[m
[32m+[m[32m      // listen to the event[m
[32m+[m[32m      ee.on(event, fn)[m
[32m+[m[32m      // push this listener to the list of cleanups[m
[32m+[m[32m      cleanups.push({[m
[32m+[m[32m        ee: ee,[m
[32m+[m[32m        event: event,[m
[32m+[m[32m        fn: fn,[m
[32m+[m[32m      })[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function callback() {[m
[32m+[m[32m    cleanup()[m
[32m+[m[32m    done.apply(null, arguments)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function cleanup() {[m
[32m+[m[32m    var x[m
[32m+[m[32m    for (var i = 0; i < cleanups.length; i++) {[m
[32m+[m[32m      x = cleanups[i][m
[32m+[m[32m      x.ee.removeListener(x.event, x.fn)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function thunk(fn) {[m
[32m+[m[32m    done = fn[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  thunk.cancel = cleanup[m
[32m+[m
[32m+[m[32m  return thunk[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create the event listener.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction listener(event, done) {[m
[32m+[m[32m  return function onevent(arg1) {[m
[32m+[m[32m    var args = new Array(arguments.length)[m
[32m+[m[32m    var ee = this[m
[32m+[m[32m    var err = event === 'error'[m
[32m+[m[32m      ? arg1[m
[32m+[m[32m      : null[m
[32m+[m
[32m+[m[32m    // copy args to prevent arguments escaping scope[m
[32m+[m[32m    for (var i = 0; i < args.length; i++) {[m
[32m+[m[32m      args[i] = arguments[i][m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    done(err, ee, event, args)[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/ee-first/package.json b/node_modules/ee-first/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..e3cc63c[m

[36m@@ -0,0 +1,34 @@[m
[32m+[m[32m# utils-merge[m
[32m+[m
[32m+[m[32mMerges the properties from a source object into a destination object.[m
[32m+[m
[32m+[m[32m## Install[m
[32m+[m
[32m+[m[32m    $ npm install utils-merge[m
[32m+[m
[32m+[m[32m## Usage[m
[32m+[m
[32m+[m[32m```javascript[m
[32m+[m[32mvar a = { foo: 'bar' }[m
[32m+[m[32m  , b = { bar: 'baz' };[m
[32m+[m
[32m+[m[32mmerge(a, b);[m
[32m+[m[32m// => { foo: 'bar', bar: 'baz' }[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## Tests[m
[32m+[m
[32m+[m[32m    $ npm install[m
[32m+[m[32m    $ npm test[m
[32m+[m
[32m+[m[32m[![Build Status](https://secure.travis-ci.org/jaredhanson/utils-merge.png)](http://travis-ci.org/jaredhanson/utils-merge)[m
[32m+[m
[32m+[m[32m## Credits[m
[32m+[m
[32m+[m[32m  - [Jared Hanson](http://github.com/jaredhanson)[m
[32m+[m
[32m+[m[32m## License[m
[32m+[m
[32m+[m[32m[The MIT License](http://opensource.org/licenses/MIT)[m
[32m+[m
[32m+[m[32mCopyright (c) 2013 Jared Hanson <[http://jaredhanson.net/](http://jaredhanson.net/)>[m
[1mdiff --git a/node_modules/utils-merge/index.js b/node_modules/utils-merge/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..4265c69[m
[1m--- /dev/null[m
[1m+++ b/node_modules/utils-merge/index.js[m
[36m@@ -0,0 +1,23 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * Merge object b with object a.[m
[32m+[m[32m *[m
[32m+[m[32m *     var a = { foo: 'bar' }[m
[32m+[m[32m *       , b = { bar: 'baz' };[m
[32m+[m[32m *[m
[32m+[m[32m *     merge(a, b);[m
[32m+[m[32m *     // => { foo: 'bar', bar: 'baz' }[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} a[m
[32m+[m[32m * @param {Object} b[m
[32m+[m[32m * @return {Object}[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports = module.exports = function(a, b){[m
[32m+[m[32m  if (a && b) {[m
[32m+[m[32m    for (var key in b) {[m
[32m+[m[32m      a[key] = b[key];[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  return a;[m
[32m+[m[32m};[m

[1mnew file mode 100644[m
[1mindex 0000000..ea82ed2[m
[1m--- /dev/null[m
[1m+++ b/node_modules/express/lib/router/route.js[m
[36m@@ -0,0 +1,216 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * express[m
[32m+[m[32m * Copyright(c) 2009-2013 TJ Holowaychuk[m
[32m+[m[32m * Copyright(c) 2013 Roman Shtylman[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar debug = require('debug')('express:router:route');[m
[32m+[m[32mvar flatten = require('array-flatten');[m
[32m+[m[32mvar Layer = require('./layer');[m
[32m+[m[32mvar methods = require('methods');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module variables.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar slice = Array.prototype.slice;[m
[32m+[m[32mvar toString = Object.prototype.toString;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = Route;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Initialize `Route` with the given `path`,[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} path[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction Route(path) {[m
[32m+[m[32m  this.path = path;[m
[32m+[m[32m  this.stack = [];[m
[32m+[m
[32m+[m[32m  debug('new %o', path)[m
[32m+[m
[32m+[m[32m  // route handlers for various http methods[m
[32m+[m[32m  this.methods = {};[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if the route handles a given method.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mRoute.prototype._handles_method = function _handles_method(method) {[m
[32m+[m[32m  if (this.methods._all) {[m
[32m+[m[32m    return true;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var name = method.toLowerCase();[m
[32m+[m
[32m+[m[32m  if (name === 'head' && !this.methods['head']) {[m
[32m+[m[32m    name = 'get';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return Boolean(this.methods[name]);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * @return {Array} supported HTTP methods[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mRoute.prototype._options = function _options() {[m
[32m+[m[32m  var methods = Object.keys(this.methods);[m
[32m+[m
[32m+[m[32m  // append automatic head[m
[32m+[m[32m  if (this.methods.get && !this.methods.head) {[m
[32m+[m[32m    methods.push('head');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < methods.length; i++) {[m
[32m+[m[32m    // make upper case[m
[32m+[m[32m    methods[i] = methods[i].toUpperCase();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return methods;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * dispatch req, res into this route[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mRoute.prototype.dispatch = function dispatch(req, res, done) {[m
[32m+[m[32m  var idx = 0;[m
[32m+[m[32m  var stack = this.stack;[m
[32m+[m[32m  if (stack.length === 0) {[m
[32m+[m[32m    return done();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var method = req.method.toLowerCase();[m
[32m+[m[32m  if (method === 'head' && !this.methods['head']) {[m
[32m+[m[32m    method = 'get';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  req.route = this;[m
[32m+[m
[32m+[m[32m  next();[m
[32m+[m
[32m+[m[32m  function next(err) {[m
[32m+[m[32m    // signal to exit route[m
[32m+[m[32m    if (err && err === 'route') {[m
[32m+[m[32m      return done();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // signal to exit router[m
[32m+[m[32m    if (err && err === 'router') {[m
[32m+[m[32m      return done(err)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var layer = stack[idx++];[m
[32m+[m[32m    if (!layer) {[m
[32m+[m[32m      return done(err);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (layer.method && layer.method !== method) {[m
[32m+[m[32m      return next(err);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (err) {[m
[32m+[m[32m      layer.handle_error(err, req, res, next);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      layer.handle_request(req, res, next);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Add a handler for all HTTP verbs to this route.[m
[32m+[m[32m *[m
[32m+[m[32m * Behaves just like middleware and can respond or call `next`[m
[32m+[m[32m * to continue processing.[m
[32m+[m[32m *[m
[32m+[m[32m * You can use multiple `.all` call to add multiple handlers.[m
[32m+[m[32m *[m
[32m+[m[32m *   function check_something(req, res, next){[m
[32m+[m[32m *     next();[m
[32m+[m[32m *   };[m
[32m+[m[32m *[m
[32m+[m[32m *   function validate_user(req, res, next){[m
[32m+[m[32m *     next();[m
[32m+[m[32m *   };[m
[32m+[m[32m *[m
[32m+[m[32m *   route[m
[32m+[m[32m *   .all(validate_user)[m
[32m+[m[32m *   .all(check_something)[m
[32m+[m[32m *   .get(function(req, res, next){[m
[32m+[m[32m *     res.send('hello world');[m
[32m+[m[32m *   });[m
[32m+[m[32m *[m
[32m+[m[32m * @param {function} handler[m
[32m+[m[32m * @return {Route} for chaining[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mRoute.prototype.all = function all() {[m
[32m+[m[32m  var handles = flatten(slice.call(arguments));[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < handles.length; i++) {[m
[32m+[m[32m    var handle = handles[i];[m
[32m+[m
[32m+[m[32m    if (typeof handle !== 'function') {[m
[32m+[m[32m      var type = toString.call(handle);[m
[32m+[m[32m      var msg = 'Route.all() requires callback functions but got a ' + type;[m
[32m+[m[32m      throw new TypeError(msg);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var layer = Layer('/', {}, handle);[m
[32m+[m[32m    layer.method = undefined;[m
[32m+[m
[32m+[m[32m    this.methods._all = true;[m
[32m+[m[32m    this.stack.push(layer);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmethods.forEach(function(method){[m
[32m+[m[32m  Route.prototype[method] = function(){[m
[32m+[m[32m    var handles = flatten(slice.call(arguments));[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < handles.length; i++) {[m
[32m+[m[32m      var handle = handles[i];[m
[32m+[m
[32m+[m[32m      if (typeof handle !== 'function') {[m
[32m+[m[32m        var type = toString.call(handle);[m
[32m+[m[32m        var msg = 'Route.' + method + '() requires callback functions but got a ' + type;[m
[32m+[m[32m        throw new Error(msg);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      debug('%s %o', method, this.path)[m
[32m+[m
[32m+[m[32m      var layer = Layer('/', {}, handle);[m
[32m+[m[32m      layer.method = method;[m
[32m+[m
[32m+[m[32m      this.methods[method] = true;[m
[32m+[m[32m      this.stack.push(layer);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return this;[m
[32m+[m[32m  };[m
[32m+[m[32m});[m

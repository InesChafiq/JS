[32m+[m[32m  'vhost',[m
[32m+[m[32m  'csrf',[m
[32m+[m[32m  'directory',[m
[32m+[m[32m  'limit',[m
[32m+[m[32m  'multipart',[m
[32m+[m[32m  'staticCache',[m
[32m+[m[32m].forEach(function (name) {[m
[32m+[m[32m  Object.defineProperty(exports, name, {[m
[32m+[m[32m    get: function () {[m
[32m+[m[32m      throw new Error('Most middleware (like ' + name + ') is no longer bundled with Express and must be installed separately. Please see https://github.com/senchalabs/connect#middleware.');[m
[32m+[m[32m    },[m
[32m+[m[32m    configurable: true[m
[32m+[m[32m  });[m
[32m+[m[32m});[m

[32m+[m[32m[npm-downloads-image]: https://badgen.net/npm/dm/parseurl[m
[32m+[m[32m[npm-url]: https://npmjs.org/package/parseurl[m
[32m+[m[32m[npm-version-image]: https://badgen.net/npm/v/parseurl[m
[32m+[m[32m[travis-image]: https://badgen.net/travis/pillarjs/parseurl/master[m
[32m+[m[32m[travis-url]: https://travis-ci.org/pillarjs/parseurl[m
[1mdiff --git a/node_modules/parseurl/index.js b/node_modules/parseurl/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..ece7223[m
[1m--- /dev/null[m
[1m+++ b/node_modules/parseurl/index.js[m
[36m@@ -0,0 +1,158 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * parseurl[m
[32m+[m[32m * Copyright(c) 2014 Jonathan Ong[m
[32m+[m[32m * Copyright(c) 2014-2017 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar url = require('url')[m
[32m+[m[32mvar parse = url.parse[m
[32m+[m[32mvar Url = url.Url[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = parseurl[m
[32m+[m[32mmodule.exports.original = originalurl[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse the `req` url with memoization.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {ServerRequest} req[m
[32m+[m[32m * @return {Object}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parseurl (req) {[m
[32m+[m[32m  var url = req.url[m
[32m+[m
[32m+[m[32m  if (url === undefined) {[m
[32m+[m[32m    // URL is undefined[m
[32m+[m[32m    return undefined[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var parsed = req._parsedUrl[m
[32m+[m
[32m+[m[32m  if (fresh(url, parsed)) {[m
[32m+[m[32m    // Return cached URL parse[m
[32m+[m[32m    return parsed[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Parse the URL[m
[32m+[m[32m  parsed = fastparse(url)[m
[32m+[m[32m  parsed._raw = url[m
[32m+[m
[32m+[m[32m  return (req._parsedUrl = parsed)[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse the `req` original url with fallback and memoization.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {ServerRequest} req[m
[32m+[m[32m * @return {Object}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction originalurl (req) {[m
[32m+[m[32m  var url = req.originalUrl[m
[32m+[m
[32m+[m[32m  if (typeof url !== 'string') {[m
[32m+[m[32m    // Fallback[m
[32m+[m[32m    return parseurl(req)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var parsed = req._parsedOriginalUrl[m
[32m+[m
[32m+[m[32m  if (fresh(url, parsed)) {[m
[32m+[m[32m    // Return cached URL parse[m
[32m+[m[32m    return parsed[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Parse the URL[m
[32m+[m[32m  parsed = fastparse(url)[m
[32m+[m[32m  parsed._raw = url[m
[32m+[m
[32m+[m[32m  return (req._parsedOriginalUrl = parsed)[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse the `str` url with fast-path short-cut.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} str[m
[32m+[m[32m * @return {Object}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction fastparse (str) {[m
[32m+[m[32m  if (typeof str !== 'string' || str.charCodeAt(0) !== 0x2f /* / */) {[m
[32m+[m[32m    return parse(str)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var pathname = str[m
[32m+[m[32m  var query = null[m
[32m+[m[32m  var search = null[m
[32m+[m
[32m+[m[32m  // This takes the regexp from https://github.com/joyent/node/pull/7878[m
[32m+[m[32m  // Which is /^(\/[^?#\s]*)(\?[^#\s]*)?$/[m
[32m+[m[32m  // And unrolls it into a for loop[m
[32m+[m[32m  for (var i = 1; i < str.length; i++) {[m
[32m+[m[32m    switch (str.charCodeAt(i)) {[m
[32m+[m[32m      case 0x3f: /* ?  */[m
[32m+[m[32m        if (search === null) {[m
[32m+[m[32m          pathname = str.substring(0, i)[m
[32m+[m[32m          query = str.substring(i + 1)[m
[32m+[m[32m          search = str.substring(i)[m
[32m+[m[32m        }[m
[32m+[m[32m        break[m
[32m+[m[32m      case 0x09: /* \t */[m
[32m+[m[32m      case 0x0a: /* \n */[m
[32m+[m[32m      case 0x0c: /* \f */[m
[32m+[m[32m      case 0x0d: /* \r */[m
[32m+[m[32m      case 0x20: /*    */[m
[32m+[m[32m      case 0x23: /* #  */[m
[32m+[m[32m      case 0xa0:[m
[32m+[m[32m      case 0xfeff:[m
[32m+[m[32m        return parse(str)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var url = Url !== undefined[m
[32m+[m[32m    ? new Url()[m
[32m+[m[32m    : {}[m
[32m+[m
[32m+[m[32m  url.path = str[m
[32m+[m[32m  url.href = str[m
[32m+[m[32m  url.pathname = pathname[m
[32m+[m
[32m+[m[32m  if (search !== null) {[m
[32m+[m[32m    url.query = query[m
[32m+[m[32m    url.search = search[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return url[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Determine if parsed is still fresh for url.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} url[m
[32m+[m[32m * @param {object} parsedUrl[m
[32m+[m[32m * @return {boolean}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction fresh (url, parsedUrl) {[m
[32m+[m[32m  return typeof parsedUrl === 'object' &&[m
[32m+[m[32m    parsedUrl !== null &&[m
[32m+[m[32m    (Url === undefined || parsedUrl instanceof Url) &&[m
[32m+[m[32m    parsedUrl._raw === url[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/parseurl/package.json b/node_modules/parseurl/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..d3dbcd5[m
[1m--- /dev/null[m
[1m+++ b/node_modules/parseurl/package.json[m
[36m@@ -0,0 +1,81 @@[m

[32m+[m[32m/*![m
[32m+[m[32m * express[m
[32m+[m[32m * Copyright(c) 2009-2013 TJ Holowaychuk[m
[32m+[m[32m * Copyright(c) 2013 Roman Shtylman[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar debug = require('debug')('express:view');[m
[32m+[m[32mvar path = require('path');[m
[32m+[m[32mvar fs = require('fs');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module variables.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar dirname = path.dirname;[m
[32m+[m[32mvar basename = path.basename;[m
[32m+[m[32mvar extname = path.extname;[m
[32m+[m[32mvar join = path.join;[m
[32m+[m[32mvar resolve = path.resolve;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = View;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Initialize a new `View` with the given `name`.[m
[32m+[m[32m *[m
[32m+[m[32m * Options:[m
[32m+[m[32m *[m
[32m+[m[32m *   - `defaultEngine` the default template engine name[m
[32m+[m[32m *   - `engines` template engine require() cache[m
[32m+[m[32m *   - `root` root path for view lookup[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} name[m
[32m+[m[32m * @param {object} options[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction View(name, options) {[m
[32m+[m[32m  var opts = options || {};[m
[32m+[m
[32m+[m[32m  this.defaultEngine = opts.defaultEngine;[m
[32m+[m[32m  this.ext = extname(name);[m
[32m+[m[32m  this.name = name;[m
[32m+[m[32m  this.root = opts.root;[m
[32m+[m
[32m+[m[32m  if (!this.ext && !this.defaultEngine) {[m
[32m+[m[32m    throw new Error('No default engine was specified and no extension was provided.');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var fileName = name;[m
[32m+[m
[32m+[m[32m  if (!this.ext) {[m
[32m+[m[32m    // get extension from default engine name[m
[32m+[m[32m    this.ext = this.defaultEngine[0] !== '.'[m
[32m+[m[32m      ? '.' + this.defaultEngine[m
[32m+[m[32m      : this.defaultEngine;[m
[32m+[m
[32m+[m[32m    fileName += this.ext;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!opts.engines[this.ext]) {[m
[32m+[m[32m    // load engine[m
[32m+[m[32m    var mod = this.ext.substr(1)[m
[32m+[m[32m    debug('require "%s"', mod)[m
[32m+[m[32m    opts.engines[this.ext] = require(mod).__express[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // store loaded engine[m
[32m+[m[32m  this.engine = opts.engines[this.ext];[m
[32m+[m
[32m+[m[32m  // lookup path[m
[32m+[m[32m  this.path = this.lookup(fileName);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Lookup view by the given `name`[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} name[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mView.prototype.lookup = function lookup(name) {[m
[32m+[m[32m  var path;[m
[32m+[m[32m  var roots = [].concat(this.root);[m
[32m+[m
[32m+[m[32m  debug('lookup "%s"', name);[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < roots.length && !path; i++) {[m
[32m+[m[32m    var root = roots[i];[m
[32m+[m
[32m+[m[32m    // resolve the path[m
[32m+[m[32m    var loc = resolve(root, name);[m
[32m+[m[32m    var dir = dirname(loc);[m
[32m+[m[32m    var file = basename(loc);[m
[32m+[m
[32m+[m[32m    // resolve the file[m
[32m+[m[32m    path = this.resolve(dir, file);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return path;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Render with the given options.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} options[m
[32m+[m[32m * @param {function} callback[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mView.prototype.render = function render(options, callback) {[m
[32m+[m[32m  debug('render "%s"', this.path);[m
[32m+[m[32m  this.engine(this.path, options, callback);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Resolve the file within the given directory.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} dir[m
[32m+[m[32m * @param {string} file[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mView.prototype.resolve = function resolve(dir, file) {[m
[32m+[m[32m  var ext = this.ext;[m
[32m+[m
[32m+[m[32m  // <path>.<ext>[m
[32m+[m[32m  var path = join(dir, file);[m
[32m+[m[32m  var stat = tryStat(path);[m
[32m+[m
[32m+[m[32m  if (stat && stat.isFile()) {[m
[32m+[m[32m    return path;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // <path>/index.<ext>[m
[32m+[m[32m  path = join(dir, basename(file, ext), 'index' + ext);[m
[32m+[m[32m  stat = tryStat(path);[m
[32m+[m
[32m+[m[32m  if (stat && stat.isFile()) {[m
[32m+[m[32m    return path;[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Return a stat, maybe.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} path[m
[32m+[m[32m * @return {fs.Stats}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction tryStat(path) {[m
[32m+[m[32m  debug('stat "%s"', path);[m
[32m+[m
[32m+[m[32m  try {[m
[32m+[m[32m    return fs.statSync(path);[m
[32m+[m[32m  } catch (e) {[m
[32m+[m[32m    return undefined;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m

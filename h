[1m+++ b/node_modules/methods/index.js[m
[36m@@ -0,0 +1,69 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * methods[m
[32m+[m[32m * Copyright(c) 2013-2014 TJ Holowaychuk[m
[32m+[m[32m * Copyright(c) 2015-2016 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar http = require('http');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = getCurrentNodeMethods() || getBasicNodeMethods();[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the current Node.js methods.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getCurrentNodeMethods() {[m
[32m+[m[32m  return http.METHODS && http.METHODS.map(function lowerCaseMethod(method) {[m
[32m+[m[32m    return method.toLowerCase();[m
[32m+[m[32m  });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the "basic" Node.js methods, a snapshot from Node.js 0.10.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getBasicNodeMethods() {[m
[32m+[m[32m  return [[m
[32m+[m[32m    'get',[m
[32m+[m[32m    'post',[m
[32m+[m[32m    'put',[m
[32m+[m[32m    'head',[m
[32m+[m[32m    'delete',[m
[32m+[m[32m    'options',[m
[32m+[m[32m    'trace',[m
[32m+[m[32m    'copy',[m
[32m+[m[32m    'lock',[m
[32m+[m[32m    'mkcol',[m
[32m+[m[32m    'move',[m
[32m+[m[32m    'purge',[m
[32m+[m[32m    'propfind',[m
[32m+[m[32m    'proppatch',[m
[32m+[m[32m    'unlock',[m
[32m+[m[32m    'report',[m
[32m+[m[32m    'mkactivity',[m
[32m+[m[32m    'checkout',[m
[32m+[m[32m    'merge',[m
[32m+[m[32m    'm-search',[m
[32m+[m[32m    'notify',[m
[32m+[m[32m    'subscribe',[m
[32m+[m[32m    'unsubscribe',[m
[32m+[m[32m    'patch',[m
[32m+[m[32m    'search',[m
[32m+[m[32m    'connect'[m
[32m+[m[32m  ];[m
[32m+[m[32m}[m

[36m@@ -0,0 +1,445 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * content-disposition[m
[32m+[m[32m * Copyright(c) 2014 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = contentDisposition[m
[32m+[m[32mmodule.exports.parse = parse[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar basename = require('path').basename[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp to match non attr-char, *after* encodeURIComponent (i.e. not including "%")[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar ENCODE_URL_ATTR_CHAR_REGEXP = /[\x00-\x20"'()*,/:;<=>?@[\\\]{}\x7f]/g // eslint-disable-line no-control-regex[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp to match percent encoding escape.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar HEX_ESCAPE_REGEXP = /%[0-9A-Fa-f]{2}/[m
[32m+[m[32mvar HEX_ESCAPE_REPLACE_REGEXP = /%([0-9A-Fa-f]{2})/g[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp to match non-latin1 characters.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar NON_LATIN1_REGEXP = /[^\x20-\x7e\xa0-\xff]/g[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp to match quoted-pair in RFC 2616[m
[32m+[m[32m *[m
[32m+[m[32m * quoted-pair = "\" CHAR[m
[32m+[m[32m * CHAR        = <any US-ASCII character (octets 0 - 127)>[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar QESC_REGEXP = /\\([\u0000-\u007f])/g[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp to match chars that must be quoted-pair in RFC 2616[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar QUOTE_REGEXP = /([\\"])/g[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp for various RFC 2616 grammar[m
[32m+[m[32m *[m
[32m+[m[32m * parameter     = token "=" ( token | quoted-string )[m
[32m+[m[32m * token         = 1*<any CHAR except CTLs or separators>[m
[32m+[m[32m * separators    = "(" | ")" | "<" | ">" | "@"[m
[32m+[m[32m *               | "," | ";" | ":" | "\" | <">[m
[32m+[m[32m *               | "/" | "[" | "]" | "?" | "="[m
[32m+[m[32m *               | "{" | "}" | SP | HT[m
[32m+[m[32m * quoted-string = ( <"> *(qdtext | quoted-pair ) <"> )[m
[32m+[m[32m * qdtext        = <any TEXT except <">>[m
[32m+[m[32m * quoted-pair   = "\" CHAR[m
[32m+[m[32m * CHAR          = <any US-ASCII character (octets 0 - 127)>[m
[32m+[m[32m * TEXT          = <any OCTET except CTLs, but including LWS>[m
[32m+[m[32m * LWS           = [CRLF] 1*( SP | HT )[m
[32m+[m[32m * CRLF          = CR LF[m
[32m+[m[32m * CR            = <US-ASCII CR, carriage return (13)>[m
[32m+[m[32m * LF            = <US-ASCII LF, linefeed (10)>[m
[32m+[m[32m * SP            = <US-ASCII SP, space (32)>[m
[32m+[m[32m * HT            = <US-ASCII HT, horizontal-tab (9)>[m
[32m+[m[32m * CTL           = <any US-ASCII control character (octets 0 - 31) and DEL (127)>[m
[32m+[m[32m * OCTET         = <any 8-bit sequence of data>[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar PARAM_REGEXP = /;[\x09\x20]*([!#$%&'*+.0-9A-Z^_`a-z|~-]+)[\x09\x20]*=[\x09\x20]*("(?:[\x20!\x23-\x5b\x5d-\x7e\x80-\xff]|\\[\x20-\x7e])*"|[!#$%&'*+.0-9A-Z^_`a-z|~-]+)[\x09\x20]*/g // eslint-disable-line no-control-regex[m
[32m+[m[32mvar TEXT_REGEXP = /^[\x20-\x7e\x80-\xff]+$/[m
[32m+[m[32mvar TOKEN_REGEXP = /^[!#$%&'*+.0-9A-Z^_`a-z|~-]+$/[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp for various RFC 5987 grammar[m
[32m+[m[32m *[m
[32m+[m[32m * ext-value     = charset  "'" [ language ] "'" value-chars[m
[32m+[m[32m * charset       = "UTF-8" / "ISO-8859-1" / mime-charset[m
[32m+[m[32m * mime-charset  = 1*mime-charsetc[m
[32m+[m[32m * mime-charsetc = ALPHA / DIGIT[m
[32m+[m[32m *               / "!" / "#" / "$" / "%" / "&"[m
[32m+[m[32m *               / "+" / "-" / "^" / "_" / "`"[m
[32m+[m[32m *               / "{" / "}" / "~"[m
[32m+[m[32m * language      = ( 2*3ALPHA [ extlang ] )[m
[32m+[m[32m *               / 4ALPHA[m
[32m+[m[32m *               / 5*8ALPHA[m
[32m+[m[32m * extlang       = *3( "-" 3ALPHA )[m
[32m+[m[32m * value-chars   = *( pct-encoded / attr-char )[m
[32m+[m[32m * pct-encoded   = "%" HEXDIG HEXDIG[m
[32m+[m[32m * attr-char     = ALPHA / DIGIT[m
[32m+[m[32m *               / "!" / "#" / "$" / "&" / "+" / "-" / "."[m
[32m+[m[32m *               / "^" / "_" / "`" / "|" / "~"[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar EXT_VALUE_REGEXP = /^([A-Za-z0-9!#$%&+\-^_`{}~]+)'(?:[A-Za-z]{2,3}(?:-[A-Za-z]{3}){0,3}|[A-Za-z]{4,8}|)'((?:%[0-9A-Fa-f]{2}|[A-Za-z0-9!#$&+.^_`|~-])+)$/[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp for various RFC 6266 grammar[m
[32m+[m[32m *[m
[32m+[m[32m * disposition-type = "inline" | "attachment" | disp-ext-type[m
[32m+[m[32m * disp-ext-type    = token[m
[32m+[m[32m * disposition-parm = filename-parm | disp-ext-parm[m
[32m+[m[32m * filename-parm    = "filename" "=" value[m
[32m+[m[32m *                  | "filename*" "=" ext-value[m
[32m+[m[32m * disp-ext-parm    = token "=" value[m
[32m+[m[32m *                  | ext-token "=" ext-value[m
[32m+[m[32m * ext-token        = <the characters in token, followed by "*">[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar DISPOSITION_TYPE_REGEXP = /^([!#$%&'*+.0-9A-Z^_`a-z|~-]+)[\x09\x20]*(?:$|;)/ // eslint-disable-line no-control-regex[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create an attachment Content-Disposition header.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} [filename][m
[32m+[m[32m * @param {object} [options][m
[32m+[m[32m * @param {string} [options.type=attachment][m
[32m+[m[32m * @param {string|boolean} [options.fallback=true][m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction contentDisposition (filename, options) {[m
[32m+[m[32m  var opts = options || {}[m
[32m+[m
[32m+[m[32m  // get type[m
[32m+[m[32m  var type = opts.type || 'attachment'[m
[32m+[m
[32m+[m[32m  // get parameters[m
[32m+[m[32m  var params = createparams(filename, opts.fallback)[m
[32m+[m
[32m+[m[32m  // format into string[m
[32m+[m[32m  return format(new ContentDisposition(type, params))[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create parameters object from filename and fallback.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} [filename][m
[32m+[m[32m * @param {string|boolean} [fallback=true][m
[32m+[m[32m * @return {object}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createparams (filename, fallback) {[m
[32m+[m[32m  if (filename === undefined) {[m
[32m+[m[32m    return[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var params = {}[m
[32m+[m
[32m+[m[32m  if (typeof filename !== 'string') {[m
[32m+[m[32m    throw new TypeError('filename must be a string')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // fallback defaults to true[m
[32m+[m[32m  if (fallback === undefined) {[m
[32m+[m[32m    fallback = true[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (typeof fallback !== 'string' && typeof fallback !== 'boolean') {[m
[32m+[m[32m    throw new TypeError('fallback must be a string or boolean')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (typeof fallback === 'string' && NON_LATIN1_REGEXP.test(fallback)) {[m
[32m+[m[32m    throw new TypeError('fallback must be ISO-8859-1 string')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // restrict to file base name[m
[32m+[m[32m  var name = basename(filename)[m
[32m+[m
[32m+[m[32m  // determine if name is suitable for quoted string[m
[32m+[m[32m  var isQuotedString = TEXT_REGEXP.test(name)[m
[32m+[m
[32m+[m[32m  // generate fallback name[m
[32m+[m[32m  var fallbackName = typeof fallback !== 'string'[m
[32m+[m[32m    ? fallback && getlatin1(name)[m
[32m+[m[32m    : basename(fallback)[m
[32m+[m[32m  var hasFallback = typeof fallbackName === 'string' && fallbackName !== name[m
[32m+[m
[32m+[m[32m  // set extended filename parameter[m
[32m+[m[32m  if (hasFallback || !isQuotedString || HEX_ESCAPE_REGEXP.test(name)) {[m
[32m+[m[32m    params['filename*'] = name[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // set filename parameter[m
[32m+[m[32m  if (isQuotedString || hasFallback) {[m
[32m+[m[32m    params.filename = hasFallback[m
[32m+[m[32m      ? fallbackName[m
[32m+[m[32m      : name[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return params[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Format object to Content-Disposition header.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {object} obj[m
[32m+[m[32m * @param {string} obj.type[m
[32m+[m[32m * @param {object} [obj.parameters][m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction format (obj) {[m
[32m+[m[32m  var parameters = obj.parameters[m
[32m+[m[32m  var type = obj.type[m
[32m+[m
[32m+[m[32m  if (!type || typeof type !== 'string' || !TOKEN_REGEXP.test(type)) {[m
[32m+[m[32m    throw new TypeError('invalid type')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // start with normalized type[m
[32m+[m[32m  var string = String(type).toLowerCase()[m
[32m+[m
[32m+[m[32m  // append parameters[m
[32m+[m[32m  if (parameters && typeof parameters === 'object') {[m
[32m+[m[32m    var param[m
[32m+[m[32m    var params = Object.keys(parameters).sort()[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < params.length; i++) {[m
[32m+[m[32m      param = params[i][m
[32m+[m
[32m+[m[32m      var val = param.substr(-1) === '*'[m
[32m+[m[32m        ? ustring(parameters[param])[m
[32m+[m[32m        : qstring(parameters[param])[m
[32m+[m
[32m+[m[32m      string += '; ' + param + '=' + val[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return string[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Decode a RFC 6987 field value (gracefully).[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} str[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction decodefield (str) {[m
[32m+[m[32m  var match = EXT_VALUE_REGEXP.exec(str)[m
[32m+[m
[32m+[m[32m  if (!match) {[m
[32m+[m[32m    throw new TypeError('invalid extended field value')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var charset = match[1].toLowerCase()[m
[32m+[m[32m  var encoded = match[2][m
[32m+[m[32m  var value[m
[32m+[m
[32m+[m[32m  // to binary string[m
[32m+[m[32m  var binary = encoded.replace(HEX_ESCAPE_REPLACE_REGEXP, pdecode)[m
[32m+[m
[32m+[m[32m  switch (charset) {[m
[32m+[m[32m    case 'iso-8859-1':[m
[32m+[m[32m      value = getlatin1(binary)[m
[32m+[m[32m      break[m
[32m+[m[32m    case 'utf-8':[m
[32m+[m[32m      value = new Buffer(binary, 'binary').toString('utf8')[m
[32m+[m[32m      break[m
[32m+[m[32m    default:[m
[32m+[m[32m      throw new TypeError('unsupported charset in extended field')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return value[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get ISO-8859-1 version of string.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} val[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getlatin1 (val) {[m
[32m+[m[32m  // simple Unicode -> ISO-8859-1 transformation[m
[32m+[m[32m  return String(val).replace(NON_LATIN1_REGEXP, '?')[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse Content-Disposition header string.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} string[m
[32m+[m[32m * @return {object}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parse (string) {[m
[32m+[m[32m  if (!string || typeof string !== 'string') {[m
[32m+[m[32m    throw new TypeError('argument string is required')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var match = DISPOSITION_TYPE_REGEXP.exec(string)[m
[32m+[m
[32m+[m[32m  if (!match) {[m
[32m+[m[32m    throw new TypeError('invalid type format')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // normalize type[m
[32m+[m[32m  var index = match[0].length[m
[32m+[m[32m  var type = match[1].toLowerCase()[m
[32m+[m
[32m+[m[32m  var key[m
[32m+[m[32m  var names = [][m
[32m+[m[32m  var params = {}[m
[32m+[m[32m  var value[m
[32m+[m
[32m+[m[32m  // calculate index to start at[m
[32m+[m[32m  index = PARAM_REGEXP.lastIndex = match[0].substr(-1) === ';'[m
[32m+[m[32m    ? index - 1[m
[32m+[m[32m    : index[m
[32m+[m
[32m+[m[32m  // match parameters[m
[32m+[m[32m  while ((match = PARAM_REGEXP.exec(string))) {[m
[32m+[m[32m    if (match.index !== index) {[m
[32m+[m[32m      throw new TypeError('invalid parameter format')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    index += match[0].length[m
[32m+[m[32m    key = match[1].toLowerCase()[m
[32m+[m[32m    value = match[2][m
[32m+[m
[32m+[m[32m    if (names.indexOf(key) !== -1) {[m
[32m+[m[32m      throw new TypeError('invalid duplicate parameter')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    names.push(key)[m
[32m+[m
[32m+[m[32m    if (key.indexOf('*') + 1 === key.length) {[m
[32m+[m[32m      // decode extended value[m
[32m+[m[32m      key = key.slice(0, -1)[m
[32m+[m[32m      value = decodefield(value)[m
[32m+[m
[32m+[m[32m      // overwrite existing value[m
[32m+[m[32m      params[key] = value[m
[32m+[m[32m      continue[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (typeof params[key] === 'string') {[m
[32m+[m[32m      continue[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (value[0] === '"') {[m
[32m+[m[32m      // remove quotes and escapes[m
[32m+[m[32m      value = value[m
[32m+[m[32m        .substr(1, value.length - 2)[m
[32m+[m[32m        .replace(QESC_REGEXP, '$1')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    params[key] = value[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (index !== -1 && index !== string.length) {[m
[32m+[m[32m    throw new TypeError('invalid parameter format')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return new ContentDisposition(type, params)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Percent decode a single character.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} str[m
[32m+[m[32m * @param {string} hex[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction pdecode (str, hex) {[m
[32m+[m[32m  return String.fromCharCode(parseInt(hex, 16))[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Percent encode a single character.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} char[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction pencode (char) {[m
[32m+[m[32m  var hex = String(char)[m
[32m+[m[32m    .charCodeAt(0)[m
[32m+[m[32m    .toString(16)[m
[32m+[m[32m    .toUpperCase()[m
[32m+[m[32m  return hex.length === 1[m
[32m+[m[32m    ? '%0' + hex[m
[32m+[m[32m    : '%' + hex[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Quote a string for HTTP.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} val[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction qstring (val) {[m
[32m+[m[32m  var str = String(val)[m
[32m+[m
[32m+[m[32m  return '"' + str.replace(QUOTE_REGEXP, '\\$1') + '"'[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Encode a Unicode string for HTTP (RFC 5987).[m
[32m+[m[32m *[m
[32m+[m[32m * @param {string} val[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction ustring (val) {[m
[32m+[m[32m  var str = String(val)[m
[32m+[m
[32m+[m[32m  // percent encode as UTF-8[m
[32m+[m[32m  var encoded = encodeURIComponent(str)[m
[32m+[m[32m    .replace(ENCODE_URL_ATTR_CHAR_REGEXP, pencode)[m
[32m+[m
[32m+[m[32m  return 'UTF-8\'\'' + encoded[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Class for parsed Content-Disposition header for v8 optimization[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction ContentDisposition (type, parameters) {[m
[32m+[m[32m  this.type = type[m
[32m+[m[32m  this.parameters = parameters[m
[32m+[m[32m}[m

[32m+[m[32m# range-parser[m
[32m+[m
[32m+[m[32m[![NPM Version][npm-version-image]][npm-url][m
[32m+[m[32m[![NPM Downloads][npm-downloads-image]][npm-url][m
[32m+[m[32m[![Node.js Version][node-image]][node-url][m
[32m+[m[32m[![Build Status][travis-image]][travis-url][m
[32m+[m[32m[![Test Coverage][coveralls-image]][coveralls-url][m
[32m+[m
[32m+[m[32mRange header field parser.[m
[32m+[m
[32m+[m[32m## Installation[m
[32m+[m
[32m+[m[32mThis is a [Node.js](https://nodejs.org/en/) module available through the[m
[32m+[m[32m[npm registry](https://www.npmjs.com/). Installation is done using the[m
[32m+[m[32m[`npm install` command](https://docs.npmjs.com/getting-started/installing-npm-packages-locally):[m
[32m+[m
[32m+[m[32m```sh[m
[32m+[m[32m$ npm install range-parser[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## API[m
[32m+[m
[32m+[m[32m<!-- eslint-disable no-unused-vars -->[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar parseRange = require('range-parser')[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### parseRange(size, header, options)[m
[32m+[m
[32m+[m[32mParse the given `header` string where `size` is the maximum size of the resource.[m
[32m+[m[32mAn array of ranges will be returned or negative numbers indicating an error parsing.[m
[32m+[m
[32m+[m[32m  * `-2` signals a malformed header string[m
[32m+[m[32m  * `-1` signals an unsatisfiable range[m
[32m+[m
[32m+[m[32m<!-- eslint-disable no-undef -->[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32m// parse header from request[m
[32m+[m[32mvar range = parseRange(size, req.headers.range)[m
[32m+[m
[32m+[m[32m// the type of the range[m
[32m+[m[32mif (range.type === 'bytes') {[m
[32m+[m[32m  // the ranges[m
[32m+[m[32m  range.forEach(function (r) {[m
[32m+[m[32m    // do something with r.start and r.end[m
[32m+[m[32m  })[m
[32m+[m[32m}[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m#### Options[m
[32m+[m
[32m+[m[32mThese properties are accepted in the options object.[m
[32m+[m
[32m+[m[32m##### combine[m
[32m+[m
[32m+[m[32mSpecifies if overlapping & adjacent ranges should be combined, defaults to `false`.[m
[32m+[m[32mWhen `true`, ranges will be combined and returned as if they were specified that[m
[32m+[m[32mway in the header.[m
[32m+[m
[32m+[m[32m<!-- eslint-disable no-undef -->[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mparseRange(100, 'bytes=50-55,0-10,5-10,56-60', { combine: true })[m
[32m+[m[32m// => [[m
[32m+[m[32m//      { start: 0,  end: 10 },[m
[32m+[m[32m//      { start: 50, end: 60 }[m
[32m+[m[32m//    ][m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## License[m
[32m+[m
[32m+[m[32m[MIT](LICENSE)[m
[32m+[m
[32m+[m[32m[coveralls-image]: https://badgen.net/coveralls/c/github/jshttp/range-parser/master[m
[32m+[m[32m[coveralls-url]: https://coveralls.io/r/jshttp/range-parser?branch=master[m
[32m+[m[32m[node-image]: https://badgen.net/npm/node/range-parser[m
[32m+[m[32m[node-url]: https://nodejs.org/en/download[m
[32m+[m[32m[npm-downloads-image]: https://badgen.net/npm/dm/range-parser[m
[32m+[m[32m[npm-url]: https://npmjs.org/package/range-parser[m
[32m+[m[32m[npm-version-image]: https://badgen.net/npm/v/range-parser[m
[32m+[m[32m[travis-image]: https://badgen.net/travis/jshttp/range-parser/master[m
[32m+[m[32m[travis-url]: https://travis-ci.org/jshttp/range-parser[m
[1mdiff --git a/node_modules/range-parser/index.js b/node_modules/range-parser/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..b7dc5c0[m
[1m--- /dev/null[m
[1m+++ b/node_modules/range-parser/index.js[m
[36m@@ -0,0 +1,162 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * range-parser[m
[32m+[m[32m * Copyright(c) 2012-2014 TJ Holowaychuk[m
[32m+[m[32m * Copyright(c) 2015-2016 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = rangeParser[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse "Range" header `str` relative to the given file `size`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Number} size[m
[32m+[m[32m * @param {String} str[m
[32m+[m[32m * @param {Object} [options][m
[32m+[m[32m * @return {Array}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction rangeParser (size, str, options) {[m
[32m+[m[32m  if (typeof str !== 'string') {[m
[32m+[m[32m    throw new TypeError('argument str must be a string')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var index = str.indexOf('=')[m
[32m+[m
[32m+[m[32m  if (index === -1) {[m
[32m+[m[32m    return -2[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // split the range string[m
[32m+[m[32m  var arr = str.slice(index + 1).split(',')[m
[32m+[m[32m  var ranges = [][m
[32m+[m
[32m+[m[32m  // add ranges type[m
[32m+[m[32m  ranges.type = str.slice(0, index)[m
[32m+[m
[32m+[m[32m  // parse all ranges[m
[32m+[m[32m  for (var i = 0; i < arr.length; i++) {[m
[32m+[m[32m    var range = arr[i].split('-')[m
[32m+[m[32m    var start = parseInt(range[0], 10)[m
[32m+[m[32m    var end = parseInt(range[1], 10)[m
[32m+[m
[32m+[m[32m    // -nnn[m
[32m+[m[32m    if (isNaN(start)) {[m
[32m+[m[32m      start = size - end[m
[32m+[m[32m      end = size - 1[m
[32m+[m[32m    // nnn-[m
[32m+[m[32m    } else if (isNaN(end)) {[m
[32m+[m[32m      end = size - 1[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // limit last-byte-pos to current length[m
[32m+[m[32m    if (end > size - 1) {[m
[32m+[m[32m      end = size - 1[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // invalid or unsatisifiable[m
[32m+[m[32m    if (isNaN(start) || isNaN(end) || start > end || start < 0) {[m
[32m+[m[32m      continue[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // add range[m
[32m+[m[32m    ranges.push({[m
[32m+[m[32m      start: start,[m
[32m+[m[32m      end: end[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (ranges.length < 1) {[m
[32m+[m[32m    // unsatisifiable[m
[32m+[m[32m    return -1[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return options && options.combine[m
[32m+[m[32m    ? combineRanges(ranges)[m
[32m+[m[32m    : ranges[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Combine overlapping & adjacent ranges.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction combineRanges (ranges) {[m
[32m+[m[32m  var ordered = ranges.map(mapWithIndex).sort(sortByRangeStart)[m
[32m+[m
[32m+[m[32m  for (var j = 0, i = 1; i < ordered.length; i++) {[m
[32m+[m[32m    var range = ordered[i][m
[32m+[m[32m    var current = ordered[j][m
[32m+[m
[32m+[m[32m    if (range.start > current.end + 1) {[m
[32m+[m[32m      // next range[m
[32m+[m[32m      ordered[++j] = range[m
[32m+[m[32m    } else if (range.end > current.end) {[m
[32m+[m[32m      // extend range[m
[32m+[m[32m      current.end = range.end[m
[32m+[m[32m      current.index = Math.min(current.index, range.index)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // trim ordered array[m
[32m+[m[32m  ordered.length = j + 1[m
[32m+[m
[32m+[m[32m  // generate combined range[m
[32m+[m[32m  var combined = ordered.sort(sortByRangeIndex).map(mapWithoutIndex)[m
[32m+[m
[32m+[m[32m  // copy ranges type[m
[32m+[m[32m  combined.type = ranges.type[m
[32m+[m
[32m+[m[32m  return combined[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Map function to add index value to ranges.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction mapWithIndex (range, index) {[m
[32m+[m[32m  return {[m
[32m+[m[32m    start: range.start,[m
[32m+[m[32m    end: range.end,[m
[32m+[m[32m    index: index[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Map function to remove index value from ranges.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction mapWithoutIndex (range) {[m
[32m+[m[32m  return {[m
[32m+[m[32m    start: range.start,[m
[32m+[m[32m    end: range.end[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Sort function to sort ranges by index.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction sortByRangeIndex (a, b) {[m
[32m+[m[32m  return a.index - b.index[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Sort function to sort ranges by start position.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction sortByRangeStart (a, b) {[m
[32m+[m[32m  return a.start - b.start[m
[32m+[m[32m}[m

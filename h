[32m+[m[32m{[m
[32m+[m[32m  "name": "chat-server",[m
[32m+[m[32m  "version": "0.0.0",[m
[32m+[m[32m  "private": true,[m
[32m+[m[32m  "scripts": {[m
[32m+[m[32m    "start": "node ./serveur.js"[m
[32m+[m[32m  },[m
[32m+[m[32m  "dependencies": {[m
[32m+[m[32m    "express": "~4.15.5",[m
[32m+[m[32m    "ws": "^3.3.2"[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/serveur.js b/serveur.js[m
[1mnew file mode 100644[m
[1mindex 0000000..4562f6c[m
[1m--- /dev/null[m
[1m+++ b/serveur.js[m
[36m@@ -0,0 +1,39 @@[m
[32m+[m[32mconst express = require('express');[m
[32m+[m[32mconst http = require('http');[m
[32m+[m[32mconst url = require('url');[m
[32m+[m[32mconst WebSocket = require('ws');[m
[32m+[m
[32m+[m[32mconst app = express();[m
[32m+[m
[32m+[m[32mapp.use(function (req, res) {[m
[32m+[m[32m    res.send({ msg: "hello" });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mconst server = http.createServer(app);[m
[32m+[m[32mconst wss = new WebSocket.Server({ server });[m
[32m+[m
[32m+[m[32mwss.on('connection', function connection(ws, req) {[m
[32m+[m[32m    const location = url.parse(req.url, true);[m
[32m+[m[32m    // You might use location.query.access_token to authenticate or share sessions[m
[32m+[m[32m    // or req.headers.cookie (see http://stackoverflow.com/a/16395220/151312)[m
[32m+[m
[32m+[m[32m    ws.on('message', function incoming(data) {[m
[32m+[m[32m        // On récupère le pseudo de l'emeteur[m
[32m+[m[32m        const emetteur = url.parse(req.url, true).query.pseudo[m
[32m+[m
[32m+[m[32m        console.log('Nouveau message de %s : %s', emetteur, data);[m
[32m+[m
[32m+[m[32m        // Pour chaque message reçu, nous le rediffusons aux autres[m
[32m+[m[32m        wss.clients.forEach(function each(client) {[m
[32m+[m[32m            // Rediffuser seulement aux autres et seulement si la websocket est encore ouverte[m
[32m+[m[32m            if (client.readyState === WebSocket.OPEN) {[m
[32m+[m[32m                // On envoi les données en json puisqu'il n'est pas possible de faire transiter des objets complexes.[m
[32m+[m[32m                client.send(JSON.stringify({emetteur: emetteur, texte: data}));[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mserver.listen(8080, function listening() {[m
[32m+[m[32m    console.log('En écoute sur le port %d', server.address().port);[m
[32m+[m[32m});[m
\ No newline at end of file[m

[32m+[m
[32m+[m[32mvar stringify = require('./stringify');[m
[32m+[m[32mvar parse = require('./parse');[m
[32m+[m[32mvar formats = require('./formats');[m
[32m+[m
[32m+[m[32mmodule.exports = {[m
[32m+[m[32m    formats: formats,[m
[32m+[m[32m    parse: parse,[m
[32m+[m[32m    stringify: stringify[m
[32m+[m[32m};[m
[1mdiff --git a/node_modules/qs/lib/parse.js b/node_modules/qs/lib/parse.js[m
[1mnew file mode 100644[m
[1mindex 0000000..b6ae645[m
[1m--- /dev/null[m
[1m+++ b/node_modules/qs/lib/parse.js[m
[36m@@ -0,0 +1,172 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar utils = require('./utils');[m
[32m+[m
[32m+[m[32mvar has = Object.prototype.hasOwnProperty;[m
[32m+[m
[32m+[m[32mvar defaults = {[m
[32m+[m[32m    allowDots: false,[m
[32m+[m[32m    allowPrototypes: false,[m
[32m+[m[32m    arrayLimit: 20,[m
[32m+[m[32m    decoder: utils.decode,[m
[32m+[m[32m    delimiter: '&',[m
[32m+[m[32m    depth: 5,[m
[32m+[m[32m    parameterLimit: 1000,[m
[32m+[m[32m    plainObjects: false,[m
[32m+[m[32m    strictNullHandling: false[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar parseValues = function parseQueryStringValues(str, options) {[m
[32m+[m[32m    var obj = {};[m
[32m+[m[32m    var cleanStr = options.ignoreQueryPrefix ? str.replace(/^\?/, '') : str;[m
[32m+[m[32m    var limit = options.parameterLimit === Infinity ? undefined : options.parameterLimit;[m
[32m+[m[32m    var parts = cleanStr.split(options.delimiter, limit);[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < parts.length; ++i) {[m
[32m+[m[32m        var part = parts[i];[m
[32m+[m
[32m+[m[32m        var bracketEqualsPos = part.indexOf(']=');[m
[32m+[m[32m        var pos = bracketEqualsPos === -1 ? part.indexOf('=') : bracketEqualsPos + 1;[m
[32m+[m
[32m+[m[32m        var key, val;[m
[32m+[m[32m        if (pos === -1) {[m
[32m+[m[32m            key = options.decoder(part, defaults.decoder);[m
[32m+[m[32m            val = options.strictNullHandling ? null : '';[m
[32m+[m[32m        } else {[m
[32m+[m[32m            key = options.decoder(part.slice(0, pos), defaults.decoder);[m
[32m+[m[32m            val = options.decoder(part.slice(pos + 1), defaults.decoder);[m
[32m+[m[32m        }[m
[32m+[m[32m        if (has.call(obj, key)) {[m
[32m+[m[32m            obj[key] = [].concat(obj[key]).concat(val);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            obj[key] = val;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return obj;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar parseObject = function parseObjectRecursive(chain, val, options) {[m
[32m+[m[32m    if (!chain.length) {[m
[32m+[m[32m        return val;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var root = chain.shift();[m
[32m+[m
[32m+[m[32m    var obj;[m
[32m+[m[32m    if (root === '[]') {[m
[32m+[m[32m        obj = [];[m
[32m+[m[32m        obj = obj.concat(parseObject(chain, val, options));[m
[32m+[m[32m    } else {[m
[32m+[m[32m        obj = options.plainObjects ? Object.create(null) : {};[m
[32m+[m[32m        var cleanRoot = root.charAt(0) === '[' && root.charAt(root.length - 1) === ']' ? root.slice(1, -1) : root;[m
[32m+[m[32m        var index = parseInt(cleanRoot, 10);[m
[32m+[m[32m        if ([m
[32m+[m[32m            !isNaN(index)[m
[32m+[m[32m            && root !== cleanRoot[m
[32m+[m[32m            && String(index) === cleanRoot[m
[32m+[m[32m            && index >= 0[m
[32m+[m[32m            && (options.parseArrays && index <= options.arrayLimit)[m
[32m+[m[32m        ) {[m
[32m+[m[32m            obj = [];[m
[32m+[m[32m            obj[index] = parseObject(chain, val, options);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            obj[cleanRoot] = parseObject(chain, val, options);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return obj;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar parseKeys = function parseQueryStringKeys(givenKey, val, options) {[m
[32m+[m[32m    if (!givenKey) {[m
[32m+[m[32m        return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Transform dot notation to bracket notation[m
[32m+[m[32m    var key = options.allowDots ? givenKey.replace(/\.([^.[]+)/g, '[$1]') : givenKey;[m
[32m+[m
[32m+[m[32m    // The regex chunks[m
[32m+[m
[32m+[m[32m    var brackets = /(\[[^[\]]*])/;[m
[32m+[m[32m    var child = /(\[[^[\]]*])/g;[m
[32m+[m
[32m+[m[32m    // Get the parent[m
[32m+[m
[32m+[m[32m    var segment = brackets.exec(key);[m
[32m+[m[32m    var parent = segment ? key.slice(0, segment.index) : key;[m
[32m+[m
[32m+[m[32m    // Stash the parent if it exists[m
[32m+[m
[32m+[m[32m    var keys = [];[m
[32m+[m[32m    if (parent) {[m
[32m+[m[32m        // If we aren't using plain objects, optionally prefix keys[m
[32m+[m[32m        // that would overwrite object prototype properties[m
[32m+[m[32m        if (!options.plainObjects && has.call(Object.prototype, parent)) {[m
[32m+[m[32m            if (!options.allowPrototypes) {[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        keys.push(parent);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Loop through children appending to the array until we hit depth[m
[32m+[m
[32m+[m[32m    var i = 0;[m
[32m+[m[32m    while ((segment = child.exec(key)) !== null && i < options.depth) {[m
[32m+[m[32m        i += 1;[m
[32m+[m[32m        if (!options.plainObjects && has.call(Object.prototype, segment[1].slice(1, -1))) {[m
[32m+[m[32m            if (!options.allowPrototypes) {[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        keys.push(segment[1]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // If there's a remainder, just add whatever is left[m
[32m+[m
[32m+[m[32m    if (segment) {[m
[32m+[m[32m        keys.push('[' + key.slice(segment.index) + ']');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return parseObject(keys, val, options);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = function (str, opts) {[m
[32m+[m[32m    var options = opts ? utils.assign({}, opts) : {};[m
[32m+[m
[32m+[m[32m    if (options.decoder !== null && options.decoder !== undefined && typeof options.decoder !== 'function') {[m
[32m+[m[32m        throw new TypeError('Decoder has to be a function.');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    options.ignoreQueryPrefix = options.ignoreQueryPrefix === true;[m
[32m+[m[32m    options.delimiter = typeof options.delimiter === 'string' || utils.isRegExp(options.delimiter) ? options.delimiter : defaults.delimiter;[m
[32m+[m[32m    options.depth = typeof options.depth === 'number' ? options.depth : defaults.depth;[m
[32m+[m[32m    options.arrayLimit = typeof options.arrayLimit === 'number' ? options.arrayLimit : defaults.arrayLimit;[m
[32m+[m[32m    options.parseArrays = options.parseArrays !== false;[m
[32m+[m[32m    options.decoder = typeof options.decoder === 'function' ? options.decoder : defaults.decoder;[m
[32m+[m[32m    options.allowDots = typeof options.allowDots === 'boolean' ? options.allowDots : defaults.allowDots;[m
[32m+[m[32m    options.plainObjects = typeof options.plainObjects === 'boolean' ? options.plainObjects : defaults.plainObjects;[m
[32m+[m[32m    options.allowPrototypes = typeof options.allowPrototypes === 'boolean' ? options.allowPrototypes : defaults.allowPrototypes;[m
[32m+[m[32m    options.parameterLimit = typeof options.parameterLimit === 'number' ? options.parameterLimit : defaults.parameterLimit;[m
[32m+[m[32m    options.strictNullHandling = typeof options.strictNullHandling === 'boolean' ? options.strictNullHandling : defaults.strictNullHandling;[m
[32m+[m
[32m+[m[32m    if (str === '' || str === null || typeof str === 'undefined') {[m
[32m+[m[32m        return options.plainObjects ? Object.create(null) : {};[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var tempObj = typeof str === 'string' ? parseValues(str, options) : str;[m
[32m+[m[32m    var obj = options.plainObjects ? Object.create(null) : {};[m
[32m+[m
[32m+[m[32m    // Iterate over the keys and setup the new object[m
[32m+[m
[32m+[m[32m    var keys = Object.keys(tempObj);[m
[32m+[m[32m    for (var i = 0; i < keys.length; ++i) {[m
[32m+[m[32m        var key = keys[i];[m
[32m+[m[32m        var newObj = parseKeys(key, tempObj[key], options);[m
[32m+[m[32m        obj = utils.merge(obj, newObj, options);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return utils.compact(obj);[m
[32m+[m[32m};[m

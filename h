[1m--- /dev/null[m
[1m+++ b/node_modules/qs/lib/formats.js[m
[36m@@ -0,0 +1,18 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar replace = String.prototype.replace;[m
[32m+[m[32mvar percentTwenties = /%20/g;[m
[32m+[m
[32m+[m[32mmodule.exports = {[m
[32m+[m[32m    'default': 'RFC3986',[m
[32m+[m[32m    formatters: {[m
[32m+[m[32m        RFC1738: function (value) {[m
[32m+[m[32m            return replace.call(value, percentTwenties, '+');[m
[32m+[m[32m        },[m
[32m+[m[32m        RFC3986: function (value) {[m
[32m+[m[32m            return value;[m
[32m+[m[32m        }[m
[32m+[m[32m    },[m
[32m+[m[32m    RFC1738: 'RFC1738',[m
[32m+[m[32m    RFC3986: 'RFC3986'[m
[32m+[m[32m};[m

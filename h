[32m+[m
[32m+[m[32mvar setPrototypeOf = require('setprototypeof')[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Initialization middleware, exposing the[m
[32m+[m[32m * request and response to each other, as well[m
[32m+[m[32m * as defaulting the X-Powered-By header field.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Function} app[m
[32m+[m[32m * @return {Function}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.init = function(app){[m
[32m+[m[32m  return function expressInit(req, res, next){[m
[32m+[m[32m    if (app.enabled('x-powered-by')) res.setHeader('X-Powered-By', 'Express');[m
[32m+[m[32m    req.res = res;[m
[32m+[m[32m    res.req = req;[m
[32m+[m[32m    req.next = next;[m
[32m+[m
[32m+[m[32m    setPrototypeOf(req, app.request)[m
[32m+[m[32m    setPrototypeOf(res, app.response)[m
[32m+[m
[32m+[m[32m    res.locals = res.locals || Object.create(null);[m
[32m+[m
[32m+[m[32m    next();[m
[32m+[m[32m  };[m
[32m+[m[32m};[m

[1m+++ b/node_modules/mime/README.md[m
[36m@@ -0,0 +1,90 @@[m
[32m+[m[32m# mime[m
[32m+[m
[32m+[m[32mComprehensive MIME type mapping API based on mime-db module.[m
[32m+[m
[32m+[m[32m## Install[m
[32m+[m
[32m+[m[32mInstall with [npm](http://github.com/isaacs/npm):[m
[32m+[m
[32m+[m[32m    npm install mime[m
[32m+[m
[32m+[m[32m## Contributing / Testing[m
[32m+[m
[32m+[m[32m    npm run test[m
[32m+[m
[32m+[m[32m## Command Line[m
[32m+[m
[32m+[m[32m    mime [path_string][m
[32m+[m
[32m+[m[32mE.g.[m
[32m+[m
[32m+[m[32m    > mime scripts/jquery.js[m
[32m+[m[32m    application/javascript[m
[32m+[m
[32m+[m[32m## API - Queries[m
[32m+[m
[32m+[m[32m### mime.lookup(path)[m
[32m+[m[32mGet the mime type associated with a file, if no mime type is found `application/octet-stream` is returned. Performs a case-insensitive lookup using the extension in `path` (the substring after the last '/' or '.').  E.g.[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mvar mime = require('mime');[m
[32m+[m
[32m+[m[32mmime.lookup('/path/to/file.txt');         // => 'text/plain'[m
[32m+[m[32mmime.lookup('file.txt');                  // => 'text/plain'[m
[32m+[m[32mmime.lookup('.TXT');                      // => 'text/plain'[m
[32m+[m[32mmime.lookup('htm');                       // => 'text/html'[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### mime.default_type[m
[32m+[m[32mSets the mime type returned when `mime.lookup` fails to find the extension searched for. (Default is `application/octet-stream`.)[m
[32m+[m
[32m+[m[32m### mime.extension(type)[m
[32m+[m[32mGet the default extension for `type`[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mmime.extension('text/html');                 // => 'html'[m
[32m+[m[32mmime.extension('application/octet-stream');  // => 'bin'[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### mime.charsets.lookup()[m
[32m+[m
[32m+[m[32mMap mime-type to charset[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mmime.charsets.lookup('text/plain');        // => 'UTF-8'[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m(The logic for charset lookups is pretty rudimentary.  Feel free to suggest improvements.)[m
[32m+[m
[32m+[m[32m## API - Defining Custom Types[m
[32m+[m
[32m+[m[32mCustom type mappings can be added on a per-project basis via the following APIs.[m
[32m+[m
[32m+[m[32m### mime.define()[m
[32m+[m
[32m+[m[32mAdd custom mime/extension mappings[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mmime.define({[m
[32m+[m[32m    'text/x-some-format': ['x-sf', 'x-sft', 'x-sfml'],[m
[32m+[m[32m    'application/x-my-type': ['x-mt', 'x-mtt'],[m
[32m+[m[32m    // etc ...[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mmime.lookup('x-sft');                 // => 'text/x-some-format'[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32mThe first entry in the extensions array is returned by `mime.extension()`. E.g.[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mmime.extension('text/x-some-format'); // => 'x-sf'[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### mime.load(filepath)[m
[32m+[m
[32m+[m[32mLoad mappings from an Apache ".types" format file[m
[32m+[m
[32m+[m[32m```js[m
[32m+[m[32mmime.load('./my_project.types');[m
[32m+[m[32m```[m
[32m+[m[32mThe .types file format is simple -  See the `types` dir for examples.[m
[1mdiff --git a/node_modules/mime/build/build.js b/node_modules/mime/build/build.js[m
[1mnew file mode 100644[m
[1mindex 0000000..ed5313e[m
[1m--- /dev/null[m
[1m+++ b/node_modules/mime/build/build.js[m
[36m@@ -0,0 +1,11 @@[m
[32m+[m[32mvar db = require('mime-db');[m
[32m+[m
[32m+[m[32mvar mapByType = {};[m
[32m+[m[32mObject.keys(db).forEach(function(key) {[m
[32m+[m[32m  var extensions = db[key].extensions;[m
[32m+[m[32m  if (extensions) {[m
[32m+[m[32m    mapByType[key] = extensions;[m
[32m+[m[32m  }[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mconsole.log(JSON.stringify(mapByType));[m
[1mdiff --git a/node_modules/mime/build/test.js b/node_modules/mime/build/test.js[m
[1mnew file mode 100644[m
[1mindex 0000000..58b9ba7[m
[1m--- /dev/null[m
[1m+++ b/node_modules/mime/build/test.js[m
[36m@@ -0,0 +1,57 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * Usage: node test.js[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar mime = require('../mime');[m
[32m+[m[32mvar assert = require('assert');[m
[32m+[m[32mvar path = require('path');[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Test mime lookups[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32massert.equal('text/plain', mime.lookup('text.txt'));     // normal file[m
[32m+[m[32massert.equal('text/plain', mime.lookup('TEXT.TXT'));     // uppercase[m
[32m+[m[32massert.equal('text/plain', mime.lookup('dir/text.txt')); // dir + file[m
[32m+[m[32massert.equal('text/plain', mime.lookup('.text.txt'));    // hidden file[m
[32m+[m[32massert.equal('text/plain', mime.lookup('.txt'));         // nameless[m
[32m+[m[32massert.equal('text/plain', mime.lookup('txt'));          // extension-only[m
[32m+[m[32massert.equal('text/plain', mime.lookup('/txt'));         // extension-less ()[m
[32m+[m[32massert.equal('text/plain', mime.lookup('\\txt'));        // Windows, extension-less[m
[32m+[m[32massert.equal('application/octet-stream', mime.lookup('text.nope')); // unrecognized[m
[32m+[m[32massert.equal('fallback', mime.lookup('text.fallback', 'fallback')); // alternate default[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Test extensions[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32massert.equal('txt', mime.extension(mime.types.text));[m
[32m+[m[32massert.equal('html', mime.extension(mime.types.htm));[m
[32m+[m[32massert.equal('bin', mime.extension('application/octet-stream'));[m
[32m+[m[32massert.equal('bin', mime.extension('application/octet-stream '));[m
[32m+[m[32massert.equal('html', mime.extension(' text/html; charset=UTF-8'));[m
[32m+[m[32massert.equal('html', mime.extension('text/html; charset=UTF-8 '));[m
[32m+[m[32massert.equal('html', mime.extension('text/html; charset=UTF-8'));[m
[32m+[m[32massert.equal('html', mime.extension('text/html ; charset=UTF-8'));[m
[32m+[m[32massert.equal('html', mime.extension('text/html;charset=UTF-8'));[m
[32m+[m[32massert.equal('html', mime.extension('text/Html;charset=UTF-8'));[m
[32m+[m[32massert.equal(undefined, mime.extension('unrecognized'));[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Test node.types lookups[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32massert.equal('application/font-woff', mime.lookup('file.woff'));[m
[32m+[m[32massert.equal('application/octet-stream', mime.lookup('file.buffer'));[m
[32m+[m[32massert.equal('audio/mp4', mime.lookup('file.m4a'));[m
[32m+[m[32massert.equal('font/opentype', mime.lookup('file.otf'));[m
[32m+[m
[32m+[m[32m//[m
[32m+[m[32m// Test charsets[m
[32m+[m[32m//[m
[32m+[m
[32m+[m[32massert.equal('UTF-8', mime.charsets.lookup('text/plain'));[m
[32m+[m[32massert.equal(undefined, mime.charsets.lookup(mime.types.js));[m
[32m+[m[32massert.equal('fallback', mime.charsets.lookup('application/octet-stream', 'fallback'));[m
[32m+[m
[32m+[m[32mconsole.log('\nAll tests passed');[m

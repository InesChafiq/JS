[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar EventEmitter = require('events').EventEmitter;[m
[32m+[m[32mvar mixin = require('merge-descriptors');[m
[32m+[m[32mvar proto = require('./application');[m
[32m+[m[32mvar Route = require('./router/route');[m
[32m+[m[32mvar Router = require('./router');[m
[32m+[m[32mvar req = require('./request');[m
[32m+[m[32mvar res = require('./response');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Expose `createApplication()`.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports = module.exports = createApplication;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create an express application.[m
[32m+[m[32m *[m
[32m+[m[32m * @return {Function}[m
[32m+[m[32m * @api public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction createApplication() {[m
[32m+[m[32m  var app = function(req, res, next) {[m
[32m+[m[32m    app.handle(req, res, next);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  mixin(app, EventEmitter.prototype, false);[m
[32m+[m[32m  mixin(app, proto, false);[m
[32m+[m
[32m+[m[32m  // expose the prototype that will get set on requests[m
[32m+[m[32m  app.request = Object.create(req, {[m
[32m+[m[32m    app: { configurable: true, enumerable: true, writable: true, value: app }[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m  // expose the prototype that will get set on responses[m
[32m+[m[32m  app.response = Object.create(res, {[m
[32m+[m[32m    app: { configurable: true, enumerable: true, writable: true, value: app }[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m  app.init();[m
[32m+[m[32m  return app;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Expose the prototypes.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.application = proto;[m
[32m+[m[32mexports.request = req;[m
[32m+[m[32mexports.response = res;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Expose constructors.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.Route = Route;[m
[32m+[m[32mexports.Router = Router;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Expose middleware[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.query = require('./middleware/query');[m
[32m+[m[32mexports.static = require('serve-static');[m
[32m+[m
[32m+[m[32m/**[m

[1mdiff --git a/node_modules/cookie-signature/index.js b/node_modules/cookie-signature/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..b8c9463[m
[1m--- /dev/null[m
[1m+++ b/node_modules/cookie-signature/index.js[m
[36m@@ -0,0 +1,51 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * Module dependencies.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar crypto = require('crypto');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Sign the given `val` with `secret`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} val[m
[32m+[m[32m * @param {String} secret[m
[32m+[m[32m * @return {String}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.sign = function(val, secret){[m
[32m+[m[32m  if ('string' != typeof val) throw new TypeError("Cookie value must be provided as a string.");[m
[32m+[m[32m  if ('string' != typeof secret) throw new TypeError("Secret string must be provided.");[m
[32m+[m[32m  return val + '.' + crypto[m
[32m+[m[32m    .createHmac('sha256', secret)[m
[32m+[m[32m    .update(val)[m
[32m+[m[32m    .digest('base64')[m
[32m+[m[32m    .replace(/\=+$/, '');[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Unsign and decode the given `val` with `secret`,[m
[32m+[m[32m * returning `false` if the signature is invalid.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} val[m
[32m+[m[32m * @param {String} secret[m
[32m+[m[32m * @return {String|Boolean}[m
[32m+[m[32m * @api private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mexports.unsign = function(val, secret){[m
[32m+[m[32m  if ('string' != typeof val) throw new TypeError("Signed cookie string must be provided.");[m
[32m+[m[32m  if ('string' != typeof secret) throw new TypeError("Secret string must be provided.");[m
[32m+[m[32m  var str = val.slice(0, val.lastIndexOf('.'))[m
[32m+[m[32m    , mac = exports.sign(str, secret);[m
[32m+[m[41m  [m
[32m+[m[32m  return sha1(mac) == sha1(val) ? str : false;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction sha1(str){[m
[32m+[m[32m  return crypto.createHash('sha1').update(str).digest('hex');[m
[32m+[m[32m}[m

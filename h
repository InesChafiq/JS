[32m+[m
[32m+[m[32mmodule.exports = eventListenerCount[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the count of listeners on an event emitter of a specific type.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction eventListenerCount (emitter, type) {[m
[32m+[m[32m  return emitter.listeners(type).length[m

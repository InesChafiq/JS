[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m[32mvar qs = require('../');[m
[32m+[m[32mvar utils = require('../lib/utils');[m
[32m+[m[32mvar iconv = require('iconv-lite');[m
[32m+[m
[32m+[m[32mtest('stringify()', function (t) {[m
[32m+[m[32m    t.test('stringifies a querystring object', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: 'b' }), 'a=b');[m
[32m+[m[32m        st.equal(qs.stringify({ a: 1 }), 'a=1');[m
[32m+[m[32m        st.equal(qs.stringify({ a: 1, b: 2 }), 'a=1&b=2');[m
[32m+[m[32m        st.equal(qs.stringify({ a: 'A_Z' }), 'a=A_Z');[m
[32m+[m[32m        st.equal(qs.stringify({ a: '€' }), 'a=%E2%82%AC');[m
[32m+[m[32m        st.equal(qs.stringify({ a: '' }), 'a=%EE%80%80');[m
[32m+[m[32m        st.equal(qs.stringify({ a: 'א' }), 'a=%D7%90');[m
[32m+[m[32m        st.equal(qs.stringify({ a: '𐐷' }), 'a=%F0%90%90%B7');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('adds query prefix', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: 'b' }, { addQueryPrefix: true }), '?a=b');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('with query prefix, outputs blank string given an empty object', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({}, { addQueryPrefix: true }), '');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('stringifies a nested object', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: 'c' } }), 'a%5Bb%5D=c');[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: { c: { d: 'e' } } } }), 'a%5Bb%5D%5Bc%5D%5Bd%5D=e');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('stringifies a nested object with dots notation', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: 'c' } }, { allowDots: true }), 'a.b=c');[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: { c: { d: 'e' } } } }, { allowDots: true }), 'a.b.c.d=e');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('stringifies an array value', function (st) {[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify({ a: ['b', 'c', 'd'] }, { arrayFormat: 'indices' }),[m
[32m+[m[32m            'a%5B0%5D=b&a%5B1%5D=c&a%5B2%5D=d',[m
[32m+[m[32m            'indices => indices'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify({ a: ['b', 'c', 'd'] }, { arrayFormat: 'brackets' }),[m
[32m+[m[32m            'a%5B%5D=b&a%5B%5D=c&a%5B%5D=d',[m
[32m+[m[32m            'brackets => brackets'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify({ a: ['b', 'c', 'd'] }),[m
[32m+[m[32m            'a%5B0%5D=b&a%5B1%5D=c&a%5B2%5D=d',[m
[32m+[m[32m            'default => indices'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('omits nulls when asked', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: 'b', c: null }, { skipNulls: true }), 'a=b');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('omits nested nulls when asked', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: 'c', d: null } }, { skipNulls: true }), 'a%5Bb%5D=c');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('omits array indices when asked', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: ['b', 'c', 'd'] }, { indices: false }), 'a=b&a=c&a=d');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('stringifies a nested array value', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: ['c', 'd'] } }, { arrayFormat: 'indices' }), 'a%5Bb%5D%5B0%5D=c&a%5Bb%5D%5B1%5D=d');[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: ['c', 'd'] } }, { arrayFormat: 'brackets' }), 'a%5Bb%5D%5B%5D=c&a%5Bb%5D%5B%5D=d');[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: ['c', 'd'] } }), 'a%5Bb%5D%5B0%5D=c&a%5Bb%5D%5B1%5D=d');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('stringifies a nested array value with dots notation', function (st) {[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: { b: ['c', 'd'] } },[m
[32m+[m[32m                { allowDots: true, encode: false, arrayFormat: 'indices' }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a.b[0]=c&a.b[1]=d',[m
[32m+[m[32m            'indices: stringifies with dots + indices'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: { b: ['c', 'd'] } },[m
[32m+[m[32m                { allowDots: true, encode: false, arrayFormat: 'brackets' }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a.b[]=c&a.b[]=d',[m
[32m+[m[32m            'brackets: stringifies with dots + brackets'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: { b: ['c', 'd'] } },[m
[32m+[m[32m                { allowDots: true, encode: false }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a.b[0]=c&a.b[1]=d',[m
[32m+[m[32m            'default: stringifies with dots + indices'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('stringifies an object inside an array', function (st) {[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify({ a: [{ b: 'c' }] }, { arrayFormat: 'indices' }),[m
[32m+[m[32m            'a%5B0%5D%5Bb%5D=c',[m
[32m+[m[32m            'indices => brackets'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify({ a: [{ b: 'c' }] }, { arrayFormat: 'brackets' }),[m
[32m+[m[32m            'a%5B%5D%5Bb%5D=c',[m
[32m+[m[32m            'brackets => brackets'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify({ a: [{ b: 'c' }] }),[m
[32m+[m[32m            'a%5B0%5D%5Bb%5D=c',[m
[32m+[m[32m            'default => indices'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify({ a: [{ b: { c: [1] } }] }, { arrayFormat: 'indices' }),[m
[32m+[m[32m            'a%5B0%5D%5Bb%5D%5Bc%5D%5B0%5D=1',[m
[32m+[m[32m            'indices => indices'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify({ a: [{ b: { c: [1] } }] }, { arrayFormat: 'brackets' }),[m
[32m+[m[32m            'a%5B%5D%5Bb%5D%5Bc%5D%5B%5D=1',[m
[32m+[m[32m            'brackets => brackets'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify({ a: [{ b: { c: [1] } }] }),[m
[32m+[m[32m            'a%5B0%5D%5Bb%5D%5Bc%5D%5B0%5D=1',[m
[32m+[m[32m            'default => indices'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('stringifies an array with mixed objects and primitives', function (st) {[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify({ a: [{ b: 1 }, 2, 3] }, { encode: false, arrayFormat: 'indices' }),[m
[32m+[m[32m            'a[0][b]=1&a[1]=2&a[2]=3',[m
[32m+[m[32m            'indices => indices'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify({ a: [{ b: 1 }, 2, 3] }, { encode: false, arrayFormat: 'brackets' }),[m
[32m+[m[32m            'a[][b]=1&a[]=2&a[]=3',[m
[32m+[m[32m            'brackets => brackets'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify({ a: [{ b: 1 }, 2, 3] }, { encode: false }),[m
[32m+[m[32m            'a[0][b]=1&a[1]=2&a[2]=3',[m
[32m+[m[32m            'default => indices'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('stringifies an object inside an array with dots notation', function (st) {[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: [{ b: 'c' }] },[m
[32m+[m[32m                { allowDots: true, encode: false, arrayFormat: 'indices' }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a[0].b=c',[m
[32m+[m[32m            'indices => indices'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: [{ b: 'c' }] },[m
[32m+[m[32m                { allowDots: true, encode: false, arrayFormat: 'brackets' }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a[].b=c',[m
[32m+[m[32m            'brackets => brackets'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: [{ b: 'c' }] },[m
[32m+[m[32m                { allowDots: true, encode: false }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a[0].b=c',[m
[32m+[m[32m            'default => indices'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: [{ b: { c: [1] } }] },[m
[32m+[m[32m                { allowDots: true, encode: false, arrayFormat: 'indices' }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a[0].b.c[0]=1',[m
[32m+[m[32m            'indices => indices'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: [{ b: { c: [1] } }] },[m
[32m+[m[32m                { allowDots: true, encode: false, arrayFormat: 'brackets' }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a[].b.c[]=1',[m
[32m+[m[32m            'brackets => brackets'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: [{ b: { c: [1] } }] },[m
[32m+[m[32m                { allowDots: true, encode: false }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a[0].b.c[0]=1',[m
[32m+[m[32m            'default => indices'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('does not omit object keys when indices = false', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: [{ b: 'c' }] }, { indices: false }), 'a%5Bb%5D=c');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('uses indices notation for arrays when indices=true', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: ['b', 'c'] }, { indices: true }), 'a%5B0%5D=b&a%5B1%5D=c');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('uses indices notation for arrays when no arrayFormat is specified', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: ['b', 'c'] }), 'a%5B0%5D=b&a%5B1%5D=c');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('uses indices notation for arrays when no arrayFormat=indices', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: ['b', 'c'] }, { arrayFormat: 'indices' }), 'a%5B0%5D=b&a%5B1%5D=c');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('uses repeat notation for arrays when no arrayFormat=repeat', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: ['b', 'c'] }, { arrayFormat: 'repeat' }), 'a=b&a=c');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('uses brackets notation for arrays when no arrayFormat=brackets', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: ['b', 'c'] }, { arrayFormat: 'brackets' }), 'a%5B%5D=b&a%5B%5D=c');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('stringifies a complicated object', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: 'c', d: 'e' } }), 'a%5Bb%5D=c&a%5Bd%5D=e');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('stringifies an empty value', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: '' }), 'a=');[m
[32m+[m[32m        st.equal(qs.stringify({ a: null }, { strictNullHandling: true }), 'a');[m
[32m+[m
[32m+[m[32m        st.equal(qs.stringify({ a: '', b: '' }), 'a=&b=');[m
[32m+[m[32m        st.equal(qs.stringify({ a: null, b: '' }, { strictNullHandling: true }), 'a&b=');[m
[32m+[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: '' } }), 'a%5Bb%5D=');[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: null } }, { strictNullHandling: true }), 'a%5Bb%5D');[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: null } }, { strictNullHandling: false }), 'a%5Bb%5D=');[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('stringifies a null object', { skip: !Object.create }, function (st) {[m
[32m+[m[32m        var obj = Object.create(null);[m
[32m+[m[32m        obj.a = 'b';[m
[32m+[m[32m        st.equal(qs.stringify(obj), 'a=b');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('returns an empty string for invalid input', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify(undefined), '');[m
[32m+[m[32m        st.equal(qs.stringify(false), '');[m
[32m+[m[32m        st.equal(qs.stringify(null), '');[m
[32m+[m[32m        st.equal(qs.stringify(''), '');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('stringifies an object with a null object as a child', { skip: !Object.create }, function (st) {[m
[32m+[m[32m        var obj = { a: Object.create(null) };[m
[32m+[m
[32m+[m[32m        obj.a.b = 'c';[m
[32m+[m[32m        st.equal(qs.stringify(obj), 'a%5Bb%5D=c');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('drops keys with a value of undefined', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: undefined }), '');[m
[32m+[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: undefined, c: null } }, { strictNullHandling: true }), 'a%5Bc%5D');[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: undefined, c: null } }, { strictNullHandling: false }), 'a%5Bc%5D=');[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: undefined, c: '' } }), 'a%5Bc%5D=');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('url encodes values', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: 'b c' }), 'a=b%20c');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('stringifies a date', function (st) {[m
[32m+[m[32m        var now = new Date();[m
[32m+[m[32m        var str = 'a=' + encodeURIComponent(now.toISOString());[m
[32m+[m[32m        st.equal(qs.stringify({ a: now }), str);[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('stringifies the weird object from qs', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ 'my weird field': '~q1!2"\'w$5&7/z8)?' }), 'my%20weird%20field=~q1%212%22%27w%245%267%2Fz8%29%3F');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('skips properties that are part of the object prototype', function (st) {[m
[32m+[m[32m        Object.prototype.crash = 'test';[m
[32m+[m[32m        st.equal(qs.stringify({ a: 'b' }), 'a=b');[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: 'c' } }), 'a%5Bb%5D=c');[m
[32m+[m[32m        delete Object.prototype.crash;[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('stringifies boolean values', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: true }), 'a=true');[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: true } }), 'a%5Bb%5D=true');[m
[32m+[m[32m        st.equal(qs.stringify({ b: false }), 'b=false');[m
[32m+[m[32m        st.equal(qs.stringify({ b: { c: false } }), 'b%5Bc%5D=false');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('stringifies buffer values', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: new Buffer('test') }), 'a=test');[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: new Buffer('test') } }), 'a%5Bb%5D=test');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('stringifies an object using an alternative delimiter', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: 'b', c: 'd' }, { delimiter: ';' }), 'a=b;c=d');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('doesn\'t blow up when Buffer global is missing', function (st) {[m
[32m+[m[32m        var tempBuffer = global.Buffer;[m
[32m+[m[32m        delete global.Buffer;[m
[32m+[m[32m        var result = qs.stringify({ a: 'b', c: 'd' });[m
[32m+[m[32m        global.Buffer = tempBuffer;[m
[32m+[m[32m        st.equal(result, 'a=b&c=d');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('selects properties when filter=array', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: 'b' }, { filter: ['a'] }), 'a=b');[m
[32m+[m[32m        st.equal(qs.stringify({ a: 1 }, { filter: [] }), '');[m
[32m+[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: { b: [1, 2, 3, 4], c: 'd' }, c: 'f' },[m
[32m+[m[32m                { filter: ['a', 'b', 0, 2], arrayFormat: 'indices' }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a%5Bb%5D%5B0%5D=1&a%5Bb%5D%5B2%5D=3',[m
[32m+[m[32m            'indices => indices'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: { b: [1, 2, 3, 4], c: 'd' }, c: 'f' },[m
[32m+[m[32m                { filter: ['a', 'b', 0, 2], arrayFormat: 'brackets' }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a%5Bb%5D%5B%5D=1&a%5Bb%5D%5B%5D=3',[m
[32m+[m[32m            'brackets => brackets'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: { b: [1, 2, 3, 4], c: 'd' }, c: 'f' },[m
[32m+[m[32m                { filter: ['a', 'b', 0, 2] }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a%5Bb%5D%5B0%5D=1&a%5Bb%5D%5B2%5D=3',[m
[32m+[m[32m            'default => indices'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('supports custom representations when filter=function', function (st) {[m
[32m+[m[32m        var calls = 0;[m
[32m+[m[32m        var obj = { a: 'b', c: 'd', e: { f: new Date(1257894000000) } };[m
[32m+[m[32m        var filterFunc = function (prefix, value) {[m
[32m+[m[32m            calls += 1;[m
[32m+[m[32m            if (calls === 1) {[m
[32m+[m[32m                st.equal(prefix, '', 'prefix is empty');[m
[32m+[m[32m                st.equal(value, obj);[m
[32m+[m[32m            } else if (prefix === 'c') {[m
[32m+[m[32m                return void 0;[m
[32m+[m[32m            } else if (value instanceof Date) {[m
[32m+[m[32m                st.equal(prefix, 'e[f]');[m
[32m+[m[32m                return value.getTime();[m
[32m+[m[32m            }[m
[32m+[m[32m            return value;[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        st.equal(qs.stringify(obj, { filter: filterFunc }), 'a=b&e%5Bf%5D=1257894000000');[m
[32m+[m[32m        st.equal(calls, 5);[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('can disable uri encoding', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: 'b' }, { encode: false }), 'a=b');[m
[32m+[m[32m        st.equal(qs.stringify({ a: { b: 'c' } }, { encode: false }), 'a[b]=c');[m
[32m+[m[32m        st.equal(qs.stringify({ a: 'b', c: null }, { strictNullHandling: true, encode: false }), 'a=b&c');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('can sort the keys', function (st) {[m
[32m+[m[32m        var sort = function (a, b) {[m
[32m+[m[32m            return a.localeCompare(b);[m
[32m+[m[32m        };[m
[32m+[m[32m        st.equal(qs.stringify({ a: 'c', z: 'y', b: 'f' }, { sort: sort }), 'a=c&b=f&z=y');[m
[32m+[m[32m        st.equal(qs.stringify({ a: 'c', z: { j: 'a', i: 'b' }, b: 'f' }, { sort: sort }), 'a=c&b=f&z%5Bi%5D=b&z%5Bj%5D=a');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('can sort the keys at depth 3 or more too', function (st) {[m
[32m+[m[32m        var sort = function (a, b) {[m
[32m+[m[32m            return a.localeCompare(b);[m
[32m+[m[32m        };[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: 'a', z: { zj: { zjb: 'zjb', zja: 'zja' }, zi: { zib: 'zib', zia: 'zia' } }, b: 'b' },[m
[32m+[m[32m                { sort: sort, encode: false }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a=a&b=b&z[zi][zia]=zia&z[zi][zib]=zib&z[zj][zja]=zja&z[zj][zjb]=zjb'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: 'a', z: { zj: { zjb: 'zjb', zja: 'zja' }, zi: { zib: 'zib', zia: 'zia' } }, b: 'b' },[m
[32m+[m[32m                { sort: null, encode: false }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a=a&z[zj][zjb]=zjb&z[zj][zja]=zja&z[zi][zib]=zib&z[zi][zia]=zia&b=b'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('can stringify with custom encoding', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ 県: '大阪府', '': '' }, {[m
[32m+[m[32m            encoder: function (str) {[m
[32m+[m[32m                if (str.length === 0) {[m
[32m+[m[32m                    return '';[m
[32m+[m[32m                }[m
[32m+[m[32m                var buf = iconv.encode(str, 'shiftjis');[m
[32m+[m[32m                var result = [];[m
[32m+[m[32m                for (var i = 0; i < buf.length; ++i) {[m
[32m+[m[32m                    result.push(buf.readUInt8(i).toString(16));[m
[32m+[m[32m                }[m
[32m+[m[32m                return '%' + result.join('%');[m
[32m+[m[32m            }[m
[32m+[m[32m        }), '%8c%a7=%91%e5%8d%e3%95%7b&=');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('receives the default encoder as a second argument', function (st) {[m
[32m+[m[32m        st.plan(2);[m
[32m+[m[32m        qs.stringify({ a: 1 }, {[m
[32m+[m[32m            encoder: function (str, defaultEncoder) {[m
[32m+[m[32m                st.equal(defaultEncoder, utils.encode);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('throws error with wrong encoder', function (st) {[m
[32m+[m[32m        st.throws(function () {[m
[32m+[m[32m            qs.stringify({}, { encoder: 'string' });[m
[32m+[m[32m        }, new TypeError('Encoder has to be a function.'));[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('can use custom encoder for a buffer object', { skip: typeof Buffer === 'undefined' }, function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: new Buffer([1]) }, {[m
[32m+[m[32m            encoder: function (buffer) {[m
[32m+[m[32m                if (typeof buffer === 'string') {[m
[32m+[m[32m                    return buffer;[m
[32m+[m[32m                }[m
[32m+[m[32m                return String.fromCharCode(buffer.readUInt8(0) + 97);[m
[32m+[m[32m            }[m
[32m+[m[32m        }), 'a=b');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('serializeDate option', function (st) {[m
[32m+[m[32m        var date = new Date();[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify({ a: date }),[m
[32m+[m[32m            'a=' + date.toISOString().replace(/:/g, '%3A'),[m
[32m+[m[32m            'default is toISOString'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        var mutatedDate = new Date();[m
[32m+[m[32m        mutatedDate.toISOString = function () {[m
[32m+[m[32m            throw new SyntaxError();[m
[32m+[m[32m        };[m
[32m+[m[32m        st.throws(function () {[m
[32m+[m[32m            mutatedDate.toISOString();[m
[32m+[m[32m        }, SyntaxError);[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify({ a: mutatedDate }),[m
[32m+[m[32m            'a=' + Date.prototype.toISOString.call(mutatedDate).replace(/:/g, '%3A'),[m
[32m+[m[32m            'toISOString works even when method is not locally present'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        var specificDate = new Date(6);[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: specificDate },[m
[32m+[m[32m                { serializeDate: function (d) { return d.getTime() * 7; } }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a=42',[m
[32m+[m[32m            'custom serializeDate function called'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('RFC 1738 spaces serialization', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: 'b c' }, { format: qs.formats.RFC1738 }), 'a=b+c');[m
[32m+[m[32m        st.equal(qs.stringify({ 'a b': 'c d' }, { format: qs.formats.RFC1738 }), 'a+b=c+d');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('RFC 3986 spaces serialization', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: 'b c' }, { format: qs.formats.RFC3986 }), 'a=b%20c');[m
[32m+[m[32m        st.equal(qs.stringify({ 'a b': 'c d' }, { format: qs.formats.RFC3986 }), 'a%20b=c%20d');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('Backward compatibility to RFC 3986', function (st) {[m
[32m+[m[32m        st.equal(qs.stringify({ a: 'b c' }), 'a=b%20c');[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('Edge cases and unknown formats', function (st) {[m
[32m+[m[32m        ['UFO1234', false, 1234, null, {}, []].forEach([m
[32m+[m[32m            function (format) {[m
[32m+[m[32m                st.throws([m
[32m+[m[32m                    function () {[m
[32m+[m[32m                        qs.stringify({ a: 'b c' }, { format: format });[m
[32m+[m[32m                    },[m
[32m+[m[32m                    new TypeError('Unknown format option provided.')[m
[32m+[m[32m                );[m
[32m+[m[32m            }[m
[32m+[m[32m        );[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('encodeValuesOnly', function (st) {[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: 'b', c: ['d', 'e=f'], f: [['g'], ['h']] },[m
[32m+[m[32m                { encodeValuesOnly: true }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a=b&c[0]=d&c[1]=e%3Df&f[0][0]=g&f[1][0]=h'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: 'b', c: ['d', 'e'], f: [['g'], ['h']] }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a=b&c%5B0%5D=d&c%5B1%5D=e&f%5B0%5D%5B0%5D=g&f%5B1%5D%5B0%5D=h'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('encodeValuesOnly - strictNullHandling', function (st) {[m
[32m+[m[32m        st.equal([m
[32m+[m[32m            qs.stringify([m
[32m+[m[32m                { a: { b: null } },[m
[32m+[m[32m                { encodeValuesOnly: true, strictNullHandling: true }[m
[32m+[m[32m            ),[m
[32m+[m[32m            'a[b]'[m
[32m+[m[32m        );[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.test('does not mutate the options argument', function (st) {[m
[32m+[m[32m        var options = {};[m
[32m+[m[32m        qs.stringify({}, options);[m
[32m+[m[32m        st.deepEqual(options, {});[m
[32m+[m[32m        st.end();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[1mdiff --git a/node_modules/qs/test/utils.js b/node_modules/qs/test/utils.js[m
[1mnew file mode 100644[m
[1mindex 0000000..eff4011[m
[1m--- /dev/null[m
[1m+++ b/node_modules/qs/test/utils.js[m
[36m@@ -0,0 +1,34 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mvar test = require('tape');[m
[32m+[m[32mvar utils = require('../lib/utils');[m
[32m+[m
[32m+[m[32mtest('merge()', function (t) {[m
[32m+[m[32m    t.deepEqual(utils.merge({ a: 'b' }, { a: 'c' }), { a: ['b', 'c'] }, 'merges two objects with the same key');[m
[32m+[m
[32m+[m[32m    var oneMerged = utils.merge({ foo: 'bar' }, { foo: { first: '123' } });[m
[32m+[m[32m    t.deepEqual(oneMerged, { foo: ['bar', { first: '123' }] }, 'merges a standalone and an object into an array');[m
[32m+[m
[32m+[m[32m    var twoMerged = utils.merge({ foo: ['bar', { first: '123' }] }, { foo: { second: '456' } });[m
[32m+[m[32m    t.deepEqual(twoMerged, { foo: { 0: 'bar', 1: { first: '123' }, second: '456' } }, 'merges a standalone and two objects into an array');[m
[32m+[m
[32m+[m[32m    var sandwiched = utils.merge({ foo: ['bar', { first: '123', second: '456' }] }, { foo: 'baz' });[m
[32m+[m[32m    t.deepEqual(sandwiched, { foo: ['bar', { first: '123', second: '456' }, 'baz'] }, 'merges an object sandwiched by two standalones into an array');[m
[32m+[m
[32m+[m[32m    var nestedArrays = utils.merge({ foo: ['baz'] }, { foo: ['bar', 'xyzzy'] });[m
[32m+[m[32m    t.deepEqual(nestedArrays, { foo: ['baz', 'bar', 'xyzzy'] });[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mtest('assign()', function (t) {[m
[32m+[m[32m    var target = { a: 1, b: 2 };[m
[32m+[m[32m    var source = { b: 3, c: 4 };[m
[32m+[m[32m    var result = utils.assign(target, source);[m
[32m+[m
[32m+[m[32m    t.equal(result, target, 'returns the target');[m
[32m+[m[32m    t.deepEqual(target, { a: 1, b: 3, c: 4 }, 'target and source are merged');[m
[32m+[m[32m    t.deepEqual(source, { b: 3, c: 4 }, 'source is untouched');[m
[32m+[m
[32m+[m[32m    t.end();[m
[32m+[m[32m});[m

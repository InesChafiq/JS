[32m+[m[32m/**[m
[32m+[m[32m * negotiator[m
[32m+[m[32m * Copyright(c) 2012 Isaac Z. Schlueter[m
[32m+[m[32m * Copyright(c) 2014 Federico Romero[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = preferredMediaTypes;[m
[32m+[m[32mmodule.exports.preferredMediaTypes = preferredMediaTypes;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module variables.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar simpleMediaTypeRegExp = /^\s*([^\s\/;]+)\/([^;\s]+)\s*(?:;(.*))?$/;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse the Accept header.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parseAccept(accept) {[m
[32m+[m[32m  var accepts = splitMediaTypes(accept);[m
[32m+[m
[32m+[m[32m  for (var i = 0, j = 0; i < accepts.length; i++) {[m
[32m+[m[32m    var mediaType = parseMediaType(accepts[i].trim(), i);[m
[32m+[m
[32m+[m[32m    if (mediaType) {[m
[32m+[m[32m      accepts[j++] = mediaType;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // trim accepts[m
[32m+[m[32m  accepts.length = j;[m
[32m+[m
[32m+[m[32m  return accepts;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse a media type from the Accept header.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parseMediaType(str, i) {[m
[32m+[m[32m  var match = simpleMediaTypeRegExp.exec(str);[m
[32m+[m[32m  if (!match) return null;[m
[32m+[m
[32m+[m[32m  var params = Object.create(null);[m
[32m+[m[32m  var q = 1;[m
[32m+[m[32m  var subtype = match[2];[m
[32m+[m[32m  var type = match[1];[m
[32m+[m
[32m+[m[32m  if (match[3]) {[m
[32m+[m[32m    var kvps = splitParameters(match[3]).map(splitKeyValuePair);[m
[32m+[m
[32m+[m[32m    for (var j = 0; j < kvps.length; j++) {[m
[32m+[m[32m      var pair = kvps[j];[m
[32m+[m[32m      var key = pair[0].toLowerCase();[m
[32m+[m[32m      var val = pair[1];[m
[32m+[m
[32m+[m[32m      // get the value, unwrapping quotes[m
[32m+[m[32m      var value = val && val[0] === '"' && val[val.length - 1] === '"'[m
[32m+[m[32m        ? val.substr(1, val.length - 2)[m
[32m+[m[32m        : val;[m
[32m+[m
[32m+[m[32m      if (key === 'q') {[m
[32m+[m[32m        q = parseFloat(value);[m
[32m+[m[32m        break;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // store parameter[m
[32m+[m[32m      params[key] = value;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    type: type,[m
[32m+[m[32m    subtype: subtype,[m
[32m+[m[32m    params: params,[m
[32m+[m[32m    q: q,[m
[32m+[m[32m    i: i[m
[32m+[m[32m  };[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the priority of a media type.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getMediaTypePriority(type, accepted, index) {[m
[32m+[m[32m  var priority = {o: -1, q: 0, s: 0};[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < accepted.length; i++) {[m
[32m+[m[32m    var spec = specify(type, accepted[i], index);[m
[32m+[m
[32m+[m[32m    if (spec && (priority.s - spec.s || priority.q - spec.q || priority.o - spec.o) < 0) {[m
[32m+[m[32m      priority = spec;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return priority;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the specificity of the media type.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction specify(type, spec, index) {[m
[32m+[m[32m  var p = parseMediaType(type);[m
[32m+[m[32m  var s = 0;[m
[32m+[m
[32m+[m[32m  if (!p) {[m
[32m+[m[32m    return null;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if(spec.type.toLowerCase() == p.type.toLowerCase()) {[m
[32m+[m[32m    s |= 4[m
[32m+[m[32m  } else if(spec.type != '*') {[m
[32m+[m[32m    return null;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if(spec.subtype.toLowerCase() == p.subtype.toLowerCase()) {[m
[32m+[m[32m    s |= 2[m
[32m+[m[32m  } else if(spec.subtype != '*') {[m
[32m+[m[32m    return null;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var keys = Object.keys(spec.params);[m
[32m+[m[32m  if (keys.length > 0) {[m
[32m+[m[32m    if (keys.every(function (k) {[m
[32m+[m[32m      return spec.params[k] == '*' || (spec.params[k] || '').toLowerCase() == (p.params[k] || '').toLowerCase();[m
[32m+[m[32m    })) {[m
[32m+[m[32m      s |= 1[m
[32m+[m[32m    } else {[m
[32m+[m[32m      return null[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    i: index,[m
[32m+[m[32m    o: spec.i,[m
[32m+[m[32m    q: spec.q,[m
[32m+[m[32m    s: s,[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the preferred media types from an Accept header.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction preferredMediaTypes(accept, provided) {[m
[32m+[m[32m  // RFC 2616 sec 14.2: no header = */*[m
[32m+[m[32m  var accepts = parseAccept(accept === undefined ? '*/*' : accept || '');[m
[32m+[m
[32m+[m[32m  if (!provided) {[m
[32m+[m[32m    // sorted list of all types[m
[32m+[m[32m    return accepts[m
[32m+[m[32m      .filter(isQuality)[m
[32m+[m[32m      .sort(compareSpecs)[m
[32m+[m[32m      .map(getFullType);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var priorities = provided.map(function getPriority(type, index) {[m
[32m+[m[32m    return getMediaTypePriority(type, accepts, index);[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  // sorted list of accepted types[m
[32m+[m[32m  return priorities.filter(isQuality).sort(compareSpecs).map(function getType(priority) {[m
[32m+[m[32m    return provided[priorities.indexOf(priority)];[m
[32m+[m[32m  });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Compare two specs.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction compareSpecs(a, b) {[m
[32m+[m[32m  return (b.q - a.q) || (b.s - a.s) || (a.o - b.o) || (a.i - b.i) || 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get full type string.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getFullType(spec) {[m
[32m+[m[32m  return spec.type + '/' + spec.subtype;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Check if a spec has any quality.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction isQuality(spec) {[m
[32m+[m[32m  return spec.q > 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Count the number of quotes in a string.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction quoteCount(string) {[m
[32m+[m[32m  var count = 0;[m
[32m+[m[32m  var index = 0;[m
[32m+[m
[32m+[m[32m  while ((index = string.indexOf('"', index)) !== -1) {[m
[32m+[m[32m    count++;[m
[32m+[m[32m    index++;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return count;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Split a key value pair.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction splitKeyValuePair(str) {[m
[32m+[m[32m  var index = str.indexOf('=');[m
[32m+[m[32m  var key;[m
[32m+[m[32m  var val;[m
[32m+[m
[32m+[m[32m  if (index === -1) {[m
[32m+[m[32m    key = str;[m
[32m+[m[32m  } else {[m
[32m+[m[32m    key = str.substr(0, index);[m
[32m+[m[32m    val = str.substr(index + 1);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return [key, val];[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Split an Accept header into media types.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction splitMediaTypes(accept) {[m
[32m+[m[32m  var accepts = accept.split(',');[m
[32m+[m
[32m+[m[32m  for (var i = 1, j = 0; i < accepts.length; i++) {[m
[32m+[m[32m    if (quoteCount(accepts[j]) % 2 == 0) {[m
[32m+[m[32m      accepts[++j] = accepts[i];[m
[32m+[m[32m    } else {[m
[32m+[m[32m      accepts[j] += ',' + accepts[i];[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // trim accepts[m
[32m+[m[32m  accepts.length = j + 1;[m
[32m+[m
[32m+[m[32m  return accepts;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Split a string of parameters.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction splitParameters(str) {[m
[32m+[m[32m  var parameters = str.split(';');[m
[32m+[m
[32m+[m[32m  for (var i = 1, j = 0; i < parameters.length; i++) {[m
[32m+[m[32m    if (quoteCount(parameters[j]) % 2 == 0) {[m
[32m+[m[32m      parameters[++j] = parameters[i];[m
[32m+[m[32m    } else {[m
[32m+[m[32m      parameters[j] += ';' + parameters[i];[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // trim parameters[m
[32m+[m[32m  parameters.length = j + 1;[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < parameters.length; i++) {[m
[32m+[m[32m    parameters[i] = parameters[i].trim();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return parameters;[m
[32m+[m[32m}[m

[36m@@ -0,0 +1,26 @@[m
[32m+[m[32m# Polyfill for `Object.setPrototypeOf`[m
[32m+[m
[32m+[m[32mA simple cross platform implementation to set the prototype of an instianted object.  Supports all modern browsers and at least back to IE8.[m
[32m+[m
[32m+[m[32m## Usage:[m
[32m+[m
[32m+[m[32m```[m
[32m+[m[32m$ npm install --save setprototypeof[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m```javascript[m
[32m+[m[32mvar setPrototypeOf = require('setprototypeof');[m
[32m+[m
[32m+[m[32mvar obj = {};[m
[32m+[m[32msetPrototypeOf(obj, {[m
[32m+[m	[32mfoo: function() {[m
[32m+[m		[32mreturn 'bar';[m
[32m+[m	[32m}[m
[32m+[m[32m});[m
[32m+[m[32mobj.foo(); // bar[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32mTypeScript is also supported:[m
[32m+[m[32m```typescript[m
[32m+[m[32mimport setPrototypeOf = require('setprototypeof');[m
[32m+[m[32m```[m
\ No newline at end of file[m
[1mdiff --git a/node_modules/http-errors/node_modules/setprototypeof/index.d.ts b/node_modules/http-errors/node_modules/setprototypeof/index.d.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..f108ecd[m
[1m--- /dev/null[m
[1m+++ b/node_modules/http-errors/node_modules/setprototypeof/index.d.ts[m
[36m@@ -0,0 +1,2 @@[m
[32m+[m[32mdeclare function setPrototypeOf(o: any, proto: object | null): any;[m
[32m+[m[32mexport = setPrototypeOf;[m
[1mdiff --git a/node_modules/http-errors/node_modules/setprototypeof/index.js b/node_modules/http-errors/node_modules/setprototypeof/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..93ea417[m
[1m--- /dev/null[m
[1m+++ b/node_modules/http-errors/node_modules/setprototypeof/index.js[m
[36m@@ -0,0 +1,15 @@[m
[32m+[m[32mmodule.exports = Object.setPrototypeOf || ({__proto__:[]} instanceof Array ? setProtoOf : mixinProperties);[m
[32m+[m
[32m+[m[32mfunction setProtoOf(obj, proto) {[m
[32m+[m	[32mobj.__proto__ = proto;[m
[32m+[m	[32mreturn obj;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction mixinProperties(obj, proto) {[m
[32m+[m	[32mfor (var prop in proto) {[m
[32m+[m		[32mif (!obj.hasOwnProperty(prop)) {[m
[32m+[m			[32mobj[prop] = proto[prop];[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn obj;[m
[32m+[m[32m}[m

[32m+[m[32m{[m
[32m+[m[32m    "rules": {[m
[32m+[m		[32m"consistent-return": 2,[m
[32m+[m[32m        "max-lines": 0,[m
[32m+[m[32m        "max-nested-callbacks": [2, 3],[m
[32m+[m[32m        "max-statements": 0,[m
[32m+[m[32m        "no-extend-native": 0,[m
[32m+[m[32m        "no-magic-numbers": 0,[m
[32m+[m[32m        "sort-keys": 0[m
[32m+[m[32m    }[m
[32m+[m[32m}[m

[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = callSiteToString[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Format a CallSite file location to a string.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction callSiteFileLocation (callSite) {[m
[32m+[m[32m  var fileName[m
[32m+[m[32m  var fileLocation = ''[m
[32m+[m
[32m+[m[32m  if (callSite.isNative()) {[m
[32m+[m[32m    fileLocation = 'native'[m
[32m+[m[32m  } else if (callSite.isEval()) {[m
[32m+[m[32m    fileName = callSite.getScriptNameOrSourceURL()[m
[32m+[m[32m    if (!fileName) {[m
[32m+[m[32m      fileLocation = callSite.getEvalOrigin()[m
[32m+[m[32m    }[m
[32m+[m[32m  } else {[m
[32m+[m[32m    fileName = callSite.getFileName()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (fileName) {[m
[32m+[m[32m    fileLocation += fileName[m
[32m+[m
[32m+[m[32m    var lineNumber = callSite.getLineNumber()[m
[32m+[m[32m    if (lineNumber != null) {[m
[32m+[m[32m      fileLocation += ':' + lineNumber[m
[32m+[m
[32m+[m[32m      var columnNumber = callSite.getColumnNumber()[m
[32m+[m[32m      if (columnNumber) {[m
[32m+[m[32m        fileLocation += ':' + columnNumber[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return fileLocation || 'unknown source'[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Format a CallSite to a string.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction callSiteToString (callSite) {[m
[32m+[m[32m  var addSuffix = true[m
[32m+[m[32m  var fileLocation = callSiteFileLocation(callSite)[m
[32m+[m[32m  var functionName = callSite.getFunctionName()[m
[32m+[m[32m  var isConstructor = callSite.isConstructor()[m
[32m+[m[32m  var isMethodCall = !(callSite.isToplevel() || isConstructor)[m
[32m+[m[32m  var line = ''[m
[32m+[m
[32m+[m[32m  if (isMethodCall) {[m
[32m+[m[32m    var methodName = callSite.getMethodName()[m
[32m+[m[32m    var typeName = getConstructorName(callSite)[m
[32m+[m
[32m+[m[32m    if (functionName) {[m
[32m+[m[32m      if (typeName && functionName.indexOf(typeName) !== 0) {[m
[32m+[m[32m        line += typeName + '.'[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      line += functionName[m
[32m+[m
[32m+[m[32m      if (methodName && functionName.lastIndexOf('.' + methodName) !== functionName.length - methodName.length - 1) {[m
[32m+[m[32m        line += ' [as ' + methodName + ']'[m
[32m+[m[32m      }[m
[32m+[m[32m    } else {[m
[32m+[m[32m      line += typeName + '.' + (methodName || '<anonymous>')[m
[32m+[m[32m    }[m
[32m+[m[32m  } else if (isConstructor) {[m
[32m+[m[32m    line += 'new ' + (functionName || '<anonymous>')[m
[32m+[m[32m  } else if (functionName) {[m
[32m+[m[32m    line += functionName[m
[32m+[m[32m  } else {[m
[32m+[m[32m    addSuffix = false[m
[32m+[m[32m    line += fileLocation[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (addSuffix) {[m
[32m+[m[32m    line += ' (' + fileLocation + ')'[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return line[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get constructor name of reviver.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getConstructorName (obj) {[m
[32m+[m[32m  var receiver = obj.receiver[m
[32m+[m[32m  return (receiver.constructor && receiver.constructor.name) || null[m
[32m+[m[32m}[m

[1m+++ b/node_modules/methods/package.json[m
[36m@@ -0,0 +1,79 @@[m
[32m+[m[32m{[m
[32m+[m[32m  "_from": "methods@~1.1.2",[m
[32m+[m[32m  "_id": "methods@1.1.2",[m
[32m+[m[32m  "_inBundle": false,[m
[32m+[m[32m  "_integrity": "sha1-VSmk1nZUE07cxSZmVoNbD4Ua/O4=",[m
[32m+[m[32m  "_location": "/methods",[m
[32m+[m[32m  "_phantomChildren": {},[m
[32m+[m[32m  "_requested": {[m
[32m+[m[32m    "type": "range",[m
[32m+[m[32m    "registry": true,[m

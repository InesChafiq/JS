[32m+[m[32m[node-version-url]: https://nodejs.org/en/download[m
[32m+[m[32m[travis-image]: https://img.shields.io/travis/jshttp/vary/master.svg[m
[32m+[m[32m[travis-url]: https://travis-ci.org/jshttp/vary[m
[32m+[m[32m[coveralls-image]: https://img.shields.io/coveralls/jshttp/vary/master.svg[m
[32m+[m[32m[coveralls-url]: https://coveralls.io/r/jshttp/vary[m
[32m+[m[32m[downloads-image]: https://img.shields.io/npm/dm/vary.svg[m
[32m+[m[32m[downloads-url]: https://npmjs.org/package/vary[m
[1mdiff --git a/node_modules/vary/index.js b/node_modules/vary/index.js[m
[1mnew file mode 100644[m
[1mindex 0000000..5b5e741[m
[1m--- /dev/null[m
[1m+++ b/node_modules/vary/index.js[m
[36m@@ -0,0 +1,149 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * vary[m
[32m+[m[32m * Copyright(c) 2014-2017 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = vary[m
[32m+[m[32mmodule.exports.append = append[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * RegExp to match field-name in RFC 7230 sec 3.2[m
[32m+[m[32m *[m
[32m+[m[32m * field-name    = token[m
[32m+[m[32m * token         = 1*tchar[m
[32m+[m[32m * tchar         = "!" / "#" / "$" / "%" / "&" / "'" / "*"[m
[32m+[m[32m *               / "+" / "-" / "." / "^" / "_" / "`" / "|" / "~"[m
[32m+[m[32m *               / DIGIT / ALPHA[m
[32m+[m[32m *               ; any VCHAR, except delimiters[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar FIELD_NAME_REGEXP = /^[!#$%&'*+\-.^_`|~0-9A-Za-z]+$/[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Append a field to a vary header.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} header[m
[32m+[m[32m * @param {String|Array} field[m
[32m+[m[32m * @return {String}[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction append (header, field) {[m
[32m+[m[32m  if (typeof header !== 'string') {[m
[32m+[m[32m    throw new TypeError('header argument is required')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!field) {[m
[32m+[m[32m    throw new TypeError('field argument is required')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // get fields array[m
[32m+[m[32m  var fields = !Array.isArray(field)[m
[32m+[m[32m    ? parse(String(field))[m
[32m+[m[32m    : field[m
[32m+[m
[32m+[m[32m  // assert on invalid field names[m
[32m+[m[32m  for (var j = 0; j < fields.length; j++) {[m
[32m+[m[32m    if (!FIELD_NAME_REGEXP.test(fields[j])) {[m
[32m+[m[32m      throw new TypeError('field argument contains an invalid header name')[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // existing, unspecified vary[m
[32m+[m[32m  if (header === '*') {[m
[32m+[m[32m    return header[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // enumerate current values[m
[32m+[m[32m  var val = header[m
[32m+[m[32m  var vals = parse(header.toLowerCase())[m
[32m+[m
[32m+[m[32m  // unspecified vary[m
[32m+[m[32m  if (fields.indexOf('*') !== -1 || vals.indexOf('*') !== -1) {[m
[32m+[m[32m    return '*'[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < fields.length; i++) {[m
[32m+[m[32m    var fld = fields[i].toLowerCase()[m
[32m+[m
[32m+[m[32m    // append value (case-preserving)[m
[32m+[m[32m    if (vals.indexOf(fld) === -1) {[m
[32m+[m[32m      vals.push(fld)[m
[32m+[m[32m      val = val[m
[32m+[m[32m        ? val + ', ' + fields[i][m
[32m+[m[32m        : fields[i][m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return val[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse a vary header into an array.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {String} header[m
[32m+[m[32m * @return {Array}[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parse (header) {[m
[32m+[m[32m  var end = 0[m
[32m+[m[32m  var list = [][m
[32m+[m[32m  var start = 0[m
[32m+[m
[32m+[m[32m  // gather tokens[m
[32m+[m[32m  for (var i = 0, len = header.length; i < len; i++) {[m
[32m+[m[32m    switch (header.charCodeAt(i)) {[m
[32m+[m[32m      case 0x20: /*   */[m
[32m+[m[32m        if (start === end) {[m
[32m+[m[32m          start = end = i + 1[m
[32m+[m[32m        }[m
[32m+[m[32m        break[m
[32m+[m[32m      case 0x2c: /* , */[m
[32m+[m[32m        list.push(header.substring(start, end))[m
[32m+[m[32m        start = end = i + 1[m
[32m+[m[32m        break[m
[32m+[m[32m      default:[m
[32m+[m[32m        end = i + 1[m
[32m+[m[32m        break[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // final token[m
[32m+[m[32m  list.push(header.substring(start, end))[m
[32m+[m
[32m+[m[32m  return list[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Mark that a request is varied on a header field.[m
[32m+[m[32m *[m
[32m+[m[32m * @param {Object} res[m
[32m+[m[32m * @param {String|Array} field[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction vary (res, field) {[m
[32m+[m[32m  if (!res || !res.getHeader || !res.setHeader) {[m
[32m+[m[32m    // quack quack[m
[32m+[m[32m    throw new TypeError('res argument is required')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // get existing header[m
[32m+[m[32m  var val = res.getHeader('Vary') || ''[m
[32m+[m[32m  var header = Array.isArray(val)[m
[32m+[m[32m    ? val.join(', ')[m
[32m+[m[32m    : String(val)[m
[32m+[m
[32m+[m[32m  // set new header[m
[32m+[m[32m  if ((val = append(header, field))) {[m
[32m+[m[32m    res.setHeader('Vary', val)[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[1mdiff --git a/node_modules/vary/package.json b/node_modules/vary/package.json[m
[1mnew file mode 100644[m
[1mindex 0000000..d3fe27e[m
[1m--- /dev/null[m
[1m+++ b/node_modules/vary/package.json[m

[32m+[m[32m/**[m
[32m+[m[32m * negotiator[m
[32m+[m[32m * Copyright(c) 2012 Isaac Z. Schlueter[m
[32m+[m[32m * Copyright(c) 2014 Federico Romero[m
[32m+[m[32m * Copyright(c) 2014-2015 Douglas Christopher Wilson[m
[32m+[m[32m * MIT Licensed[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module exports.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mmodule.exports = preferredLanguages;[m
[32m+[m[32mmodule.exports.preferredLanguages = preferredLanguages;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Module variables.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar simpleLanguageRegExp = /^\s*([^\s\-;]+)(?:-([^\s;]+))?\s*(?:;(.*))?$/;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse the Accept-Language header.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parseAcceptLanguage(accept) {[m
[32m+[m[32m  var accepts = accept.split(',');[m
[32m+[m
[32m+[m[32m  for (var i = 0, j = 0; i < accepts.length; i++) {[m
[32m+[m[32m    var language = parseLanguage(accepts[i].trim(), i);[m
[32m+[m
[32m+[m[32m    if (language) {[m
[32m+[m[32m      accepts[j++] = language;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // trim accepts[m
[32m+[m[32m  accepts.length = j;[m
[32m+[m
[32m+[m[32m  return accepts;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse a language from the Accept-Language header.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction parseLanguage(str, i) {[m
[32m+[m[32m  var match = simpleLanguageRegExp.exec(str);[m
[32m+[m[32m  if (!match) return null;[m
[32m+[m
[32m+[m[32m  var prefix = match[1],[m
[32m+[m[32m    suffix = match[2],[m
[32m+[m[32m    full = prefix;[m
[32m+[m
[32m+[m[32m  if (suffix) full += "-" + suffix;[m
[32m+[m
[32m+[m[32m  var q = 1;[m
[32m+[m[32m  if (match[3]) {[m
[32m+[m[32m    var params = match[3].split(';')[m
[32m+[m[32m    for (var j = 0; j < params.length; j++) {[m
[32m+[m[32m      var p = params[j].split('=');[m
[32m+[m[32m      if (p[0] === 'q') q = parseFloat(p[1]);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    prefix: prefix,[m
[32m+[m[32m    suffix: suffix,[m
[32m+[m[32m    q: q,[m
[32m+[m[32m    i: i,[m
[32m+[m[32m    full: full[m
[32m+[m[32m  };[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the priority of a language.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getLanguagePriority(language, accepted, index) {[m
[32m+[m[32m  var priority = {o: -1, q: 0, s: 0};[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < accepted.length; i++) {[m
[32m+[m[32m    var spec = specify(language, accepted[i], index);[m
[32m+[m
[32m+[m[32m    if (spec && (priority.s - spec.s || priority.q - spec.q || priority.o - spec.o) < 0) {[m
[32m+[m[32m      priority = spec;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return priority;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the specificity of the language.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction specify(language, spec, index) {[m
[32m+[m[32m  var p = parseLanguage(language)[m
[32m+[m[32m  if (!p) return null;[m
[32m+[m[32m  var s = 0;[m
[32m+[m[32m  if(spec.full.toLowerCase() === p.full.toLowerCase()){[m
[32m+[m[32m    s |= 4;[m
[32m+[m[32m  } else if (spec.prefix.toLowerCase() === p.full.toLowerCase()) {[m
[32m+[m[32m    s |= 2;[m
[32m+[m[32m  } else if (spec.full.toLowerCase() === p.prefix.toLowerCase()) {[m
[32m+[m[32m    s |= 1;[m
[32m+[m[32m  } else if (spec.full !== '*' ) {[m
[32m+[m[32m    return null[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    i: index,[m
[32m+[m[32m    o: spec.i,[m
[32m+[m[32m    q: spec.q,[m
[32m+[m[32m    s: s[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the preferred languages from an Accept-Language header.[m
[32m+[m[32m * @public[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction preferredLanguages(accept, provided) {[m
[32m+[m[32m  // RFC 2616 sec 14.4: no header = *[m
[32m+[m[32m  var accepts = parseAcceptLanguage(accept === undefined ? '*' : accept || '');[m
[32m+[m
[32m+[m[32m  if (!provided) {[m
[32m+[m[32m    // sorted list of all languages[m
[32m+[m[32m    return accepts[m
[32m+[m[32m      .filter(isQuality)[m
[32m+[m[32m      .sort(compareSpecs)[m
[32m+[m[32m      .map(getFullLanguage);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var priorities = provided.map(function getPriority(type, index) {[m
[32m+[m[32m    return getLanguagePriority(type, accepts, index);[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  // sorted list of accepted languages[m
[32m+[m[32m  return priorities.filter(isQuality).sort(compareSpecs).map(function getLanguage(priority) {[m
[32m+[m[32m    return provided[priorities.indexOf(priority)];[m
[32m+[m[32m  });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Compare two specs.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction compareSpecs(a, b) {[m
[32m+[m[32m  return (b.q - a.q) || (b.s - a.s) || (a.o - b.o) || (a.i - b.i) || 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get full language string.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction getFullLanguage(spec) {[m
[32m+[m[32m  return spec.full;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Check if a spec has any quality.[m
[32m+[m[32m * @private[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction isQuality(spec) {[m
[32m+[m[32m  return spec.q > 0;[m
[32m+[m[32m}[m
